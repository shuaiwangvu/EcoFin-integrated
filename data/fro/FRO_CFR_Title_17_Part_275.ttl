  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-53> ;
.
fro-cfr-t17-p275:r-1-41-54
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(iv) Officers, agents, or employees of the State or political subdivision or any agency, authority or instrumentality thereof, acting in their official capacity." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 54 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-54> ;
.
fro-cfr-t17-p275:r-1-41-55
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Official" ;
  fro-cfr:hasParagraphText "(6)" ;
  fro-cfr:hasParagraphText "means any person (including any election committee for the person) who was, at the time of the contribution, an incumbent, candidate or successful candidate for elective office of a government entity, if the office:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 55 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-55> ;
.
fro-cfr-t17-p275:r-1-41-56
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) Is directly or indirectly responsible for, or can influence the outcome of, the hiring of an investment adviser by a government entity; or" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 56 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-56> ;
.
fro-cfr-t17-p275:r-1-41-57
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) Has authority to appoint any person who is directly or indirectly responsible for, or can influence the outcome of, the hiring of an investment adviser by a government entity." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 57 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-57> ;
.
fro-cfr-t17-p275:r-1-41-58
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Payment" ;
  fro-cfr:hasParagraphText "(7)means any gift, subscription, loan, advance, or deposit of money or anything of value." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 58 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-58> ;
.
fro-cfr-t17-p275:r-1-41-59
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Plan or program of a government entity" ;
  fro-cfr:hasParagraphText "(8)" ;
  fro-cfr:hasParagraphText "means any participant-directed investment program or plan sponsored or established by a State or political subdivision or any agency, authority or instrumentality thereof, including, but not limited to, a “qualified tuition plan” authorized by section 529 of the Internal Revenue Code (26 U.S.C. 529), a retirement plan authorized by section 403(b) or 457 of the Internal Revenue Code (26 U.S.C. 403(b) or 457), or any similar program or plan." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 59 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-59> ;
.
fro-cfr-t17-p275:r-1-41-6
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(A) A regulated person; or" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;

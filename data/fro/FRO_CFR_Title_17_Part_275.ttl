  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-70> ;
.
fro-cfr-t17-p275:r-1-41-71
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) With respect to investment advisory services, to communicate, directly or indirectly, for the purpose of obtaining or retaining a client for, or referring a client to, an investment adviser; and" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 71 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-71> ;
.
fro-cfr-t17-p275:r-1-41-72
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) With respect to a contribution or payment, to communicate, directly or indirectly, for the purpose of obtaining or arranging a contribution or payment." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 72 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-72> ;
.
fro-cfr-t17-p275:r-1-41-8
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) To coordinate, or to solicit any person or political action committee to make, any:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-8> ;
.
fro-cfr-t17-p275:r-1-41-9
  rdf:type fro-cfr:CFR_Parapraph ;

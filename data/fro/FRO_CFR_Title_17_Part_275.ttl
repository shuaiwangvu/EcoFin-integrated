  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Holdings reports." ;
  fro-cfr:hasParagraphEnumText "Reporting requirements" ;
  fro-cfr:hasParagraphText "(b)" ;
  fro-cfr:hasParagraphText "—(1)The code of ethics must require your access persons to submit to your chief compliance officer or other persons you designate in your code of ethics a report of the access person's current securities holdings that meets the following requirements:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-29 ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-29-8> ;
.
fro-cfr-t17-p275:r-1-29-9
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Content of holdings reports." ;
  fro-cfr:hasParagraphText "(i)Each holdings report must contain, at a minimum:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-29 ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-29-9> ;
.

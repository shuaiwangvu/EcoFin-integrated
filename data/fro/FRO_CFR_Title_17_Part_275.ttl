.
fro-cfr-t17-p275:r-1-39-24
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Solicitor" ;
  fro-cfr:hasParagraphText "(1)" ;
  fro-cfr:hasParagraphText "means any person who, directly or indirectly, solicits any client for, or refers any client to, an investment adviser." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-39 ;
  fro-leg-ref:hasSequenceNumber 24 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-39-24> ;

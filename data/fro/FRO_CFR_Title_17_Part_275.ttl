  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-17-4> ;
.
fro-cfr-t17-p275:r-1-17-5
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Non-United States investment advisers." ;
  fro-cfr:hasParagraphText "(b)For purposes of section 203(m) of the Act (15 U.S.C. 80b-3(m)), an investment adviser with its principal office and place of business outside of the United States is exempt from the requirement to register under section 203 of the Act if:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-17 ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-17-5> ;
.
fro-cfr-t17-p275:r-1-17-6
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(1) The investment adviser has no client that is a United States person except for one or more qualifying private funds; and" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-17 ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-17-6> ;
.
fro-cfr-t17-p275:r-1-17-7
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(2) All assets managed by the investment adviser at a place of business in the United States are solely attributable to private fund assets, the total value of which is less than $150 million." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-17 ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-17-7> ;
.

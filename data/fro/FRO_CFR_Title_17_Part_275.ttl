  fro-leg-ref:divides fro-cfr-t17-p275:r-1-12 ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-12-6> ;
.
fro-cfr-t17-p275:r-1-12-7

.
fro-cfr-t17-p275:r-1-41-70
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Solicit" ;
  fro-cfr:hasParagraphText "(10)means:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;

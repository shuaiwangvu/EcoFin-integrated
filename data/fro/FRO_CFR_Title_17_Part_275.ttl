  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Client." ;
  fro-cfr:hasParagraphText "(a)You may deem the following to be a single client for purposes of section 202(a)(30) of the Act (15 U.S.C. 80b-2(a)(30)):" ;

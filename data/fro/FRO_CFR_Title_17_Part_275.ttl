  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-45-2> ;
.
fro-cfr-t17-p275:r-1-45-3
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Place of business." ;
  fro-cfr:hasParagraphText "(a)“Place of business” of an investment adviser means:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-45 ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-45-3> ;
.
fro-cfr-t17-p275:r-1-45-4
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(1) An office at which the investment adviser regularly provides investment advisory services, solicits, meets with, or otherwise communicates with clients; and" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-45 ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-45-4> ;
.
fro-cfr-t17-p275:r-1-45-5
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(2) Any other location that is held out to the general public as a location at which the investment adviser provides investment advisory services, solicits, meets with, or otherwise communicates with clients." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-45 ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-45-5> ;
.
fro-cfr-t17-p275:r-1-45-6
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Principal office and place of business." ;
  fro-cfr:hasParagraphText "(b)" ;
  fro-cfr:hasParagraphText "“Principal office and place of business” of an investment adviser means the executive office of the investment adviser from which the officers, partners, or managers of the investment adviser direct, control, and coordinate the activities of the investment adviser." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-45 ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-45-6> ;
.
fro-cfr-t17-p275:r-1-46
  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionCitation "[76 FR 43014, July 19, 2011]" ;
  fro-cfr:hasSectionNumber "§ 275.222-2" ;
  fro-cfr:hasSectionSubject "Definition of “client” for purposes of the national de minimis standard." ;
  fro-leg-ref:divides fro-cfr:CFR_Title-17_Part-275 ;
  fro-leg-ref:hasSequenceNumber 46 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-46> ;
.
fro-cfr-t17-p275:r-1-46-2
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "For purposes of section 222(d)(2) of the Act (15 U.S.C. 80b-18a(d)(2)), an investment adviser may rely upon the definition of “client” provided by § 275.202(a)(30)-1, without giving regard to paragraph (b)(4) of that section." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-46 ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-46-2> ;
.
fro-cfr-t17-p275:r-1-5
  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionCitation "[30 FR 4129, Mar. 30, 1965]" ;
  fro-cfr:hasSectionNumber "§ 275.0-3" ;
  fro-cfr:hasSectionSubject "References to rules and regulations." ;

  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-5> ;
.
fro-cfr-t17-p275:r-1-5-2
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "rules and regulations" ;
  fro-cfr:hasParagraphText "The termrefers to all rules and regulations adopted by the Commission pursuant to the Act, including the forms for registration and reports and the accompanying instructions thereto." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-5 ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-5-2> ;
.
fro-cfr-t17-p275:r-1-6
  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionCitation "[41 FR 39019, Sept. 14, 1976, as amended at 44 FR 4666, Jan. 23, 1979; 47 FR 58239, Dec. 30, 1982; 68 FR 42248, July 17, 2003; 76 FR 71877, Nov. 21, 2011]" ;
  fro-cfr:hasSectionNumber "§ 275.0-4" ;
  fro-cfr:hasSectionSubject "General requirements of papers and applications." ;
  fro-leg-ref:divides fro-cfr:CFR_Title-17_Part-275 ;

fro-cfr-t17-p275:r-1-18-2
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Eligibility for SEC registration of mid-sized investment advisers." ;
  fro-cfr:hasParagraphText "(a)If you are an investment adviser described in section 203A(a)(2)(B) of the Act (15 U.S.C. 80b-3a(a)(2)(B)):" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-18 ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-18-2> ;
.
fro-cfr-t17-p275:r-1-18-3
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Threshold for SEC registration and registration buffer." ;
  fro-cfr:hasParagraphText "(1)You may, but are not required to register with the Commission if you have assets under management of at least $100,000,000 but less than $110,000,000, and you need not withdraw your registration unless you have less than $90,000,000 of assets under management." ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-18 ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-18-3> ;
.
fro-cfr-t17-p275:r-1-18-4
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Exceptions." ;
  fro-cfr:hasParagraphText "(2)This paragraph (a) does not apply if:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-18 ;

  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Contribution" ;
  fro-cfr:hasParagraphText "(1)" ;
  fro-cfr:hasParagraphText "means any gift, subscription, loan, advance, or deposit of money or anything of value made for:" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-41 ;
  fro-leg-ref:hasSequenceNumber 34 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-34> ;
.

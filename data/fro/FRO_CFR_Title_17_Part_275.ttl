  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-17-2> ;
.
fro-cfr-t17-p275:r-1-17-3
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(1) Acts solely as an investment adviser to one or more qualifying private funds; and" ;
  fro-leg-ref:divides fro-cfr-t17-p275:r-1-17 ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-17-3> ;
.

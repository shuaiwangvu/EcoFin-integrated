  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-45> ;
.
fro-cfr-t17-p275:r-1-45-2
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "For purposes of section 222 (15 U.S.C. 80b-18a) of the Act:" ;

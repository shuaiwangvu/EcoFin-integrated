  fro-leg-ref:hasSequenceNumber 35 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2012-title17-vol3-part275.ttl#r-1-41-35> ;
.
fro-cfr-t17-p275:r-1-41-36

  composite:child cfr-12-225:r-1-16-3-15-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company may rebut a presumption that it is routinely managing or operating a portfolio company under paragraph (b)(2) of this section by presenting information to the Board demonstrating to the Board's satisfaction that the financial holding company is not routinely managing or operating the portfolio company." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;
.

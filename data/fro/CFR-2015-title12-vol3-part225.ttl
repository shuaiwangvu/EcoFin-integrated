  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) Provide assistance to a portfolio company in connection with the underwriting or private placement of its securities, including acting as the underwriter or placement agent for such securities; and" ;

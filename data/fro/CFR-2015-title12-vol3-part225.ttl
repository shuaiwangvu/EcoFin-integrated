cfr-12-225:r-1-18-7-71
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Section 501(b) of the GLBA required the Agencies to establish appropriate standards for financial institutions subject to their jurisdiction that include administrative, technical, and physical safeguards, to protect the security and confidentiality of customer information. Accordingly, the Agencies issued Security Guidelines requiring every financial institution to have an information security program designed to:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "71"^^xsd:int ;
.
cfr-12-225:r-1-18-7-72
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. Ensure the security and confidentiality of customer information;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "72"^^xsd:int ;
.
cfr-12-225:r-1-18-7-73
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2. Protect against any anticipated threats or hazards to the security or integrity of such information; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "73"^^xsd:int ;
.
cfr-12-225:r-1-18-7-74
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-74-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "3. Protect against unauthorized access to or use of such information that could result in substantial harm or inconvenience to any customer." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "74"^^xsd:int ;
.

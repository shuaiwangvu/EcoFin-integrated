.
cfr-12-225:r-1-18-3-155
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-155-1 ;
  composite:child cfr-12-225:r-1-18-3-155-3 ;
  composite:child cfr-12-225:r-1-18-3-155-4 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "d." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "i. No deduction is required for nonfinancial equity investments that are held by a bank holding company through one or more SBICs that are consolidated with the bank holding company or in one or more SBICs that are not consolidated with the bank holding company to the extent that all such investments, in the aggregate, do not exceed 15 percent of the aggregate of the bank holding company's pro rata interests in the Tier 1 capital of its subsidiary banks. Any nonfinancial equity investment that is held through or in an SBIC and not required to be deducted from Tier 1 capital under this section II.B.5.d. will be assigned a 100 percent risk-weight and included in the parent holding company's consolidated risk-weighted assets." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "155"^^xsd:int ;
.
cfr-12-225:r-1-18-3-155-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "SBIC investments." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

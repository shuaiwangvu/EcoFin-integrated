      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "12 CFR 6.3(b)-(c), 208.42(b)-(c), and 325.102(b)-(c)." ;
      composite:index "7"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A company becomes aware that a depository institution it controls is no longer well capitalized upon the occurrence of any material event that would change the category assigned to the institution for purposes of section 38 of the Federal Deposit Insurance Act (12 U.S.C. 1831o)." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(i)" ;

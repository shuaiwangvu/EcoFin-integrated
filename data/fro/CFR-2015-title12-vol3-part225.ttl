  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) Any insured depository institution that would be controlled by the company on consummation of the proposal has not achieved at least a rating of “satisfactory record of meeting community credit needs” under the Community Reinvestment Act at the institution's most recent examination." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "24"^^xsd:int ;
.
cfr-12-225:r-1-15-3-25
  rdf:type cfr-fdsys-s:P ;

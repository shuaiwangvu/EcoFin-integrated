      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "This § 225.174 shall cease to be effective on the date that a final rule issued by the Board that specifically addresses the appropriate regulatory capital treatment of merchant banking investments becomes effective." ;

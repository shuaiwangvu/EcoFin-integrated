  composite:index "21"^^xsd:int ;
.
cfr-12-225:r-1-16-3-22
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iii) Significant changes to the business plan or accounting methods or policies of the portfolio company;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "22"^^xsd:int ;
.
cfr-12-225:r-1-16-3-23
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;

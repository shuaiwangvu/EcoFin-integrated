  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-9-8-5
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-9-8-5-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Any request for a Board determination that an activity is so closely related to banking or managing or controlling banks as to be a proper incident thereto, shall be submitted to the Board in writing, and shall contain evidence that the proposed activity is so closely related to banking or managing or controlling banks as to be a proper incident thereto." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.

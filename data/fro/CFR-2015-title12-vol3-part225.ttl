cfr-12-225:r-1-7-5-1
  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Corporate practices." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-7-5-10
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-7-5-10-1 ;
  composite:child cfr-12-225:r-1-7-5-10-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Within 15 calendar days of receipt of a notice under this section, the appropriate Reserve Bank shall either approve the transaction proposed in the notice or refer the notice to the Board for decision. If the notice is referred to the Board for" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "decision, the Board shall act on the notice within 30 calendar days after the Reserve Bank receives the notice." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:index "10"^^xsd:int ;
.
cfr-12-225:r-1-7-5-10-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Acting on notice." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-7-5-10-3
  rdf:type cfr-fdsys-s:PRTPAGE ;
  cfr-12-225:P-PRTPAGE "153" ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-7-5-11
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-7-5-11-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(4)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) The Board may disapprove a proposed purchase or redemption if it finds that the proposal would constitute an unsafe or unsound practice, or would violate any law, regulation, Board order, directive, or any condition imposed by, or written agreement with, the Board." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "11"^^xsd:int ;
.
cfr-12-225:r-1-7-5-11-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Factors considered in acting on notice." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-7-5-12
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-7-5-12-1 ;
  composite:child cfr-12-225:r-1-7-5-12-2 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) In determining whether a proposal constitutes an unsafe or unsound practice, the Board shall consider whether the bank holding company's financial condition, after giving effect to the proposed purchase or redemption, meets the financial standards applied by the Board under section 3 of the BHC Act, including 12 CFR part 217," ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "and the Board's Policy Statement for Small Bank Holding Companies (appendix C of this part)." ;
      composite:index "4"^^xsd:int ;
    ] ;
  composite:index "12"^^xsd:int ;
.
cfr-12-225:r-1-7-5-12-1
  rdf:type cfr-fdsys-s:SU ;

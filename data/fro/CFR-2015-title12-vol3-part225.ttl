  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "4." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Maturity is generally not a factor in assigning items to risk categories with the exception of claims on non-OECD banks, commitments, and interest rate and foreign exchange rate contracts. Except for commitments, short-term is defined as one year or less" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "maturity and long-term is defined as over one year" ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "maturity and long-term is defined as over one year" ;
      composite:index "9"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "maturity. In the case of commitments, short-term is defined as one year or less" ;
      composite:index "7"^^xsd:int ;
    ] ;
  composite:child [

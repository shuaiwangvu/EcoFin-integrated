  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "must not exceed 15 percent of the sum of all core capital elements, including restricted core capital elements, net of goodwill less any associated deferred tax liability." ;
      composite:index "8"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;

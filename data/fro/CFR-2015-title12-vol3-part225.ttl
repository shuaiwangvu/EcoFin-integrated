  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iii) Meet with the officers or employees of a portfolio company to monitor or provide advice with respect to the portfolio company's performance or activities." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "30"^^xsd:int ;
.
cfr-12-225:r-1-16-3-31
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-3-31-1 ;
  composite:child cfr-12-225:r-1-16-3-31-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company may routinely manage or operate a portfolio company only when intervention by the financial holding company is necessary or required to obtain a reasonable return on the financial holding company's investment in the portfolio company upon resale or other disposition of the investment, such as to avoid or address a significant operating loss or in connection with a loss of senior management at the portfolio company." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(1)" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "31"^^xsd:int ;
.
cfr-12-225:r-1-16-3-31-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "When may a financial holding company routinely manage or operate a portfolio company?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-3-31-3
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Special circumstances required." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-16-3-32
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-3-32-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company may routinely manage or operate a portfolio company only for the period of time as may be necessary to address the cause of the financial holding company's involvement, to obtain suitable alternative management arrangements, to dispose of the investment, or to otherwise obtain a reasonable return upon the resale or disposition of the investment." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "32"^^xsd:int ;
.
cfr-12-225:r-1-16-3-32-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Duration Limited." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

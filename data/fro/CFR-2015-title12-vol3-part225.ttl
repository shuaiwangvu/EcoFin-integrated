  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) Directors, officers or employees of a bank or thrift subsidiary of a bank holding company, or a bank or thrift subsidiary or branch or agency of a foreign bank, shall not serve as a majority of the board of directors or the chief executive officer of an affiliated section 20 subsidiary." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "12"^^xsd:int ;
.

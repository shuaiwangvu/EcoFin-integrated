cfr-12-225:r-1-18-2-17
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-2-17-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A director, officer, or employee of a bank, thrift, branch or agency may not express an opinion on the value or the advisability of the purchase or the sale of a bank-ineligible security that he or she knows is being underwritten or dealt in by a section 20 affiliate unless he or she notifies the customer of the affiliate's role." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "17"^^xsd:int ;
.

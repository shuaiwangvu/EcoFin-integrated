cfr-12-225:r-1-15-16-15-1
  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Foreign bank holding companies." ;
      composite:index "1"^^xsd:int ;

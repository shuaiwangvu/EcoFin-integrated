  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) All depository institutions controlled by the bank holding company must be and remain well capitalized;" ;
      composite:index "1"^^xsd:int ;
    ] ;

cfr-12-225:r-1-18-5-7
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-5-7-1 ;
  composite:child cfr-12-225:r-1-18-5-7-3 ;
  composite:child cfr-12-225:r-1-18-5-7-4 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "This policy statement applies only to bank holding companies with" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "consolidated assets of less than $500 million that (i) are not engaged in significant nonbanking activities either directly or through a nonbank subsidiary; (ii) do not conduct significant off-balance sheet activities (including securitization and asset management or administration) either directly or through a nonbank subsidiary; and (iii) do not have a material amount of debt or equity securities outstanding (other than trust preferred securities) that are registered with the Securities and Exchange Commission. The Board may in its discretion exclude any bank holding company, regardless of asset size, from the policy statement if such action is warranted for supervisory purposes." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.

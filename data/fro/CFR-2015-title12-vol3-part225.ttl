  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. The Security Guidelines direct every financial institution to assess the following risks, among others, when developing its information security program:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "76"^^xsd:int ;
.

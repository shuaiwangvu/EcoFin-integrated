  composite:child cfr-12-225:r-1-7-9-111-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(A) The bank holding company is, and after the capital distribution" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "would remain, well capitalized as defined in § 225.2(r) of Regulation Y (12 CFR 225.2(r));" ;

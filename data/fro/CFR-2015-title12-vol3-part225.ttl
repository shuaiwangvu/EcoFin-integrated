  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-4-5-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A company becomes aware that a depository institution it controls is no longer well managed at the time the depository institution receives written notice from the appropriate Federal or state banking agency that either its composite rating or its rating for management is not at least satisfactory." ;
      composite:index "3"^^xsd:int ;
    ] ;

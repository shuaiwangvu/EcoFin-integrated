.
cfr-12-225:r-1-11-4-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-11-4-2-1 ;
  composite:child cfr-12-225:r-1-11-4-2-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) A notice required under this subpart shall be filed with the appropriate Reserve Bank and shall contain all the information required by paragraph 6 of the Bank Control Act (12 U.S.C. 1817(j)(6)), or prescribed in the designated Board form." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-11-4-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Filing notice." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "whose financial statements are not consolidated for accounting or regulatory reporting purposes, regardless of whether the investment is made by the parent bank holding company or its direct or indirect subsidiaries, will be deducted from the consolidated parent banking organization's total capital components." ;
      composite:index "8"^^xsd:int ;
    ] ;

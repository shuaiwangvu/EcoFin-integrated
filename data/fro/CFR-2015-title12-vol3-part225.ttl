  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "How may a financial holding company rebut a presumption that it is routinely managing or operating a portfolio company?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-3-16

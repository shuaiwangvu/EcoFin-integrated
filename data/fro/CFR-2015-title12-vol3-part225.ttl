    ] ;
  composite:index "12"^^xsd:int ;
.
cfr-12-225:r-1-16-8-13
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-8-13-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) A person that is not affiliated or associated with the financial holding company owns or controls a greater percentage of the equity capital of the portfolio company than the amount owned or controlled by the financial" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "holding company, and no more than one officer or employee of the holding company serves as a director or trustee (or individual exercising similar functions) of the company; or" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "13"^^xsd:int ;
.

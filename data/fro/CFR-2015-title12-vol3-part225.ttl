.
cfr-12-225:r-1-15-16-15-11
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(7) Similar facility for the manufacture, distribution, purchasing, furnishing, or financing of goods or services locally in the United States." ;
      composite:index "1"^^xsd:int ;
    ] ;

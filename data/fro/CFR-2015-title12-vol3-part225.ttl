  composite:child cfr-12-225:r-1-7-7-2-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) Section 8 of the BHC Act provides criminal penalties for willful violation, and civil" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "penalties for violation, by any company or individual, of the BHC Act or any regulation or order issued under it, or for making a false entry in any book, report, or statement of a bank holding company." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

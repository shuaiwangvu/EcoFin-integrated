      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Except as provided in § 225.22 of this subpart, a bank holding company or a subsidiary may not engage in, or acquire or control, directly or indirectly, voting securities or assets of a company engaged in, any activity other than:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

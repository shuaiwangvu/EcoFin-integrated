  composite:child cfr-12-225:r-1-10-1-20-1 ;
  composite:child cfr-12-225:r-1-10-1-20-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A company that, together with its management officials" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "or controlling shareholders (including members of the immediate families of either), owns, controls, or holds with power to vote 25 percent or more of the outstanding shares of any class of voting securities of a bank or other company controls the bank or other company, if the first company owns, controls, or holds with power to vote more than 5 percent of the outstanding shares of any class of voting securities of the bank or other company." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.

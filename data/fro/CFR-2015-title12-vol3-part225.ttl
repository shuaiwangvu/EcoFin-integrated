cfr-12-225:r-1-18-3-72-0
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-72-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Allocated transfer risk reserves are reserves that have been established in accordance with Section 905(a) of the International Lending Supervision Act of 1983, 12 U.S.C. 3904(a), against certain assets whose value U.S. supervisory authorities have found to be significantly impaired by protracted transfer risk problems." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

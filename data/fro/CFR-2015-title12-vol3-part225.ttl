cfr-12-225:r-1-8-6-6
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-8-6-6-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Board shall act on an application under this subpart that is referred to it for decision within 60 calendar days after the acceptance date for the application, unless the Board notifies the applicant that the 60-day period is being extended for a specified period and states the reasons for the extension. In no event may the extension exceed the 91-day period provided in § 225.16(f). The Board may, at any time, request additional information that it believes is necessary for its decision." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.

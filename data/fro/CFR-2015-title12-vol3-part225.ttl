  rdf:type cfr-fdsys-s:CITA ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "[32 FR 15004, Oct. 3, 1967, as amended at 35 FR 19662, Dec. 29, 1970. Redesignated at 36 FR 21666, Nov. 12, 1971]" ;
      composite:index "1"^^xsd:int ;

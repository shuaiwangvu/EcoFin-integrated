.
cfr-12-225:r-1-11-4-18
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The notificant(s) may consummate the proposed transaction before the expiration of the 60-day period if the Board notifies the notificant(s) in writing of the Board's intention not to disapprove the acquisition." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "18"^^xsd:int ;
.
cfr-12-225:r-1-11-4-19
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-11-4-19-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) The Board may extend the 60-day period in paragraph (d)(1) of this section for an additional 30 days by notifying the acquiring person(s)." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "19"^^xsd:int ;
.

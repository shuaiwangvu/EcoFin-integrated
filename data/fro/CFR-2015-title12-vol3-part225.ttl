    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-3-3-3
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Examples of routine management or operation" ;

      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Except as permitted in paragraph (e) of this section, a financial holding company may not routinely manage or operate any portfolio company." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-16-3-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "May a financial holding company routinely manage or operate a portfolio company?" ;

      rdf:type sxml:TextNode ;
      sxml:text "Bank holding company subsidiaries of U.S. intermediate holding companies required to be established by July 1, 2016." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-7-9-2
  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;

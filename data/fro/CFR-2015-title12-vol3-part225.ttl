  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-16-29-5-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Board notes that where the dpc shares or other similar interests represent less than 5 percent of the total of such interests outstanding, they may be retained on the basis of section 4(c)(6), even if originally acquired dpc." ;

.
cfr-12-225:r-1-10-1-22
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-10-1-22-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iv)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The presumptions in paragraphs (d)(2) (ii) and (iii) of this section do not apply if the securities are held by the company in a fiduciary capacity without sole discretionary authority to exercise the voting rights." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "22"^^xsd:int ;
.
cfr-12-225:r-1-10-1-22-1
  rdf:type cfr-fdsys-s:E ;

    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-6-3
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) 30 percent of the Tier 1 capital of the financial holding company; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-16-6-4
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) After excluding interests in private equity funds, 20 percent of the Tier 1 capital of the financial holding company." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-16-6-5
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-6-5-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Paragraph (a) of this section applies to the interest acquired or controlled by the financial holding company under this subpart in a private equity fund. Paragraph (a) of this section does not apply to any interest in a company held by a private equity fund or to any interest held by a person that is not affiliated with the financial holding company." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.

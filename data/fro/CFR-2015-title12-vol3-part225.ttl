cfr-12-225:r-1-15-4-14
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) The Board may impose any limitations or conditions on the conduct or activities of the company or any of its affiliates as the Board finds to be appropriate and consistent with the purposes of the BHC Act; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "14"^^xsd:int ;
.
cfr-12-225:r-1-15-4-15
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) The company and its affiliates may not commence any additional activity or acquire control or shares of any company under section 4(k) of the BHC Act without prior approval from the Board." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;
.
cfr-12-225:r-1-15-4-16
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-4-16-1 ;
  composite:child cfr-12-225:r-1-15-4-16-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "If a company does not correct the conditions described in a notice under paragraph (a) of this section within 180 days of receipt of the notice or such additional time as the Board may permit, the Board may order the company to divest ownership or control of any depository institution owned or controlled by the company. Such divestiture must be done in accordance with the terms and conditions established by the Board." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(1)" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "16"^^xsd:int ;
.
cfr-12-225:r-1-15-4-16-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Consequences of failure to correct conditions within 180 days" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-4-16-3
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Divestiture of depository institutions." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-15-4-17
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-4-17-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A company may comply with an order issued under paragraph (e)(1) of this section by ceasing to engage (both directly and through any subsidiary that is not a depository institution or a subsidiary of a depository institution) in any activity that may be conducted only under section 4(k), (n), or (o) of the BHC Act (12 U.S.C. 1843(k), (n), or (o)). The termination of activities must be completed within the time period referred to in paragraph (e)(1) of this section and in accordance with the terms and conditions acceptable to the Board." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "17"^^xsd:int ;
.
cfr-12-225:r-1-15-4-17-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;

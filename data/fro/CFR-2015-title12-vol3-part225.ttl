    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "However," ;
      composite:index "6"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Once the credit equivalent amount for a derivative contract, or a group of derivative contracts subject to a qualifying bilateral netting contract, has been determined, that amount is assigned to the risk category appropriate to the counterparty, or, if relevant, the guarantor or the nature of any collateral." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "the maximum risk weight applicable to the credit equivalent amount of such contracts is 50 percent." ;
      composite:index "8"^^xsd:int ;
    ] ;
  composite:index "364"^^xsd:int ;
.

  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A State certified appraiser or a State licensed appraiser may not be excluded from consideration for an assignment for a federally related transaction solely by virtue of membership or lack of membership in any particular appraisal organization." ;

      sxml:text "14" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.
cfr-12-225:r-1-18-3-75
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-75-1 ;
  composite:child cfr-12-225:r-1-18-3-75-3 ;
  composite:child cfr-12-225:r-1-18-3-75-4 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Perpetual preferred stock (and related surplus) that meets the requirements set forth in section II.A.1.c.ii.(1) of this appendix is eligible for inclusion in tier 2 capital without limit." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "b." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "75"^^xsd:int ;
.

cfr-12-225:r-1-18-3-398
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-398-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A banking organization that excludes assets of consolidated VIEs from risk-weighted assets pursuant to section IV.C.1.a. may, for the third and fourth quarters after the implementation date (phase-in period), including for the two calendar quarter-end regulatory report dates within those quarters, exclude from risk-weighted assets 50 percent of the exclusion amount, provided that the banking organization may not include in risk-weighted assets pursuant to this paragraph an amount less than the aggregate risk-weighted assets calculated pursuant to section IV.C.1.b." ;
      composite:index "3"^^xsd:int ;

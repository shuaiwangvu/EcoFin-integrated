  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A regulated institution shall give the Board 30 days' written notice, as specified in § 225.73, before adding or replacing any member of its board of directors, employing any person as a senior executive officer of the institution, or changing the responsibilities of any senior executive officer so that the person would assume a different senior executive officer position, if:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

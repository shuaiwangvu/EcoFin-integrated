  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Contents of request." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-3-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-3-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A bank holding company may elect to become a financial holding company by filing a written declaration with the appropriate Reserve Bank. A declaration by a bank holding company is considered to be filed on the date that all information required by paragraph (b) of this section is received by the appropriate Reserve Bank." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-15-3-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Filing requirement." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-3-20
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) State that the company seeks to become a financial holding company on consummation of its proposal to become a bank holding company; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.
cfr-12-225:r-1-15-3-21
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) Certify that each depository institution that would be controlled by the company on consummation of its proposal to become a bank holding company will be both well capitalized and well managed as of the date the company consummates the proposal." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "21"^^xsd:int ;
.
cfr-12-225:r-1-15-3-22
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-3-22-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A complete request submitted by a company under this paragraph (f) becomes a complete declaration by a bank holding company for purposes of section 4(l) of the BHC Act (12 U.S.C. 1843(l)) and becomes an effective election for purposes of § 225.81(b) on the date that the company lawfully consummates its proposal under section 3 of the BHC Act (12 U.S.C. 1842), unless the Board notifies the company at any time prior to consummation of the proposal and that:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "22"^^xsd:int ;
.
cfr-12-225:r-1-15-3-22-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Request becomes a declaration and an effective election on date of consummation of bank holding company proposal." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

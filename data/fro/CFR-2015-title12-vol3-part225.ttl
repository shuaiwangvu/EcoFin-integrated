      rdf:type sxml:TextNode ;
      sxml:text "Earlier notification that an election is effective." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-3-18
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-3-18-1 ;
  composite:child cfr-12-225:r-1-15-3-18-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(f)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A company that is not a bank holding company and has applied for the Board's approval to become a bank holding company under section 3(a)(1) of the BHC Act (12 U.S.C. 1842(a)(1)) may as part of that application submit a request to become a financial holding company." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;

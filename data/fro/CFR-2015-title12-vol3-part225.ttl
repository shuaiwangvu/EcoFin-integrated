cfr-12-225:r-1-8-6-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-8-6-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Except as provided in § 225.14, an application for the Board's prior approval under this subpart shall be governed by the provisions of this section and shall be filed with the appropriate Reserve Bank on the designated form." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-8-6-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Filing application." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-8-6-3
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-8-6-3-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Upon receipt of an application under this subpart, the Reserve Bank shall promptly furnish notice and a copy of the application to the primary banking supervisor of each bank to be acquired. The primary supervisor shall have 30 calendar days from the date of the letter giving notice in which to submit its views and recommendations to the Board." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-8-6-3-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Notice to primary banking supervisor." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-8-6-4
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-8-6-4-1 ;
  composite:child cfr-12-225:r-1-8-6-4-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Within 7 calendar days after the Reserve Bank receives an application under this section, the Reserve Bank shall accept it for processing as of the date the application was filed or return the application if it is substantially incomplete. Upon accepting an application, the Reserve Bank shall immediately send copies to the Board. The Reserve Bank or the Board may request additional information necessary to complete the record of an application at any time after accepting the application for processing." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.

      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Board may, at any time, on its own initiative or in response to a written request from any person, initiate a proceeding to determine whether any activity is so closely related to banking or managing or controlling banks as to be a proper incident thereto." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(1)" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.

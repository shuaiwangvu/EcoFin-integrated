    ] ;
  composite:index "25"^^xsd:int ;
.
cfr-12-225:r-1-16-3-26
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(vii) The sale, merger, consolidation, spin-off, recapitalization, liquidation, dissolution or sale of substantially all of the assets of the portfolio company or any of its significant subsidiaries." ;

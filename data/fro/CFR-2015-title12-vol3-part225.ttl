  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-13-6-3-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) If an appraisal is prepared by a fee appraiser, the appraiser shall be engaged directly by the regulated institution or its agent, and have no direct or indirect interest, financial or otherwise, in the property or the transaction." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.

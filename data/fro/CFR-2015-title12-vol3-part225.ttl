      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Upon request by a company, the Board may extend the 45-day period under paragraph (c)(1) of this section if the Board determines that granting additional time is appropriate under the circumstances. A request by a company for additional time must include an explanation of why an extension is necessary." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.

    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-10-1-23
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-10-1-23-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) In any proceeding under this section, there is a presumption that any company that directly or indirectly owns, controls, or has power to vote less than 5 percent of the outstanding shares of any class of voting securities of a bank or other company does not have control over that bank or other company." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "23"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Subordinated debt and intermediate-term preferred stock" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-3-83-3
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Five-year minimum maturity." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-18-3-84
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-84-1 ;
  composite:child cfr-12-225:r-1-18-3-84-3 ;
  composite:child cfr-12-225:r-1-18-3-84-5 ;
  composite:child cfr-12-225:r-1-18-3-84-6 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "12 CFR 250.166." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Accordingly, such subordinated debt must meet the following requirements:" ;
      composite:index "8"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Subordinated debt included in tier 2 capital must comply with the Federal Reserve's subordinated debt policy statement set forth in" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "ii." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "84"^^xsd:int ;
.
cfr-12-225:r-1-18-3-84-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Other restrictions on subordinated debt." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-3-84-3
  rdf:type cfr-fdsys-s:PRTPAGE ;
  cfr-12-225:P-PRTPAGE "287" ;
  composite:index "3"^^xsd:int ;
.

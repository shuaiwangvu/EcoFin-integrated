      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A book-entry securities transfer against payment shall be posted: (i) to the transferor's account at the time the entry is made by the transferor's Reserve Bank; and (ii) to the transferee's account at the time the entry is made by the transferee's Reserve Bank." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "25"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-16-33-40-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "During the Senate debates on CEBA, Senator Proxmire in response to a statement from Senator Cranston that the joint-marketing restrictions do not lock into place the specific terms or conditions of the particular grandfathered product or service, stated:" ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) The extension of credit is made pursuant to, and consistent with any conditions imposed in a preexisting line of credit that was not established in contemplation of the underwriting; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.
cfr-12-225:r-1-18-2-21
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The extension of credit is made in connection with clearing transactions for the section 20 affiliate." ;
      composite:index "1"^^xsd:int ;
    ] ;

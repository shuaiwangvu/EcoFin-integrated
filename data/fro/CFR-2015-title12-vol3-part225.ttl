      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Within 45 days after receiving a notice from the Board under paragraph (a) of this section, the company must execute an agreement acceptable to the Board to comply with all applicable capital and management requirements." ;
      composite:index "5"^^xsd:int ;
    ] ;

      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company must maintain and make available to the Board upon request a written record describing its involvement in routinely managing or operating a portfolio company." ;

.
cfr-12-225:r-1-15-13-10
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(4) The Board does not have sufficient information to assess whether the foreign bank or company making the election meets the requirements of this subpart." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "10"^^xsd:int ;
.
cfr-12-225:r-1-15-13-11
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-13-11-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(d)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "An insured depository institution will be excluded for purposes of the review of CRA ratings described in paragraph (c)(2) of this section consistent with the provisions of § 225.82(d)." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "11"^^xsd:int ;
.
cfr-12-225:r-1-15-13-11-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "How is CRA performance of recently acquired insured depository institutions considered?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-13-12
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-13-12-1 ;
  composite:child cfr-12-225:r-1-15-13-12-3 ;
  composite:child cfr-12-225:r-1-15-13-12-5 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In determining whether a foreign bank is well capitalized and well managed in accordance with comparable capital and management standards, the Board will give due regard to national treatment and equality of competitive opportunity. In this regard, the Board may take into account the foreign bank's composition of capital, Tier 1 capital to total assets leverage ratio, accounting standards, long-term debt ratings, reliance on government support to meet capital requirements, the foreign bank's anti-money laundering procedures, whether the foreign bank is subject to comprehensive supervision or regulation on a consolidated basis," ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "and other factors that may affect analysis of capital and management. The Board will consult with the home country supervisor for the foreign bank as appropriate." ;
      composite:index "7"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(1)" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "12"^^xsd:int ;
.
cfr-12-225:r-1-15-13-12-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Factors used in the Board's determination regarding comparability of capital and management" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-13-12-3
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In general." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-15-13-12-5
  rdf:type cfr-fdsys-s:PRTPAGE ;
  cfr-12-225:P-PRTPAGE "219" ;
  composite:index "5"^^xsd:int ;
.
cfr-12-225:r-1-15-13-13
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-13-13-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A foreign bank that is not subject to comprehensive supervision on a consolidated basis by its home country authorities may not be considered well capitalized and well managed unless:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "13"^^xsd:int ;
.
cfr-12-225:r-1-15-13-13-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Assessment of consolidated supervision." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-13-14
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) The home country has made significant progress in establishing arrangements for comprehensive supervision on a consolidated basis; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "14"^^xsd:int ;
.
cfr-12-225:r-1-15-13-15
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The foreign bank is in strong financial condition as demonstrated, for example, by capital levels that significantly exceed the minimum levels that are required for a well capitalized determination and strong asset quality." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;
.
cfr-12-225:r-1-15-13-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-13-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "An election described in § 225.91 is effective on the 31st day after the date that an election was received by the appropriate Federal Reserve Bank, unless the Board notifies the foreign bank or company prior to that time that:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-15-13-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In general." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-13-3
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) The election is ineffective; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-15-13-4
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) The period is extended with the consent of the foreign bank or company making the election." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-15-13-5
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-13-5-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Board or the appropriate Federal Reserve Bank may notify a foreign bank or company that its election to be treated as a financial holding company is effective prior to the 31st day after the election was filed with the appropriate Federal Reserve Bank. Such notification must be in writing." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.
cfr-12-225:r-1-15-13-5-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Earlier notification that an election is effective." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-13-6
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-13-6-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "An election to be treated as a financial holding company shall not be effective if, during the period provided in paragraph (a) of this section, the Board finds that:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.
cfr-12-225:r-1-15-13-6-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Under what circumstances will the Board find an election to be ineffective?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-13-7
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) The foreign bank certificant, or any foreign bank that operates a branch or agency or owns or controls a commercial lending company in the United States and is controlled by a foreign bank or company certificant, is not both well capitalized and well managed;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.
cfr-12-225:r-1-15-13-8
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;

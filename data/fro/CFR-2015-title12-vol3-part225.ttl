cfr-12-225:r-1-18-3-351-0
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-351-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A walkaway clause is a provision in a netting contract that permits a non-defaulting counterparty to make lower payments than it would make otherwise under the contract, or no payment at all, to a defaulter or to the estate of a defaulter, even if the defaulter or the estate of the defaulter is a net creditor under the contract." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Any person acting directly or indirectly, or through or in concert with one or more persons, shall give the Board 60 days' written notice, as specified in § 225.43 of this subpart, before acquiring control of a state member bank or bank holding company, unless the acquisition is exempt under § 225.42." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

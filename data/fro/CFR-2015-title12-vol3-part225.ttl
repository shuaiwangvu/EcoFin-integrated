  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) Submit for the Board's approval a specific plan for the prompt termination of the control relationship;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.
cfr-12-225:r-1-10-1-8
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) File an application under subpart B or C of this regulation to retain the control relationship; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "8"^^xsd:int ;
.
cfr-12-225:r-1-10-1-9
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3) Contest the preliminary determination by filing a response, setting forth the facts and circumstances in support of its position that no control exists, and, if desired, requesting a hearing or other proceeding." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "9"^^xsd:int ;
.

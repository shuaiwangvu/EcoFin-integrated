      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "For purposes of paragraph (b)(1) of this section, equity capital includes options, warrants and any other instrument convertible into equity capital." ;

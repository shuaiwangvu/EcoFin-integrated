  composite:child cfr-12-225:r-1-15-3-23-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) Any depository institution that would be controlled by the company on consummation of the proposal will not" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "be both well capitalized and well managed on the date of consummation; or" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "23"^^xsd:int ;
.

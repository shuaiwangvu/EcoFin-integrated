cfr-12-225:r-1-15-14-14
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) The Board may impose any limitations or conditions on the conduct or the U.S. activities of the foreign bank or company or any of its affiliates as the Board finds to be appropriate and consistent with the purposes of the Bank Holding Company Act; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "14"^^xsd:int ;
.
cfr-12-225:r-1-15-14-15
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) The foreign bank or company and its affiliates may not commence any additional activity in the United States or acquire control or shares of any company under section 4(k) of the Bank Holding Company Act (12 U.S.C. 1843(k)) without prior approval from the Board." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;
.
cfr-12-225:r-1-15-14-16
  rdf:type cfr-fdsys-s:P ;

  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(4)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company controls a private equity fund for purposes of this subpart if the financial holding company, including any director, officer, employee or principal shareholder of the financial holding company:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;
.
cfr-12-225:r-1-16-5-15-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "When does a financial holding company control a private equity fund?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-5-16
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) Serves as a general partner, managing member, or trustee of the private equity fund (or serves in a similar role with respect to the private equity fund);" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "16"^^xsd:int ;
.
cfr-12-225:r-1-16-5-17
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-5-17-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) Owns or controls 25 percent or more of any class of voting shares or similar interests in the private equity fund;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "17"^^xsd:int ;
.
cfr-12-225:r-1-16-5-17-1
  rdf:type cfr-fdsys-s:PRTPAGE ;
  cfr-12-225:P-PRTPAGE "270" ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-5-18
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iii) In any manner selects, controls or constitutes a majority of the directors, trustees or management of the private equity fund; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "18"^^xsd:int ;
.
cfr-12-225:r-1-16-5-19
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iv) Owns or controls more than 5 percent of any class of voting shares or similar interests in the private equity fund and is the investment adviser to the fund." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "19"^^xsd:int ;
.
cfr-12-225:r-1-16-5-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-5-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "For purposes of this subpart, a “private equity fund” is any company that:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-16-5-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "What is a private equity fund?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-5-3
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) Is formed for the purpose of and is engaged exclusively in the business of investing in shares, assets, and ownership interests of financial and nonfinancial companies for resale or other disposition;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-16-5-4
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) Is not an operating company;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-16-5-5
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3) No more than 25 percent of the total equity of which is held, owned or controlled, directly or indirectly, by the financial holding company and its directors, officers, employees and principal shareholders;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.
cfr-12-225:r-1-16-5-6
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(4) Has a maximum term of not more than 15 years; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.
cfr-12-225:r-1-16-5-7
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(5) Is not formed or operated for the purpose of making investments inconsistent with the authority granted under section 4(k)(4)(H) of the Bank Holding Company Act (12 U.S.C. 1843(k)(4)(H)) or evading the limitations governing merchant banking investments contained in this subpart." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.
cfr-12-225:r-1-16-5-8
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-5-8-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A private equity fund may be a corporation, partnership, limited liability company or other type of company that issues ownership interests in any form." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "8"^^xsd:int ;
.
cfr-12-225:r-1-16-5-8-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "What form may a private equity fund take?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-5-9
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-5-9-1 ;
  composite:child cfr-12-225:r-1-16-5-9-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company may own, control or hold any interest in a private equity fund under this subpart and any interest in a portfolio company that is owned or controlled by a private equity fund in which the financial holding company owns or controls any interest under this subpart for the duration of the fund, up to a maximum of 15 years." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(1)" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "9"^^xsd:int ;
.
cfr-12-225:r-1-16-5-9-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "What is the holding period permitted for interests in private equity funds?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

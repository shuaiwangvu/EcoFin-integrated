cfr-12-225:r-1-7-9-122
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(A) To the extent that the Board or appropriate Reserve Bank indicates in writing its non-objection pursuant to paragraph (g)(5) of this section, following a request for non-objection from the bank holding company that includes all of the information required to be submitted under paragraph (g)(4) of this section;" ;
      composite:index "1"^^xsd:int ;
    ] ;

    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-10-1-5
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) If the Board makes a preliminary determination of control under this section, the Board shall send notice to the controlling company containing a statement of the facts upon which the preliminary determination is based." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.

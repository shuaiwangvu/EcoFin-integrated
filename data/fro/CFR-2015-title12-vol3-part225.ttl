    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A bank holding company shall comply with the Interagency Guidelines Establishing Information Security Standards, as set forth in appendix F of this part, prescribed pursuant to sections 501 and 505 of the Gramm-Leach-Bliley Act (15 U.S.C. 6801 and 6805). A bank holding company shall properly dispose of consumer information in accordance with the rules set forth at 16 CFR part 682." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "31"^^xsd:int ;
.

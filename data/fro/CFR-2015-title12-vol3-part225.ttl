cfr-12-225:r-1-18-7-20
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-20-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Guidelines apply to customer information maintained by or on behalf of bank holding companies and their nonbank subsidiaries or affiliates (except brokers, dealers, persons providing insurance, investment companies, and investment advisors), for which the Board has supervisory authority." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.

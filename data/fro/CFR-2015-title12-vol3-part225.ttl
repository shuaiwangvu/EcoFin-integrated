.
cfr-12-225:r-1-18-3-127
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-127-1 ;
  composite:child cfr-12-225:r-1-18-3-127-2 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In general, when investments in a consolidated subsidiary are deducted from a consolidated parent banking organization's capital, the subsidiary's assets will also be excluded from the consolidated assets of the parent banking organization in order to assess the latter's capital adequacy." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "127"^^xsd:int ;
.
cfr-12-225:r-1-18-3-127-1
  rdf:type cfr-fdsys-s:SU ;

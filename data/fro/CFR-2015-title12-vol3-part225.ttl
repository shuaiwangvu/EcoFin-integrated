cfr-12-225:r-1-18-7-32-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Objectives." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-33
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. Ensure the security and confidentiality of customer information;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "33"^^xsd:int ;
.
cfr-12-225:r-1-18-7-34
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2. Protect against any anticipated threats or hazards to the security or integrity of such information; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "34"^^xsd:int ;
.
cfr-12-225:r-1-18-7-35
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "3. Protect against unauthorized access to or use of such information that could result in substantial harm or inconvenience to any customer." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "35"^^xsd:int ;
.
cfr-12-225:r-1-18-7-36
  rdf:type cfr-fdsys-s:HD ;
  cfr-12-225:SOURCE-HD "HD1" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "III. Development and Implementation of Information Security Program" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "36"^^xsd:int ;
.
cfr-12-225:r-1-18-7-37
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-37-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The board of directors or an appropriate committee of the board of each bank holding company shall:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "37"^^xsd:int ;
.
cfr-12-225:r-1-18-7-37-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Involve the Board of Directors." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-38
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. Approve the bank holding company's written information security program; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "38"^^xsd:int ;
.
cfr-12-225:r-1-18-7-39
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2. Oversee the development, implementation, and maintenance of the bank holding company's information security program, including assigning specific responsibility for its implementation and reviewing reports from management." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "39"^^xsd:int ;
.
cfr-12-225:r-1-18-7-4
  rdf:type cfr-fdsys-s:FP ;
  cfr-12-225:SOURCE-FP "FP1-2" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A. Scope" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-18-7-40
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-40-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "B." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Each bank holding company shall:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "40"^^xsd:int ;
.
cfr-12-225:r-1-18-7-40-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Assess Risk." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-41
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. Identify reasonably foreseeable internal and external threats that could result in unauthorized disclosure, misuse, alteration, or destruction of customer information or customer information systems." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "41"^^xsd:int ;
.
cfr-12-225:r-1-18-7-42
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2. Assess the likelihood and potential damage of these threats, taking into consideration the sensitivity of customer information." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "42"^^xsd:int ;
.
cfr-12-225:r-1-18-7-43
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "3. Assess the sufficiency of policies, procedures, customer information systems, and other arrangements in place to control risks." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "43"^^xsd:int ;
.
cfr-12-225:r-1-18-7-44
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-44-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "C." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Each bank holding company shall:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "44"^^xsd:int ;
.
cfr-12-225:r-1-18-7-44-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Manage and Control Risk." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-45
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. Design its information security program to control the identified risks, commensurate with the sensitivity of the information as well as the complexity and scope of the bank holding company's activities. Each bank holding company must consider whether the following security measures are appropriate for the bank holding company and, if so, adopt those measures the bank holding company concludes are appropriate:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "45"^^xsd:int ;
.
cfr-12-225:r-1-18-7-46
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "a. Access controls on customer information systems, including controls to authenticate and permit access only to authorized individuals and controls to prevent employees from providing customer information to unauthorized individuals who may seek to obtain this information through fraudulent means." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "46"^^xsd:int ;
.
cfr-12-225:r-1-18-7-47
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "b. Access restrictions at physical locations containing customer information, such as buildings, computer facilities, and records storage facilities to permit access only to authorized individuals;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "47"^^xsd:int ;
.
cfr-12-225:r-1-18-7-48
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "c. Encryption of electronic customer information, including while in transit or in storage on networks or systems to which unauthorized individuals may have access;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "48"^^xsd:int ;
.
cfr-12-225:r-1-18-7-49
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "d. Procedures designed to ensure that customer information system modifications are consistent with the bank holding company's information security program;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "49"^^xsd:int ;
.
cfr-12-225:r-1-18-7-5
  rdf:type cfr-fdsys-s:FP ;
  cfr-12-225:SOURCE-FP "FP1-2" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "B. Preservation of Existing Authority" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.
cfr-12-225:r-1-18-7-50
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "e. Dual control procedures, segregation of duties, and employee background checks for employees with responsibilities for or access to customer information;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "50"^^xsd:int ;
.
cfr-12-225:r-1-18-7-51
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "f. Monitoring systems and procedures to detect actual and attempted attacks on or intrusions into customer information systems;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "51"^^xsd:int ;
.
cfr-12-225:r-1-18-7-52
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "g. Response programs that specify actions to be taken when the bank holding company suspects or detects that unauthorized individuals have gained access to customer information systems, including appropriate reports to regulatory and law enforcement agencies; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "52"^^xsd:int ;
.
cfr-12-225:r-1-18-7-53
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "h. Measures to protect against destruction, loss, or damage of customer information due to potential environmental hazards, such as fire and water damage or technological failures." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "53"^^xsd:int ;
.
cfr-12-225:r-1-18-7-54
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2. Train staff to implement the bank holding company's information security program." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "54"^^xsd:int ;
.
cfr-12-225:r-1-18-7-55
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-55-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "3. Regularly test the key controls, systems and procedures of the information security program. The frequency and nature of such tests should be determined by the bank holding company's risk assessment. Tests should be conducted or reviewed by independent third parties or staff independent of those" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "that develop or maintain the security programs." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "55"^^xsd:int ;
.
cfr-12-225:r-1-18-7-55-1
  rdf:type cfr-fdsys-s:PRTPAGE ;
  cfr-12-225:P-PRTPAGE "319" ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-56
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-56-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "D." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Each bank holding company shall:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "56"^^xsd:int ;
.
cfr-12-225:r-1-18-7-56-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Oversee Service Provider Arrangements." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-57
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1. Exercise appropriate due diligence in selecting its service providers;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "57"^^xsd:int ;
.
cfr-12-225:r-1-18-7-58
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2. Require its service providers by contract to implement appropriate measures designed to meet the objectives of these Guidelines; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "58"^^xsd:int ;
.
cfr-12-225:r-1-18-7-59
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "3. Where indicated by the bank holding company's risk assessment, monitor its service providers to confirm that they have satisfied their obligations as required by paragraph D.2. As part of this monitoring, a bank holding company should review audits, summaries of test results, or other equivalent evaluations of its service providers." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "59"^^xsd:int ;
.
cfr-12-225:r-1-18-7-6
  rdf:type cfr-fdsys-s:FP ;
  cfr-12-225:SOURCE-FP "FP1-2" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "C. Definitions" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.
cfr-12-225:r-1-18-7-60
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-60-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "E." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Each bank holding company shall monitor, evaluate, and adjust, as appropriate, the information security program in light of any relevant changes in technology, the sensitivity of its customer information, internal or external threats to information, and the bank holding company's own changing business arrangements, such as mergers and acquisitions, alliances and joint ventures, outsourcing arrangements, and changes to customer information systems." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "60"^^xsd:int ;
.
cfr-12-225:r-1-18-7-60-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Adjust the Program." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-61
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-61-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Each bank holding company shall report to its board or an appropriate committee of the board at least annually. This report should describe the overall status of the information security program and the bank holding company's compliance with these Guidelines. The reports should discuss material matters related to its program, addressing issues such as: risk assessment; risk management and control decisions; service provider arrangements; results of testing; security breaches or violations and management's responses; and recommendations for changes in the information security program." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "F." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "61"^^xsd:int ;
.
cfr-12-225:r-1-18-7-61-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Report to the Board." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-62
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-62-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "G." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "62"^^xsd:int ;
.
cfr-12-225:r-1-18-7-62-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Implement the Standards." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-63
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-63-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Each bank holding company must implement an information security program pursuant to these Guidelines by July 1, 2001." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "63"^^xsd:int ;
.
cfr-12-225:r-1-18-7-63-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Effective date." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-64
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-64-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Until July 1, 2003, a contract that a bank holding company has entered into with a service provider to perform services for it or functions on its behalf satisfies the provisions of section III.D., even if the contract does not include a requirement that the servicer maintain the security and confidentiality of customer information, as long as the bank holding company entered into the contract on or before March 5, 2001." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "64"^^xsd:int ;
.
cfr-12-225:r-1-18-7-64-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Two-year grandfathering of agreements with service providers." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-65
  rdf:type cfr-fdsys-s:HD ;
  cfr-12-225:SOURCE-HD "HD1" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Supplement A to Appendix F to Part 225—Interagency Guidance on Response Programs for Unauthorized Access to Customer Information and Customer Notice" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "65"^^xsd:int ;
.
cfr-12-225:r-1-18-7-66
  rdf:type cfr-fdsys-s:HD ;
  cfr-12-225:SOURCE-HD "HD1" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "I. Background" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "66"^^xsd:int ;
.
cfr-12-225:r-1-18-7-67
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-67-1 ;
  composite:child cfr-12-225:r-1-18-7-67-2 ;
  composite:child cfr-12-225:r-1-18-7-67-4 ;
  composite:child cfr-12-225:r-1-18-7-67-5 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "This Guidance" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "and describes response programs, including customer notification procedures, that a financial institution should develop and implement to address unauthorized access to or use of customer information that could result in substantial harm or inconvenience to a customer. The scope of, and definitions of terms used in, this Guidance are identical to those of the Security Guidelines. For example, the term “customer information” is the same term used in the Security Guidelines, and means any record containing nonpublic personal information about a customer, whether in paper, electronic, or other form, maintained by or on behalf of the institution." ;
      composite:index "7"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "interprets section 501(b) of the Gramm-Leach-Bliley Act (“GLBA”) and the Interagency Guidelines Establishing Information Security Standards (the “Security Guidelines”)" ;
      composite:index "4"^^xsd:int ;
    ] ;
  composite:index "67"^^xsd:int ;
.
cfr-12-225:r-1-18-7-67-1
  rdf:type cfr-fdsys-s:SU ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "1" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-7-67-2
  rdf:type cfr-fdsys-s:FTREF ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-18-7-67-4
  rdf:type cfr-fdsys-s:SU ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "2" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-18-7-67-5
  rdf:type cfr-fdsys-s:FTREF ;
  composite:index "5"^^xsd:int ;
.
cfr-12-225:r-1-18-7-68
  rdf:type cfr-fdsys-s:FTNT ;
  composite:child cfr-12-225:r-1-18-7-68-0 ;
  composite:index "68"^^xsd:int ;
.
cfr-12-225:r-1-18-7-68-0
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-7-68-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "This Guidance is being jointly issued by the Board of Governors of the Federal Reserve System (Board), the Federal Deposit Insurance Corporation (FDIC), the Office of the Comptroller of the Currency (OCC), and the Office of Thrift Supervision (OTS)." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;

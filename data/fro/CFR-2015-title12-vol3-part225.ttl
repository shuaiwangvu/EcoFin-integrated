cfr-12-225:r-1-15-16-13-1
  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Bank holding company ownership of mortgage companies." ;
      composite:index "1"^^xsd:int ;

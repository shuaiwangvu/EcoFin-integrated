.
cfr-12-225:r-1-18-3-361
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-361-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "3.h.i. of this appendix A). Net negative mark-to-market values for individual counterparties may not be used to offset net positive current exposures for other counterparties." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "ii. Under the aggregate approach, the NGR is the ratio of the sum of all of the net current exposures for qualifying bilateral netting contracts to the sum of all of the gross current exposures for those netting contracts (each gross current exposure is calculated in the same manner as in section III." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "361"^^xsd:int ;
.
cfr-12-225:r-1-18-3-361-1
  rdf:type cfr-fdsys-s:E ;

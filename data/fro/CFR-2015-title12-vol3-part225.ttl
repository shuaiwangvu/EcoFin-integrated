.
cfr-12-225:r-1-18-3-69
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-69-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The maximum amount of tier 2 capital that may be included in an institution's qualifying total capital is limited to 100 percent of tier 1 capital (net of goodwill, other intangible assets, interest-only strips receivables and nonfinancial equity investments that are required to be deducted in accordance with section II." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "of this appendix A)." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "69"^^xsd:int ;

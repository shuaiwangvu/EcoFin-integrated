      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company may not routinely manage or operate a portfolio company for a period greater than nine months without prior written notice to the Board." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "33"^^xsd:int ;
.
cfr-12-225:r-1-16-3-33-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Notice required for extended involvement." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

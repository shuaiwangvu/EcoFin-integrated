  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-70-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The elements of supplementary capital are discussed in greater detail below." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "70"^^xsd:int ;
.

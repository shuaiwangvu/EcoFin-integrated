    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Generally, investments for this purpose are defined as equity and debt capital investments and any other instruments that are deemed to be capital in the particular subsidiary." ;
      composite:index "11"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The aggregate amount of investments in banking or finance subsidiaries" ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;

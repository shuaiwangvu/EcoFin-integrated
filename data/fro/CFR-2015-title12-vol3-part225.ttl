  composite:child cfr-12-225:r-1-15-16-28-13-7 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3) Although section 2(g)(3) refers to transfers of" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Furthermore, the transfer of all or substantially all of the assets of a company, or the transfer of such a significant volume of assets that the transfer may in effect constitute the disposition" ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "it is not, in the Board's view, limited to disposition of corporate stock. General or limited partnership interests, for example, are included within the term" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "of a separate activity of the company, is deemed by the Board to involve a transfer of" ;

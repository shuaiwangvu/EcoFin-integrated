      rdf:type sxml:TextNode ;
      sxml:text "(i) To the transferor's account at the time the transfer is actually made by the transferor's Federal Reserve Bank; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "23"^^xsd:int ;
.
cfr-12-225:r-1-12-1-24
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) To the transferee's account at the time the transferee's Reserve Bank sends the transfer item or sends or telephones the advice of credit for the item to the transferee, whichever occurs first." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "24"^^xsd:int ;
.
cfr-12-225:r-1-12-1-25
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-12-1-25-1 ;

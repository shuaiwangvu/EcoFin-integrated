cfr-12-225:r-1-15-16-26-3-0
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-16-26-3-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "It should be noted that every Board Order granting approval under section 4(c)(8) of the Act contains the following paragraph:" ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

cfr-12-225:r-1-7-5-13-0
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-7-5-13-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Before January 1, 2015, the Board will consider the financial standards at 12 CFR part 225 appendices A, C, and E for a bank holding company that is not an advanced approaches bank holding company." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

.
cfr-12-225:r-1-18-5-23
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Failure to meet the criteria in this section would normally result in denial of an application." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "23"^^xsd:int ;
.

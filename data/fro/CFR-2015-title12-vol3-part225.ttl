  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Small bank holding companies formed before the effective date of this policy statement may switch to a plan that adheres to the intent of this statement provided they comply with the requirements set forth above." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "18"^^xsd:int ;
.

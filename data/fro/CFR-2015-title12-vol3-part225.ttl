      rdf:type sxml:TextNode ;
      sxml:text "(5)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Any intra-day extension of credit to a section 20 subsidiary by an affiliated bank, thrift, branch or agency shall be on market terms consistent with section 23B of the Federal Reserve Act." ;

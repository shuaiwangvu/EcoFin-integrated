      sxml:text "(i) Provide financial, investment and management consulting advice to a" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "portfolio company in a manner consistent with and subject to any restrictions on such activities contained in §§ 225.28(b)(6) or 225.86(b)(1) of this part (12 CFR 225.28(b)(6) and 225.86(b)(1));" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "28"^^xsd:int ;
.

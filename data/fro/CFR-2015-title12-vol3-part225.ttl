      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A private equity fund may routinely manage or operate a portfolio company so long as no financial holding company controls the private equity fund or as permitted under § 225.171(e)." ;

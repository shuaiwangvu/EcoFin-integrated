      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A private equity fund that is controlled by a financial holding company may not routinely manage or operate a portfolio company, except as permitted under § 225.171(e)." ;

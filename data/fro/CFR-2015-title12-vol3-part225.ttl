  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) In addition, the aggregate amount of restricted core capital elements (other than qualifying mandatory convertible preferred securities" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;

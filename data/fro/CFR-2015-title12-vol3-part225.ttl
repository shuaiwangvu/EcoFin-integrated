cfr-12-225:r-1-15-16-14-10
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(C) Facilities for the use of the excess capacity do not include the provision of any software, other than systems software (including language), network communications support, and the operating personnel and documentation necessary for the maintenance and use of these facilities." ;
      composite:index "1"^^xsd:int ;
    ] ;

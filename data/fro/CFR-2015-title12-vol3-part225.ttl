  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-11-2-20-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(f)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Transactions other than those set forth in paragraph (c) of this section resulting in a person's control of less than 25 percent of a class of voting securities of a state member bank or bank holding company are not deemed by the Board to constitute control for purposes of the Bank Control Act." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.

      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Reserve Bank shall approve an application under this section within 30 calendar days after the acceptance date for the application, unless the Reserve Bank, upon notice to the applicant, refers the application to the Board for decision because action under delegated authority is not appropriate." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "—(1)" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.

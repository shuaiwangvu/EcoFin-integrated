cfr-12-225:r-1-18-3-71
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-71-1 ;
  composite:child cfr-12-225:r-1-18-3-71-3 ;
  composite:child cfr-12-225:r-1-18-3-71-4 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Allowances for loan and lease losses are reserves that have been established through a charge against earnings to absorb future losses on loans or lease financing receivables. Allowances for loan and lease losses exclude “allocated transfer risk reserves,”" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "a." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "and reserves created against identified losses." ;
      composite:index "6"^^xsd:int ;
    ] ;
  composite:index "71"^^xsd:int ;
.
cfr-12-225:r-1-18-3-71-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Allowance for loan and lease losses." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

  composite:child cfr-12-225:r-1-18-3-126-0-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In assessing the overall capital adequacy of a banking organization, the Federal Reserve may also consider the organization's fully consolidated capital position." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

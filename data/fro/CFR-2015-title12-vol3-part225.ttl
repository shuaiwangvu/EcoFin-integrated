cfr-12-225:r-1-7-7-3
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) Civil money penalty assessments for violations of the BHC Act shall be made in accordance with subpart C of the Board's Rules of Practice for Hearings (12 CFR part 263, subpart C). For any willful violation of the Bank Control Act or any regulation or order issued under it, the Board may assess a civil penalty as provided in 12 U.S.C. 1817(j)(15)." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.

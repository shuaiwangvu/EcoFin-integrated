  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "B." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "These Guidelines do not in any way limit the authority of the Board to address unsafe or unsound practices, violations of law, unsafe or unsound conditions, or other practices. The Board may take action under these Guidelines independently of, in conjunction with, or in addition to, any other enforcement action available to the Board." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "21"^^xsd:int ;
.

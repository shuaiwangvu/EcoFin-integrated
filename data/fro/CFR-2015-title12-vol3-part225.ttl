cfr-12-225:r-1-16-2-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-2-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Section 4(k)(4)(H) of the Bank Holding Company Act (12 U.S.C. 1843(k)(4)(H)) and this subpart authorize a financial holding company, directly or indirectly and as principal or on behalf of one or more persons, to acquire or control any amount of shares, assets or ownership interests of a company or other entity that is engaged in any activity not otherwise authorized for the financial holding company under section 4 of the Bank Holding Company Act. For purposes of this subpart, shares, assets or ownership interests acquired or controlled under section 4(k)(4)(H) and this subpart are referred to as “merchant banking investments.” A financial holding company may not directly or indirectly acquire or control any merchant banking investment except in compliance with the requirements of this subpart." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-16-2-2-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "What types of investments are permitted by this subpart?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-2-3
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-2-3-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The acquisition or control of shares, assets or ownership interests under this subpart is not permitted unless it is part of a bona fide underwriting or merchant or investment banking activity." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
cfr-12-225:r-1-16-2-3-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Must the investment be a bona fide merchant banking investment?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-2-4
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-2-4-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Shares, assets or ownership interests of a company or other entity include any debt or equity security, warrant, option, partnership interest, trust certificate or other instrument representing an ownership interest in the company or entity, whether voting or nonvoting." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
cfr-12-225:r-1-16-2-4-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "What types of ownership interests may be acquired?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-16-2-5
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-16-2-5-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(d)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A financial holding company and any subsidiary (other than a depository institution or subsidiary of a depository institution) may acquire or control merchant banking investments. A financial holding company and its subsidiaries may not acquire or control merchant banking investments on behalf of a depository institution or subsidiary of a depository institution." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.

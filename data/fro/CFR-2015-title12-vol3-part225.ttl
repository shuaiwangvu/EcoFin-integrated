  composite:child cfr-12-225:r-1-18-3-350-2 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "b. A contract containing a walkaway clause is not eligible for netting for purposes of calculating the credit equivalent amount." ;
      composite:index "1"^^xsd:int ;

  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The Board believes that the term" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "This approach is consistent with the treatment in CEBA's legislative history of certificates of deposit as a product line rather than each particular type of CD as a separate product." ;
      composite:index "10"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "a one-year certificate of deposit) simply represent a difference in the terms of the product." ;
      composite:index "7"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "certificates of deposit) and differences in versions of the product (" ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "must be interpreted in light of its accepted ordinary commercial usage. In some instances, commercial usage has identified a group of products so closely related that they constitute a product line (" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "38"^^xsd:int ;
.

    ] ;
  composite:index "0"^^xsd:int ;
.
cfr-12-225:r-1-18-3-73
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-73-1 ;
  composite:child cfr-12-225:r-1-18-3-73-2 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "During the transition period, the risk-based capital guidelines provide for reducing the amount of this allowance that may be included in an institution's total capital. Initially, it is unlimited. However, by year-end 1990, the amount of the allowance for loan and lease losses that will qualify as capital will be limited to 1.5 percent of an institution's weighted risk assets. By the end of the transition period, the amount of the allowance qualifying for inclusion in Tier 2 capital may not exceed 1.25 percent of weighted risk assets." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "73"^^xsd:int ;
.
cfr-12-225:r-1-18-3-73-1
  rdf:type cfr-fdsys-s:SU ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "14" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-18-3-73-2
  rdf:type cfr-fdsys-s:FTREF ;
  composite:index "2"^^xsd:int ;
.
cfr-12-225:r-1-18-3-74
  rdf:type cfr-fdsys-s:FTNT ;
  composite:child cfr-12-225:r-1-18-3-74-0 ;

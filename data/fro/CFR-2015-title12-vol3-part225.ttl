  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-7-9-12
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-7-9-12-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(5)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Unless the context otherwise requires, any reference to bank holding company in this section shall include a U.S. intermediate holding company and shall include a nonbank financial company supervised by the Board to the extent this section is made applicable pursuant to a rule or order of the Board." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "12"^^xsd:int ;
.

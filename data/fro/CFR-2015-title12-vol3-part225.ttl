  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-15-16-33-20-0-1 ;
  composite:child cfr-12-225:r-1-15-16-33-20-0-2 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "With respect to deposits, the Committee recognizes that it is legitimate for an institution currently involved in offering demand deposits or other third party transaction accounts to make use of new technologies that are in the process of replacing the existing check-based, paper payment system. Again, however, the Committee does not believe that technology should be used as a lever for an institution that was only incidentally involved in the payment system to transform itself into a significant offeror of transaction account capability." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

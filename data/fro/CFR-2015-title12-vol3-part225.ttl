      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In acting upon a notice filed under this subpart, the Board shall consider all public comments received in writing within the period specified in the newspaper or" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "announcement, whichever is later. At the Board's option, comments received after this period may, but need not, be considered." ;
      composite:index "5"^^xsd:int ;
    ] ;

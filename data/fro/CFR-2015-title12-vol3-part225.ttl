  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) A regulated institution also may accept an appraisal that was prepared by an appraiser engaged directly by another financial services institution, if:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.

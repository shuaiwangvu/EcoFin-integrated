cfr-12-225:r-1-18-3-146
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-225:r-1-18-3-146-1 ;
  composite:child cfr-12-225:r-1-18-3-146-3 ;
  composite:child cfr-12-225:r-1-18-3-146-5 ;
  composite:child cfr-12-225:r-1-18-3-146-6 ;
  composite:child cfr-12-225:r-1-18-3-146-8 ;
  composite:child cfr-12-225:r-1-18-3-146-9 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "); under section 4(c)(6) or 4(c)(7) of BHC Act in a nonfinancial company or in a company that makes investments in nonfinancial companies; in a nonfinancial company through a small business investment company (SBIC) under section 302(b) of the Small Business Investment Act of 1958;" ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A nonfinancial company is an entity that engages in any activity that has not been determined to be financial in nature or incidental to financial activities under section 4(k) of the Bank Holding Company Act (12 U.S.C. 1843(k))." ;
      composite:index "11"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A nonfinancial equity investment means any equity investment held by the bank holding company: under the merchant banking authority of section 4(k)(4)(H) of the BHC Act and subpart J of the Board's Regulation Y (12 CFR 225.175" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "b." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "in a nonfinancial company under the portfolio investment provisions of the Board's Regulation K (12 CFR 211.8(c)(3)); or in a nonfinancial company under section 24 of the Federal Deposit Insurance Act (other than section 24(f))." ;
      composite:index "8"^^xsd:int ;
    ] ;
  composite:index "146"^^xsd:int ;
.
cfr-12-225:r-1-18-3-146-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-225:T-E "03" ;

cfr-12-225:r-1-15-3-1
  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "How does a bank holding company elect to become a financial holding company?" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-225:r-1-15-3-10
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) Any insured depository institution controlled by the bank holding company (except an institution excluded under paragraph (d) of this section) has not achieved at least a rating of “satisfactory record of meeting community credit needs” under the Community Reinvestment Act at the institution's most recent examination; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "10"^^xsd:int ;
.
cfr-12-225:r-1-15-3-11
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) Any depository institution controlled by the bank holding company is not both well capitalized and well managed." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "11"^^xsd:int ;
.
cfr-12-225:r-1-15-3-12
  rdf:type cfr-fdsys-s:P ;

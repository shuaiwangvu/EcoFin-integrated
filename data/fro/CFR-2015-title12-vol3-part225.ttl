  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "iii. The banking organization establishes and maintains procedures to ensure that the legal characteristics of netting contracts are kept under review in the light of possible changes in relevant law." ;

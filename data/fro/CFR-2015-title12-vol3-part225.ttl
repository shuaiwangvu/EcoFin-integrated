      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Board seeks to act on every notice referred to it for decision within 60 days of the date that the notice is filed with the Reserve Bank. If the Board is unable to act within this period, the Board shall notify the notificant and explain the reasons and the date by which the Board expects to act." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "31"^^xsd:int ;
.

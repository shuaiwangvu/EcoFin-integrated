  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Board may require a covered company to use one or more additional scenarios in the stress test required by this section based on the company's financial condition, size, complexity, risk profile, scope of operations, or activities, or risks to the U.S. economy." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.

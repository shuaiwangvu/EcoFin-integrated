  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The board of directors, or a committee thereof, of a covered company must approve and review the policies and procedures of the stress testing processes as frequently as economic conditions or the condition of the covered company may warrant, but no less than annually. The board of directors and senior management of the covered company must receive a summary of the results of any stress test conducted under this subpart." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "13"^^xsd:int ;
.

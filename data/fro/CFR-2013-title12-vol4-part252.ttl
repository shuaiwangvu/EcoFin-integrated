  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-252:r-1-6-3-2-1 ;
  composite:child cfr-12-252:r-1-6-3-2-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1)" ;

cfr-12-252:r-1-6-4-1
  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Annual analysis conducted by the Board." ;
      composite:index "1"^^xsd:int ;

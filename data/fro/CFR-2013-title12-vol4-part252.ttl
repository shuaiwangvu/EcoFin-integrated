  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A covered company must conduct an annual stress test by January 5 during each stress test cycle based on data as of September 30 of the preceding calendar year, unless the time or the as of date is extended by the Board in writing." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "In addition to the stress test required under section 252.144 of this subpart, a covered company must conduct a stress test by July 5 during each stress test cycle based on data as of March 31 of that calendar year, unless the time or the as-of date is extended by the Board in writing." ;
      composite:index "3"^^xsd:int ;
    ] ;

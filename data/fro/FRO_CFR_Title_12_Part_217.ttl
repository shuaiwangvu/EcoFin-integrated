  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "12 U.S.C. 248(a), 321-338a, 481-486, 1462a, 1467a, 1818, 1828, 1831n, 1831o, 1831p-l, 1831w, 1835, 1844(b), 1851, 3904, 3906-3909, 4808, 5365, 5368, 5371." ;

  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-4-3 ;
  fro-leg-ref:hasSequenceNumber 27 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-3-27> ;
.

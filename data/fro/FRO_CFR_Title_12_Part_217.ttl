  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Insurance company" ;
  fro-cfr:hasParagraphText "means an insurance company as defined in section 201 of the Dodd-Frank Act (12 U.S.C. 5381)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;

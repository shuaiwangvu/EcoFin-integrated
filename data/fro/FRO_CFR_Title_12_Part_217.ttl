  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "De minimis" ;
  fro-cfr:hasParagraphText "assets related to the operation of the issuing entity can be disregarded for purposes of this criterion." ;
  fro-leg-ref:hasSequenceNumber 0 ;

  fro-leg-ref:hasSequenceNumber 72 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-2-72> ;
.
fro-cfr-t12-p217:r-1-10-5-2-73
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(A) The net sum of all positive and negative fair values of the individual OTC derivative contracts subject to the qualifying master netting agreement; or" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-2 ;
  fro-leg-ref:hasSequenceNumber 73 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-2-73> ;
.
fro-cfr-t12-p217:r-1-10-5-2-74
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(B) Zero; and" ;

  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-240> ;
.
fro-cfr-t12-p217:r-1-6-2-241
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "High volatility commercial real estate (HVCRE) exposure" ;
  fro-cfr:hasParagraphText "means a credit facility that, prior to conversion to permanent financing, finances or has financed the acquisition, development, or construction (ADC) of real property, unless the facility finances:" ;

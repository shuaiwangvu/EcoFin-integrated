  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Bankruptcy remote" ;
  fro-cfr:hasParagraphText "means, with respect to an entity or asset, that the entity or asset would be excluded from an insolvent entity's estate in receivership, insolvency, liquidation, or similar proceeding." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;

  fro-leg-ref:hasSequenceNumber 237 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-237> ;
.
fro-cfr-t12-p217:r-1-6-2-238
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "General obligation" ;
  fro-cfr:hasParagraphText "means a bond or similar obligation that is backed by the full faith and credit of a public sector entity (PSE)." ;

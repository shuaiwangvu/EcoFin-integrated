  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(B) Capital conservation buffer was less than 2.5 percent as of the end of the previous calendar quarter." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-7-2 ;
  fro-leg-ref:hasSequenceNumber 18 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-7-2-18> ;

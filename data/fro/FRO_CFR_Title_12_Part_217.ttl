fro-cfr-t12-p217:r-1-11-10-63
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Non-modeled securitization positions." ;
  fro-cfr:hasParagraphText "(d)" ;
  fro-cfr:hasParagraphText "For securitization positions that are not correlation trading positions and for securitizations that are correlation trading positions not modeled under § 217.209, the total specific risk add-on is the greater of:" ;

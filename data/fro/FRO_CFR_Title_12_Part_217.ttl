  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Effective portion of hedge pairs." ;
  fro-cfr:hasParagraphText "(ii)" ;
  fro-cfr:hasParagraphText "The effective portion of a hedge pair." ;

  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-3-23> ;
.
fro-cfr-t12-p217:r-1-9-4-3-24
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) Pro rata share, which is the par value of the Board-regulated institution's securitization exposure as a percent of the par value of the tranche in" ;
  fro-cfr:hasParagraphText "which the securitization exposure resides;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-4-3 ;
  fro-leg-ref:hasSequenceNumber 24 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-3-24> ;
.

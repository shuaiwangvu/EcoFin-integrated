  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(A) The mean of the on-balance sheet assets calculated as of each day of the reporting quarter; and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-7-1 ;
  fro-leg-ref:hasSequenceNumber 26 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-7-1-26> ;
.
fro-cfr-t12-p217:r-1-7-1-27
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(B) The mean of the off-balance sheet exposures calculated as of the last day of each of the most recent three months, minus the applicable deductions under § 217.22(a), (c), and (d)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-7-1 ;
  fro-leg-ref:hasSequenceNumber 27 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-7-1-27> ;
.

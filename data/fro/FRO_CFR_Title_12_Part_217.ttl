  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(4) Commercial real estate projects in which:" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 247 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-247> ;

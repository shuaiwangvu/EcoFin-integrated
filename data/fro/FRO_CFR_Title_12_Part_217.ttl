  fro-cfr:hasParagraphText "A Board-regulated institution's common equity tier 1 capital ratio is the ratio of the Board-regulated institution's common equity tier 1 capital to standardized total risk-weighted assets;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-7-1 ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-7-1-9> ;

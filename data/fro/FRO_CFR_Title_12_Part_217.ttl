  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-13> ;
.
fro-cfr-t12-p217:r-1-9-2-3-14
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) Guarantees;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-2-3 ;
  fro-leg-ref:hasSequenceNumber 14 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-14> ;
.
fro-cfr-t12-p217:r-1-9-2-3-15
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) Repurchase agreements (the off-balance sheet component of which equals the sum of the current fair values of all positions the Board-regulated institution has sold subject to repurchase);" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-2-3 ;
  fro-leg-ref:hasSequenceNumber 15 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-15> ;
.
fro-cfr-t12-p217:r-1-9-2-3-16
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(iii) Credit-enhancing representations and warranties that are not securitization exposures;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-2-3 ;
  fro-leg-ref:hasSequenceNumber 16 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-16> ;
.
fro-cfr-t12-p217:r-1-9-2-3-17
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(iv) Off-balance sheet securities lending transactions (the off-balance sheet component of which equals the sum of the current fair values of all positions the Board-regulated institution has lent under the transaction);" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-2-3 ;
  fro-leg-ref:hasSequenceNumber 17 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-17> ;

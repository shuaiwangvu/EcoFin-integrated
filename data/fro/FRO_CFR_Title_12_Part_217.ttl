  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Statutory multifamily mortgage" ;
  fro-cfr:hasParagraphText "means a loan secured by a multifamily residential property that meets the requirements under section 618(b)(1) of" ;
  fro-cfr:hasParagraphText "the Resolution Trust Corporation Refinancing, Restructuring, and Improvement Act of 1991, and that meets the following criteria:" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;

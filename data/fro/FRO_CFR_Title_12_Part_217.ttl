.
fro-cfr-t12-p217:r-1-10-5-5-4
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) An eligible guarantee issued by an eligible double default guarantor; or" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-5 ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-5-4> ;
.
fro-cfr-t12-p217:r-1-10-5-5-5
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) An eligible credit derivative that meets the requirements of § 217.134(b)(2) and that is issued by an eligible double default guarantor." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-5 ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-5-5> ;
.
fro-cfr-t12-p217:r-1-10-5-5-6
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(2) The guarantee or credit derivative is:" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-5 ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-5-6> ;
.
fro-cfr-t12-p217:r-1-10-5-5-7
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) An uncollateralized guarantee or uncollateralized credit derivative (for example, a credit default swap) that provides protection with respect to a single reference obligor; or" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-5 ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-5-7> ;
.
fro-cfr-t12-p217:r-1-10-5-5-8
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "th" ;
  fro-cfr:hasParagraphText "(ii) An n" ;
  fro-cfr:hasParagraphText "-to-default credit derivative (subject to the requirements of § 217.142(m)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-5 ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-5-8> ;
.
fro-cfr-t12-p217:r-1-10-5-5-9
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(3) The hedged exposure is a wholesale exposure (other than a sovereign exposure)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-5 ;

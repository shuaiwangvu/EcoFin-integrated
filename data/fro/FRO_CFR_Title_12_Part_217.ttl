  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "total leverage exposure" ;
  fro-cfr:hasParagraphText "(ii) For purposes of this part," ;
  fro-cfr:hasParagraphText "means the sum of the items described in paragraphs (c)(4)(ii)(A) through (H) of this section, as adjusted pursuant to paragraph (c)(4)(ii)(I) for a clearing member Board-regulated institution:" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-7-1 ;

  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "G" ;
  fro-cfr:hasParagraphText "(1) K" ;
  fro-cfr:hasParagraphText "equal to 0.08)." ;
  fro-cfr:hasParagraphText "is expressed as a decimal value between zero and one (that is, an average risk weight of 100 percent represents a value of K" ;

  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Regulatory adjustments to common equity tier 1 capital." ;
  fro-cfr:hasParagraphText "(1) A Board-regulated institution must adjust the sum of common equity tier 1 capital elements pursuant to the requirements set forth in this paragraph (b). Such adjustments to common equity tier 1 capital must be made net of the associated deferred tax effects." ;
  fro-cfr:hasParagraphText "(b)" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-8-3 ;
  fro-leg-ref:hasSequenceNumber 13 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-8-3-13> ;
.

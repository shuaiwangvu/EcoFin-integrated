  fro-leg-ref:hasSequenceNumber 74 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-2-74> ;
.
fro-cfr-t12-p217:r-1-10-5-2-75
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Adjusted sum of the PFE." ;
  fro-cfr:hasParagraphEnumText "net" ;
  fro-cfr:hasParagraphText "(ii)" ;
  fro-cfr:hasParagraphText ", is calculated as" ;
  fro-cfr:hasParagraphText "The adjusted sum of the PFE, A" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-2 ;
  fro-leg-ref:hasSequenceNumber 75 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-10-5-2-75> ;
.
fro-cfr-t12-p217:r-1-10-5-2-78
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "gross" ;
  fro-cfr:hasParagraphText "(A) A" ;
  fro-cfr:hasParagraphText "= the gross PFE (that is, the sum of the PFE amounts (as determined under paragraph (c)(5)(ii) of this section) for each individual derivative contract subject to the qualifying master netting agreement); and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-2 ;
  fro-leg-ref:hasSequenceNumber 78 ;

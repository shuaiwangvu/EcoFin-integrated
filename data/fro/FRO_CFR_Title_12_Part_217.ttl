.
fro-cfr-t12-p217:r-1-11-2-11
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) A resecuritization position;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-11-2 ;
  fro-leg-ref:hasSequenceNumber 11 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-11-2-11> ;
.
fro-cfr-t12-p217:r-1-11-2-12
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) A derivative of a securitization position that does not provide a pro rata share in the proceeds of a securitization tranche; or" ;

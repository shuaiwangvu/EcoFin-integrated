  fro-cfr:hasParagraphText "(c)" ;
  fro-cfr:hasParagraphText "A Board-regulated institution must determine the EAD for an OTC derivative contract that is not subject to a qualifying master netting agreement using the current exposure methodology in paragraph (c)(5) of this section or using the internal models methodology described in paragraph (d) of this section." ;
  fro-cfr:hasParagraphText "—(1)" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-10-5-2 ;
  fro-leg-ref:hasSequenceNumber 60 ;

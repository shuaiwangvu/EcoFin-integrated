  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-4-4 ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-4-9> ;
.
fro-cfr-t12-p217:r-1-9-4-5
  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionNumber "§ 217.45" ;
  fro-cfr:hasSectionSubject "Recognition of credit risk mitigants for securitization exposures." ;
  fro-leg-ref:divides fro-cfr:CFR_Title-12_Part-217 ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-5> ;
.
fro-cfr-t12-p217:r-1-9-4-5-2
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "General." ;
  fro-cfr:hasParagraphText "(1) An originating Board-regulated institution that has obtained a credit risk mitigant to hedge its exposure to a synthetic or traditional securitization that satisfies the operational criteria provided in § 217.41 may recognize the credit risk mitigant under §§ 217.36 or 217.37, but only as provided in this section." ;
  fro-cfr:hasParagraphText "(a)" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-4-5 ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-5-2> ;
.
fro-cfr-t12-p217:r-1-9-4-5-3
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(2) An investing Board-regulated institution that has obtained a credit risk mitigant to hedge a securitization exposure may recognize the credit risk mitigant under §§ 217.36 or 217.37, but only as provided in this section." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-4-5 ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-5-3> ;
.
fro-cfr-t12-p217:r-1-9-4-5-4
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Mismatches." ;
  fro-cfr:hasParagraphText "(b)" ;
  fro-cfr:hasParagraphText "A Board-regulated institution must make any applicable adjustment to the protection amount of an eligible guarantee or credit derivative as required in § 217.36(d), (e), and (f) for any hedged securitization exposure. In the context of a synthetic" ;
  fro-cfr:hasParagraphText "securitization, when an eligible guarantee or eligible credit derivative covers multiple hedged exposures that have different residual maturities, the Board-regulated institution must use the longest residual maturity of any of the hedged exposures as the residual maturity of all hedged exposures." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-4-5 ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-4-5-4> ;
.

  fro-leg-ref:hasSequenceNumber 407 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-407> ;
.
fro-cfr-t12-p217:r-1-6-2-408
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Tier 1 minority interest" ;
  fro-cfr:hasParagraphText "means the tier 1 capital of a consolidated subsidiary of a Board-regulated institution that is not owned by the Board-regulated institution." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 408 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-408> ;
.
fro-cfr-t12-p217:r-1-6-2-409
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Tier 2 capital" ;
  fro-cfr:hasParagraphText "is defined in § 217.20(d)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 409 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-409> ;
.
fro-cfr-t12-p217:r-1-6-2-41
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Clearing member" ;
  fro-cfr:hasParagraphText "means a member of, or direct participant in, a CCP that is entitled to enter into transactions with the CCP." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 41 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-41> ;
.
fro-cfr-t12-p217:r-1-6-2-410
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Total capital" ;
  fro-cfr:hasParagraphText "means the sum of tier 1 capital and tier 2 capital." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 410 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-410> ;
.
fro-cfr-t12-p217:r-1-6-2-411
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Total capital minority interest" ;
  fro-cfr:hasParagraphText "means the total capital of a consolidated subsidiary of a Board-regulated institution that is not owned by the Board-regulated institution." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 411 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-411> ;
.
fro-cfr-t12-p217:r-1-6-2-412
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Total leverage exposure" ;
  fro-cfr:hasParagraphText "is defined in § 217.10(c)(4)(ii)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 412 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-412> ;
.
fro-cfr-t12-p217:r-1-6-2-413
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Traditional securitization" ;
  fro-cfr:hasParagraphText "means a transaction in which:" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 413 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-413> ;
.
fro-cfr-t12-p217:r-1-6-2-414
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(1) All or a portion of the credit risk of one or more underlying exposures is" ;
  fro-cfr:hasParagraphText "transferred to one or more third parties other than through the use of credit derivatives or guarantees;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 414 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-414> ;
.
fro-cfr-t12-p217:r-1-6-2-415
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(2) The credit risk associated with the underlying exposures has been separated into at least two tranches reflecting different levels of seniority;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 415 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-415> ;
.
fro-cfr-t12-p217:r-1-6-2-416
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(3) Performance of the securitization exposures depends upon the performance of the underlying exposures;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 416 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-416> ;
.
fro-cfr-t12-p217:r-1-6-2-417
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(4) All or substantially all of the underlying exposures are financial exposures (such as loans, commitments, credit derivatives, guarantees, receivables, asset-backed securities, mortgage-backed securities, other debt securities, or equity securities);" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 417 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-417> ;
.
fro-cfr-t12-p217:r-1-6-2-418
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(5) The underlying exposures are not owned by an operating company;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 418 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-418> ;
.
fro-cfr-t12-p217:r-1-6-2-419
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(6) The underlying exposures are not owned by a small business investment company defined in section 302 of the Small Business Investment Act;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 419 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-419> ;
.
fro-cfr-t12-p217:r-1-6-2-42
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Clearing member client" ;
  fro-cfr:hasParagraphText "means a party to a cleared transaction associated with a CCP in which a clearing member acts either as a financial intermediary with respect to the party or guarantees the performance of the party to the CCP." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 42 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-42> ;
.
fro-cfr-t12-p217:r-1-6-2-420
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(7) The underlying exposures are not owned by a firm an investment in which qualifies as a community development investment under section 24(Eleventh) of the National Bank Act;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 420 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-420> ;
.
fro-cfr-t12-p217:r-1-6-2-421
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(8) The Board may determine that a transaction in which the underlying exposures are owned by an investment firm that exercises substantially unfettered control over the size and composition of its assets, liabilities, and off-balance sheet exposures is not a traditional securitization based on the transaction's leverage, risk profile, or economic substance;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 421 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-421> ;
.
fro-cfr-t12-p217:r-1-6-2-422
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(9) The Board may deem a transaction that meets the definition of a traditional securitization, notwithstanding paragraph (5), (6), or (7) of this definition, to be a traditional securitization based on the transaction's leverage, risk profile, or economic substance; and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 422 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-422> ;
.
fro-cfr-t12-p217:r-1-6-2-423
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(10) The transaction is not:" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 423 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-423> ;
.
fro-cfr-t12-p217:r-1-6-2-424
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) An investment fund;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 424 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-424> ;
.
fro-cfr-t12-p217:r-1-6-2-425
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) A collective investment fund (as defined in 12 CFR 208.34);" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 425 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-425> ;
.
fro-cfr-t12-p217:r-1-6-2-426
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(iii) An employee benefit plan (as defined in paragraphs (3) and (32) of section 3 of ERISA), a “governmental plan” (as defined in 29 U.S.C. 1002(32)) that complies with the tax deferral qualification requirements provided in the Internal Revenue Code, or any similar employee benefit plan established under the laws of a foreign jurisdiction;" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 426 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-426> ;
.
fro-cfr-t12-p217:r-1-6-2-427
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(iv) A synthetic exposure to the capital of a financial institution to the extent deducted from capital under § 217.22; or" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 427 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-427> ;
.
fro-cfr-t12-p217:r-1-6-2-428
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(v) Registered with the SEC under the Investment Company Act of 1940 (15 U.S.C. 80a-1) or foreign equivalents thereof." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 428 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-428> ;
.
fro-cfr-t12-p217:r-1-6-2-429
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Tranche" ;
  fro-cfr:hasParagraphText "means all securitization exposures associated with a securitization that have the same seniority level." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 429 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-429> ;
.
fro-cfr-t12-p217:r-1-6-2-43
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Collateral agreement" ;
  fro-cfr:hasParagraphText "confers upon the Board-regulated institution a perfected, first-priority security interest (notwithstanding the prior security interest of any custodial agent), or the legal equivalent thereof, in the collateral posted by the counterparty under the agreement. This security interest must provide the Board-regulated institution with a right to close-out the financial positions and liquidate the collateral upon an event of default of, or failure to perform by, the counterparty under the collateral agreement. A contract would not satisfy this requirement if the Board-regulated institution's exercise of rights under the agreement may be stayed or avoided under applicable law in the relevant jurisdictions, other than:" ;
  fro-cfr:hasParagraphText "means a legal contract that specifies the time when, and circumstances under which, a counterparty is required to pledge collateral to a Board-regulated institution for a single financial contract or for all financial contracts in a netting set and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 43 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-43> ;
.
fro-cfr-t12-p217:r-1-6-2-430
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Two-way market" ;
  fro-cfr:hasParagraphText "means a market where there are independent bona fide offers to buy and sell so that a price reasonably related to the last sales price or current bona fide competitive bid and offer quotations can be determined within one day and settled at that price within a relatively short time frame conforming to trade custom." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 430 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-430> ;
.
fro-cfr-t12-p217:r-1-6-2-431
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Unconditionally cancelable" ;
  fro-cfr:hasParagraphText "means with respect to a commitment, that a Board-regulated institution may, at any time, with or without cause, refuse to extend credit under the commitment (to the extent permitted under applicable law)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 431 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-431> ;
.
fro-cfr-t12-p217:r-1-6-2-432
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Underlying exposures" ;
  fro-cfr:hasParagraphText "means one or more exposures that have been securitized in a securitization transaction." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 432 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-432> ;
.
fro-cfr-t12-p217:r-1-6-2-433
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Unregulated financial institution" ;
  fro-cfr:hasParagraphText "means, for purposes of § 217.131, a financial institution that is not a regulated financial institution, including any financial institution that would meet the definition of “financial institution” under this section but for the ownership interest thresholds set forth in paragraph (4)(i) of that definition." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 433 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-433> ;
.
fro-cfr-t12-p217:r-1-6-2-434
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "U.S. Government agency" ;
  fro-cfr:hasParagraphText "means an instrumentality of the U.S. Government whose obligations are fully and explicitly guaranteed as to the timely payment of principal and interest by the full faith and credit of the U.S. Government." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 434 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-434> ;
.
fro-cfr-t12-p217:r-1-6-2-435
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Value-at-Risk (VaR)" ;
  fro-cfr:hasParagraphText "means the estimate of the maximum amount that the value of one or more exposures could decline due to market price or rate movements during a fixed holding period within a stated confidence interval." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 435 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-435> ;
.
fro-cfr-t12-p217:r-1-6-2-436
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Wrong-way risk" ;
  fro-cfr:hasParagraphText "means the risk that arises when an exposure to a particular counterparty is positively correlated with the probability of default of such counterparty itself." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 436 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-436> ;
.
fro-cfr-t12-p217:r-1-6-2-438-1
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "At 79 FR 24540, May 1, 2014, § 217.2 was amended by adding a definition of “covered BHC” in alphabetical order, effective Jan. 1, 2018. For the convenience of the user, the added text is set forth as follows:" ;
  fro-leg-ref:hasSequenceNumber 1 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-438-1> ;
.
fro-cfr-t12-p217:r-1-6-2-438-2-0
  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionNumber "§ 217.2" ;
  fro-cfr:hasSectionSubject "Definitions." ;
  fro-leg-ref:divides fro-cfr:CFR_Title-12_Part-217 ;
  fro-leg-ref:hasSequenceNumber 0 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-438-2-0> ;
.
fro-cfr-t12-p217:r-1-6-2-438-2-0-3
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Covered BHC" ;
  fro-cfr:hasParagraphText "means a U.S. top-tier bank holding company that has more than $700 billion in total assets as reported on the company's most recent Consolidated Financial Statements for Holding Companies (FR Y-9C) or more than $10 trillion in assets under custody as reported on the company's most recent Banking Organization Systemic Risk Report (FR Y-15)." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2-438-2-0 ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-438-2-0-3> ;
.
fro-cfr-t12-p217:r-1-6-2-44
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(1) In receivership, conservatorship, or resolution under the Federal Deposit Insurance Act, Title II of the Dodd-Frank Act, or under any similar insolvency law applicable to GSEs, or laws of foreign jurisdictions that are substantially similar" ;
  fro-cfr:hasParagraphText "to the U.S. laws referenced in this paragraph (1) in order to facilitate the orderly resolution of the defaulting counterparty; or" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 44 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-44> ;
.
fro-cfr-t12-p217:r-1-6-2-45-0
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "The Board expects to evaluate jointly with the OCC and Federal Deposit Insurance Corporation whether foreign special resolution regimes meet the requirements of this paragraph." ;
  fro-leg-ref:hasSequenceNumber 0 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-45-0> ;
.
fro-cfr-t12-p217:r-1-6-2-46
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(2) Where the agreement is subject by its terms to any of the laws referenced in paragraph (1) of this definition." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 46 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-46> ;
.
fro-cfr-t12-p217:r-1-6-2-47
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Commitment" ;
  fro-cfr:hasParagraphText "means any legally binding arrangement that obligates a Board-regulated institution to extend credit or to purchase assets." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 47 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-47> ;
.
fro-cfr-t12-p217:r-1-6-2-48
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Commodity derivative contract" ;
  fro-cfr:hasParagraphText "means a commodity-linked swap, purchased commodity-linked option, forward commodity-linked contract, or any other instrument linked to commodities that gives rise to similar counterparty credit risks." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 48 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-48> ;
.
fro-cfr-t12-p217:r-1-6-2-49
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Commodity Exchange Act" ;

  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(ii) Whose creditworthiness is not positively correlated with the credit risk of the exposures for which it has provided guarantees; and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 155 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-155> ;

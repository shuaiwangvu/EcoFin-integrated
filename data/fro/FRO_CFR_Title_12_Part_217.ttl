  fro-leg-ref:hasSequenceNumber 153 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-153> ;
.
fro-cfr-t12-p217:r-1-6-2-154
  rdf:type fro-cfr:CFR_Parapraph ;

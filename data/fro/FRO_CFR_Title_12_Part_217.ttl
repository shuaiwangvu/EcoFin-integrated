fro-cfr-t12-p217:r-1-9-2-3-18
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(v) Off-balance sheet securities borrowing transactions (the off-balance sheet component of which equals the sum of the current fair values of all non-cash positions the Board-regulated institution has posted as collateral under the transaction);" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-2-3 ;
  fro-leg-ref:hasSequenceNumber 18 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-18> ;
.
fro-cfr-t12-p217:r-1-9-2-3-19
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(vi) Financial standby letters of credit; and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-9-2-3 ;
  fro-leg-ref:hasSequenceNumber 19 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-9-2-3-19> ;

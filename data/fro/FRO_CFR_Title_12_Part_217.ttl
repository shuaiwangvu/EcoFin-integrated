  fro-cfr:hasParagraphEnumText "Interest rate derivative contract" ;
  fro-cfr:hasParagraphText "means a single-currency interest rate swap, basis swap, forward rate agreement, purchased interest rate option, when-issued securities, or any other instrument linked to interest rates that gives rise to similar counterparty credit risks." ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-6-2 ;
  fro-leg-ref:hasSequenceNumber 256 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol2-part217.ttl#r-1-6-2-256> ;
.

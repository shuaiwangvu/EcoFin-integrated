  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(A) Eligible retained income is negative; and" ;
  fro-leg-ref:divides fro-cfr-t12-p217:r-1-7-2-39-2-0 ;

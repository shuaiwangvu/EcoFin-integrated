  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol3-part225.ttl#r-1-9-5-25> ;
.
fro-cfr-t12-p225:r-1-9-5-26
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(A) Approve the notice; or" ;

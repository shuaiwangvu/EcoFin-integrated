fro-cfr-t12-p225:r-1-18-3-400
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Implicit recourse limitation." ;
  fro-cfr:hasParagraphText "3." ;

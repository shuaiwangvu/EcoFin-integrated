  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Popular, Inc.," ;
  fro-cfr:hasParagraphText "(vi) In connection with offering banking services, providing notary public services, selling postage stamps and postage-paid envelopes, providing vehicle registration services, and selling public transportation tickets and tokens (" ;
  fro-cfr:hasParagraphText "84 Federal Reserve Bulletin 481 (1998)); and" ;

  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionCitation "[Reg. Y, 62 FR 9333, Feb. 28, 1997, as amended by Reg. Y, 62 FR 60640, Nov. 12, 1997]" ;
  fro-cfr:hasSectionNumber "§ 225.25" ;

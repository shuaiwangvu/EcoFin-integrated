  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "The First National Company," ;
  fro-cfr:hasParagraphText "(vii) Real estate title abstracting (" ;
  fro-cfr:hasParagraphText "81 Federal Reserve Bulletin 805 (1995))." ;
  fro-leg-ref:divides fro-cfr-t12-p225:r-1-15-7 ;

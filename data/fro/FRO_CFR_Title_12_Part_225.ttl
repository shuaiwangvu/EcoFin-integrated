.
fro-cfr-t12-p225:r-1-15-7-46
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(i) Lending, exchanging, transferring, investing for others, or safeguarding financial assets other than money or securities;" ;
  fro-leg-ref:divides fro-cfr-t12-p225:r-1-15-7 ;
  fro-leg-ref:hasSequenceNumber 46 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol3-part225.ttl#r-1-15-7-46> ;
.
fro-cfr-t12-p225:r-1-15-7-47
  rdf:type fro-cfr:CFR_Parapraph ;

fro-cfr-t12-p225:r-1-15-14-14
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(1) The Board may impose any limitations or conditions on the conduct or the U.S. activities of the foreign bank or company or any of its affiliates as the Board finds to be appropriate and consistent with the purposes of the Bank Holding Company Act; and" ;
  fro-leg-ref:divides fro-cfr-t12-p225:r-1-15-14 ;
  fro-leg-ref:hasSequenceNumber 14 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol3-part225.ttl#r-1-15-14-14> ;

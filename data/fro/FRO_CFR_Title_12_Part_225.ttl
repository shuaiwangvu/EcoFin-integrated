  fro-leg-ref:hasSequenceNumber 41 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol3-part225.ttl#r-1-15-7-41> ;
.
fro-cfr-t12-p225:r-1-15-7-42
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphText "(D) A finder may not engage in any activity that would require the company to register or obtain a license as a real estate agent or broker under applicable law." ;

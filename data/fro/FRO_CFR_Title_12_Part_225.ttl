  rdf:type fro-cfr:CFR_Section ;
  fro-cfr:hasSectionCitation "[Reg. Y, 66 FR 415, Jan. 3, 2001, as amended at 66 FR 19081, Apr. 13, 2001]" ;
  fro-cfr:hasSectionNumber "§ 225.86" ;

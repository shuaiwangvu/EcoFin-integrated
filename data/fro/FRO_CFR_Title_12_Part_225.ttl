  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol3-part225.ttl#r-1-15-7-48> ;
.
fro-cfr-t12-p225:r-1-15-7-49
  rdf:type fro-cfr:CFR_Parapraph ;
  fro-cfr:hasParagraphEnumText "Is a specific request required?" ;
  fro-cfr:hasParagraphEnumText "Review of specific activities" ;
  fro-cfr:hasParagraphText "(2)" ;
  fro-cfr:hasParagraphText "A financial holding company that wishes to engage on the basis of paragraph (e)(1) of this section in an activity that is not otherwise permissible for a financial holding company must obtain a determination from the Board that the activity is permitted under paragraph (e)(1)." ;
  fro-cfr:hasParagraphText "—(i)" ;
  fro-leg-ref:divides fro-cfr-t12-p225:r-1-15-7 ;
  fro-leg-ref:hasSequenceNumber 49 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/cfr/CFR-2015-title12-vol3-part225.ttl#r-1-15-7-49> ;
.

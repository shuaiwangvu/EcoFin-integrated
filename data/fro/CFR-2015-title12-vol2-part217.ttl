  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A Board-regulated institution must obtain the prior written approval of the Board under § 217.135 to use the double default treatment." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "23"^^xsd:int ;
.
cfr-12-217:r-1-10-4-2-23-1
  rdf:type cfr-fdsys-s:E ;

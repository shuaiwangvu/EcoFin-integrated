  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) 10 percent or more of quarter-end total assets as reported on the most recent quarterly [Call Report or FR Y-9C]; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.

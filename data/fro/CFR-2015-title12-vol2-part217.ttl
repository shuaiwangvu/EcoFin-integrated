  composite:index "1"^^xsd:int ;
.
cfr-12-217:r-1-9-2-4-3
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-9-2-4-3-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The current credit exposure for a single OTC derivative contract is the greater of the mark-to-fair value of the OTC derivative contract or zero." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.

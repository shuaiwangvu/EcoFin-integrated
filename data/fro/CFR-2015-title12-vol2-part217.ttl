  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iii) Es equals the absolute value of the net position in a given instrument or in gold (where the net position in" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "the instrument or gold equals the sum of the current fair values of the instrument or gold the Board-regulated institution has lent, sold subject to repurchase, or posted as collateral to the counterparty minus the sum of the current fair values of that same instrument or gold the Board-regulated institution has borrowed, purchased subject to resale, or taken as collateral from the counterparty);" ;

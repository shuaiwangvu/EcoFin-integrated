.
cfr-12-217:r-1-11-6-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-11-6-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "At least weekly, a Board-regulated institution must use the same internal model(s) used to calculate its VaR-based measure to calculate a stressed VaR-based measure." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-12-217:r-1-11-6-2-1

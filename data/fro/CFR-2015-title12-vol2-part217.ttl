  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A Board-regulated institution must calculate its total wholesale and retail risk-weighted asset amount in four distinct phases:" ;

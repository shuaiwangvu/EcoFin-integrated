      sxml:text "(iv) Is a subsidiary of a bank holding company or savings and loan holding company that uses the advanced approaches pursuant to 12 CFR part 217 to calculate its total risk-weighted assets; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "18"^^xsd:int ;
.
cfr-12-217:r-1-10-1-19
  rdf:type cfr-fdsys-s:P ;

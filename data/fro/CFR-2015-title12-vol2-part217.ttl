  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iii) The internal models methodology described in paragraph (d) of this section." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "12"^^xsd:int ;
.

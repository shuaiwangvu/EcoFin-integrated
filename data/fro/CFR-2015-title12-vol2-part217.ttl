      rdf:type sxml:TextNode ;
      sxml:text "PSE debt positions." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-217:r-1-11-10-37
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(B) A Board-regulated institution may assign a lower specific risk-weighting factor than would otherwise apply under Tables 3 and 4 of this section to a debt position that is an exposure to a foreign PSE if:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "37"^^xsd:int ;
.
cfr-12-217:r-1-11-10-38
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-11-10-38-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ") The PSE's home country allows banks under its jurisdiction to assign a lower specific risk-weighting factor to such position; and" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "38"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3) The Board-regulated institution obtains a well-reasoned opinion from legal counsel that confirms the enforceability of the credit risk mitigant in all relevant jurisdictions; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.

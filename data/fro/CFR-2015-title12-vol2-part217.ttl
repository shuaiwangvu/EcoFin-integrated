  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-11-1-3-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) This subpart applies to any Board-regulated institution with aggregate trading assets and trading liabilities (as reported in the Board-regulated institution's most recent quarterly Call Report, for a state member bank, or FR Y-9C, for a bank holding company or savings and loan holding company, as applicable, any savings and loan holding company that does not file the FR Y-9C should follow the instructions to the FR Y-9C) equal to:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.

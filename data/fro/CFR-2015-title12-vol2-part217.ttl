      sxml:text "Double default treatment." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-217:r-1-10-4-2-24
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-10-4-2-24-1 ;
  composite:child [

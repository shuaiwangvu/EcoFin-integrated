  composite:child cfr-12-217:r-1-9-4-2-15-1 ;
  composite:child cfr-12-217:r-1-9-4-2-15-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "If a Board-regulated institution provides support to a securitization in excess of the Board-" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "regulated institution's contractual obligation to provide credit support to the securitization (implicit support):" ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;
.

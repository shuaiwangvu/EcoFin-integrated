  rdf:type cfr-fdsys-s:E ;
  cfr-12-217:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "et seq." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-217:r-1-8-1-33
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(A) The Board-regulated institution must receive prior approval from the Board to exercise a call option on the instrument." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "33"^^xsd:int ;
.
cfr-12-217:r-1-8-1-34
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(B) The Board-regulated institution does not create at issuance of the instrument, through any action or communication, an expectation that the call option will be exercised." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "34"^^xsd:int ;
.
cfr-12-217:r-1-8-1-35
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-8-1-35-1 ;
  composite:child cfr-12-217:r-1-8-1-35-2 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(C) Prior to exercising the call option, or immediately thereafter, the Board-regulated institution must either: Replace the instrument to be called with an equal amount of instruments that meet the criteria under paragraph (b) of this section or this paragraph (c);" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "or demonstrate to the satisfaction of the Board that following redemption, the Board-regulated institution will continue to hold capital commensurate with its risk." ;
      composite:index "4"^^xsd:int ;
    ] ;
  composite:index "35"^^xsd:int ;
.

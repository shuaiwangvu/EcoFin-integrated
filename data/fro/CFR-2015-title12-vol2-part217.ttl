cfr-12-217:r-1-10-9-3-13-28
  rdf:type cfr-fdsys-s:TNOTE ;
  composite:child cfr-12-217:r-1-10-9-3-13-28-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "This disclosure item is not intended to be prescriptive about the period used for this assessment. Upon implementation, it is expected that a Board-regulated institution would provide these disclosures for as long a set of data as possible—for example, if a Board-regulated institution has 10 years of data, it might choose to disclose the average default rates for each PD grade over that 10-year period. Annual amounts need not be disclosed." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "28"^^xsd:int ;
.

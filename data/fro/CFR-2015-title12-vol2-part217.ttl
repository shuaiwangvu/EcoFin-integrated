  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) A Board-regulated institution must apply a risk weight to the unsecured portion of the exposure based on the risk weight applicable to the exposure under this subpart." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "12"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:GPH ;
  cfr-12-217:DEEP-GPH "119" ;
  cfr-12-217:SPAN-GPH "2" ;
  composite:child cfr-12-217:r-1-10-5-2-113-0 ;
  composite:index "113"^^xsd:int ;
.
cfr-12-217:r-1-10-5-2-113-0
  rdf:type cfr-fdsys-s:GID ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "ER11OC13.034" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.
cfr-12-217:r-1-10-5-2-114
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) If the remaining maturity of the exposure or the longest-dated contract in the netting set is one year or less, the Board-regulated institution must set M for the exposure or netting set equal to one year, except as provided in § 217.131(d)(7)." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "114"^^xsd:int ;

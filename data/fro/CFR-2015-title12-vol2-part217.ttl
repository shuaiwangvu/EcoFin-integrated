cfr-12-217:r-1-9-5-3-8
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-9-5-3-8-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(c)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Under the simple modified look-through approach, the risk-weighted asset amount for a Board-regulated institution's equity exposure to an investment fund equals the adjusted carrying value of the equity exposure multiplied by the highest risk weight that applies to any exposure the fund is permitted to hold under the prospectus, partnership agreement, or similar agreement that defines the fund's permissible investments (excluding derivative contracts that are used for hedging rather than speculative purposes and that do not constitute a material portion of the fund's exposures)." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "8"^^xsd:int ;
.

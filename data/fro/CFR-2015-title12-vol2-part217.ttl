cfr-12-217:r-1-10-9-3-13-27
  rdf:type cfr-fdsys-s:TNOTE ;
  composite:child cfr-12-217:r-1-10-9-3-13-27-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "These disclosures are a way of further informing the reader about the reliability of the information provided in the “quantitative disclosures: Risk assessment” over the long run. The disclosures are requirements from year-end 2010; in the meantime, early adoption is encouraged. The phased implementation is to allow a Board-regulated institution sufficient time to build up a longer run of data that will make these disclosures meaningful." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "27"^^xsd:int ;
.

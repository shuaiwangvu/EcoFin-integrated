  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) Instruments (plus any related surplus) that meet the following criteria:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "27"^^xsd:int ;
.
cfr-12-217:r-1-8-1-28
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) The instrument is issued and paid-in;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "28"^^xsd:int ;
.

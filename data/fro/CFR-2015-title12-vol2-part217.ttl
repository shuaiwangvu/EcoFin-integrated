  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) Set the risk-weighted asset amount of the Board-regulated institution's exposure to the fund equal to the product of:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.
cfr-12-217:r-1-10-7-4-7
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(i) The aggregate risk-weighted asset amounts of the exposures held by the fund as if they were held directly by the Board-regulated institution; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.
cfr-12-217:r-1-10-7-4-8
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The Board-regulated institution's proportional ownership share of the fund; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "8"^^xsd:int ;
.
cfr-12-217:r-1-10-7-4-9
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) Include the Board-regulated institution's proportional ownership share of each exposure held by the fund in the Board-regulated institution's IMA." ;

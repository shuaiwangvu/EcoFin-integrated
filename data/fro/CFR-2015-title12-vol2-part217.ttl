  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(B) For collateralized derivative contracts and netting sets thereof, ΣE equals the exposure amount of the OTC derivative contract (or netting set) calculated under § 217.34 (a)(1) or (2)." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "22"^^xsd:int ;

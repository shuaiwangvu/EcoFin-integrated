      rdf:type sxml:TextNode ;
      sxml:text "(ii) $1 billion or more." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.
cfr-12-217:r-1-11-1-6
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) The Board may apply this subpart to any Board-regulated institution if the Board deems it necessary or appropriate because of the level of market risk of the Board-regulated institution or to ensure safe and sound banking practices." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.
cfr-12-217:r-1-11-1-7
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-11-1-7-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3) The Board may exclude a Board-regulated institution that meets the criteria of paragraph (b)(1) of this section from application of this subpart if the Board determines that the exclusion is appropriate based on the level of" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "market risk of the Board-regulated institution and is consistent with safe and sound banking practices." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.
cfr-12-217:r-1-11-1-7-1
  rdf:type cfr-fdsys-s:PRTPAGE ;
  cfr-12-217:P-PRTPAGE "580" ;

cfr-12-217:r-1-8-3-30
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-8-3-30-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(3)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A bank holding company or savings and loan holding company must deduct an amount equal to the regulatory capital requirement for insurance underwriting risks established by the regulator of any insurance underwriting activities of the company. The bank holding company or savings and loan holding company must take the deduction 50 percent from tier 1 capital and 50 percent from tier 2 capital. If the amount deductible from tier 2 capital exceeds the Board-regulated institution's tier 2 capital, the Board-regulated institution must deduct the excess from tier 1 capital." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "30"^^xsd:int ;
.

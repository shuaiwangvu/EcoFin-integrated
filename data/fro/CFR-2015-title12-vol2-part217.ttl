      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A Board-regulated institution must obtain the prior written approval of the Board under § 217.153 to use the internal models approach for equity exposures." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "24"^^xsd:int ;
.
cfr-12-217:r-1-10-4-2-24-1
  rdf:type cfr-fdsys-s:E ;

  composite:child cfr-12-217:r-1-9-4-5-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) An originating Board-regulated institution that has obtained a credit risk mitigant to hedge its exposure to a synthetic or traditional securitization that satisfies the operational criteria provided in § 217.41 may recognize the credit risk mitigant under §§ 217.36 or 217.37, but only as provided in this section." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;

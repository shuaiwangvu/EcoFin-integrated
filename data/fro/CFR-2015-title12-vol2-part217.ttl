cfr-12-217:r-1-10-9-3-13-25
  rdf:type cfr-fdsys-s:TNOTE ;
  composite:child cfr-12-217:r-1-10-9-3-13-25-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The PD, LGD and EAD disclosures in Table 6 (c) to § 217.173 should reflect the effects of collateral, qualifying master netting agreements, eligible guarantees and eligible credit derivatives as defined under this part. Disclosure of each PD grade should include the exposure-weighted average PD for each grade. Where a Board-regulated institution aggregates PD grades for the purposes of disclosure, this should be a representative breakdown of the distribution of PD grades used for regulatory capital purposes." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "25"^^xsd:int ;
.

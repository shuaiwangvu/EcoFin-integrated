  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The instrument is subordinated to depositors, general creditors, and subordinated debt holders of the Board-regulated institution in a receivership, insolvency, liquidation, or similar proceeding;" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "29"^^xsd:int ;
.

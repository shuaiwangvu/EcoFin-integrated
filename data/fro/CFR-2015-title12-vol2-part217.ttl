  composite:child cfr-12-217:r-1-10-5-4-13-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) The reference exposure and the hedged exposure are exposures to the same legal entity, and legally enforceable cross-default or cross-acceleration clauses are in place to assure payments" ;

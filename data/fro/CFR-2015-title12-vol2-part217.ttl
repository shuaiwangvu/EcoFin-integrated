  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(4) If multiple eligible guarantees or eligible credit derivatives cover a single exposure described in paragraph (a)(1) of this section, a Board-regulated institution may treat the hedged exposure as multiple separate exposures each covered by a single eligible guarantee or eligible credit derivative and may calculate a separate risk-based capital requirement for each separate exposure as described in paragraph (a)(3) of this section." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "7"^^xsd:int ;
.
cfr-12-217:r-1-10-5-4-8
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(5) If a single eligible guarantee or eligible credit derivative covers multiple hedged wholesale exposures described in paragraph (a)(1) of this section, a Board-regulated institution must treat each hedged exposure as covered by a separate eligible guarantee or eligible credit derivative and must calculate a separate risk-based capital requirement for each exposure as described in paragraph (a)(3) of this section." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "8"^^xsd:int ;
.
cfr-12-217:r-1-10-5-4-9

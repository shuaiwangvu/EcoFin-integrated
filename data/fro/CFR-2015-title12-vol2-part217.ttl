  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(D) Is a subsidiary of a bank holding company or savings and loan holding company that uses 12 CFR part 217, subpart E, to calculate its risk-based capital requirements; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "16"^^xsd:int ;
.

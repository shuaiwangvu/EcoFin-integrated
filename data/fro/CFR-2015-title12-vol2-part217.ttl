.
cfr-12-217:r-1-9-4-2-14
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-9-4-2-14-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(d)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "If a Board-regulated institution has multiple securitization exposures that provide duplicative coverage to the underlying exposures of a securitization (such as when a Board-regulated institution provides a program-wide credit enhancement and multiple pool-specific liquidity facilities to an ABCP program), the Board-regulated institution is not required to hold duplicative risk-based capital against the overlapping position. Instead, the Board-regulated institution may apply to the overlapping position the applicable risk-based capital treatment that results in the highest risk-based capital requirement." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "14"^^xsd:int ;
.
cfr-12-217:r-1-9-4-2-14-1
  rdf:type cfr-fdsys-s:E ;
  cfr-12-217:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Overlapping exposures." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:E ;
  cfr-12-217:T-E "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Alternative modified look-through approach." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

      sxml:text "(4)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Significant investments in the capital of unconsolidated financial institutions in the form of common stock that are not deducted from capital pursuant to § 217.22(b)(4) are assigned a 250 percent risk weight." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "13"^^xsd:int ;
.

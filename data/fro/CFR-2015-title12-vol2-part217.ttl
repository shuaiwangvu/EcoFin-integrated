  rdf:type cfr-fdsys-s:SUBJECT ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Purpose, applicability, and principle of conservatism." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-12-217:r-1-10-1-10
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-10-1-10-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ") Has consolidated total on-balance sheet foreign exposure at the most recent year-end equal to $10 billion (excluding exposures held by an insurance underwriting subsidiary). Total on-balance sheet foreign exposure equals total cross-border claims less claims with head office or guarantor located in another country plus redistributed guaranteed amounts to the country of head office or guarantor plus local country claims on local residents plus revaluation gains on foreign exchange and derivative products, calculated in accordance with the Federal Financial Institutions Examination Council (FFIEC) 009 Country Exposure Report); or" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "10"^^xsd:int ;
.

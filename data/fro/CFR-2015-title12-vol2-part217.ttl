cfr-12-217:r-1-8-3-13
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-12-217:r-1-8-3-13-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(1) A Board-regulated institution must adjust the sum of common equity tier 1 capital elements pursuant to the requirements set forth in this paragraph (b). Such adjustments to common equity tier 1 capital must be made net of the associated deferred tax effects." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "13"^^xsd:int ;
.

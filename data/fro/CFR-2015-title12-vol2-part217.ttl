      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "A Board-regulated institution must determine the exposure amount for an eligible margin loan, repo-style transaction, collateralized derivative contract, or a single-product netting set of such transactions by setting the exposure amount equal to max {0, [(ΣE − ΣC) + Σ(Es × Hs) + Σ(Efx × Hfx)]}, where:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.

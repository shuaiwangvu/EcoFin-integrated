cfr-12-217:r-1-8-3-43
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(C) A tier 2 capital instrument if it is not included in GAAP equity but considered regulatory capital by the primary supervisor of the financial institution." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "43"^^xsd:int ;
.
cfr-12-217:r-1-8-3-44
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(iii) If an investment is in the form of a non-qualifying capital instrument (as defined in § 217.300(c)), the Board-regulated institution must treat the instrument as:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "44"^^xsd:int ;
.
cfr-12-217:r-1-8-3-45
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(A) An additional tier 1 capital instrument if such instrument was included in the issuer's tier 1 capital prior to May 19, 2010; or" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "45"^^xsd:int ;
.
cfr-12-217:r-1-8-3-46
  rdf:type cfr-fdsys-s:P ;

usc-t12-ch17:r-1-0-4-9-7-0-3
  rdf:type uslm:p ;
  uslm:class-p "indent0" ;
  composite:child usc-t12-ch17:r-1-0-4-9-7-0-3-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", That the Board may, in its discretion, compromise, modify, or remit any civil money penalty which is subject to imposition or has been imposed under authority of this subsection. The penalty may be assessed and collected by the Board by written notice. As used in the section, the term ‘violates’ includes without any limitation any action (alone or with another or others) for or toward causing, bringing about, participating in, counseling, or aiding or abetting a violation." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "“(1) Any company which violates or any individual who participates in a violation of any provision of this chapter, or any regulation or order issued pursuant thereto, shall forfeit and pay a civil penalty of not more than $1,000 per day for each day during which such violation continues:" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.
usc-t12-ch17:r-1-0-4-9-7-0-3-1
  rdf:type uslm:i ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Provided" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
usc-t12-ch17:r-1-0-4-9-7-0-4
  rdf:type uslm:p ;
  uslm:class-p "indent0" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "“(2) In determining the amount of the penalty the Board shall take into account the appropriateness of the penalty with respect to the size of financial resources and good faith of the company or person charged, the gravity of the violation, the history of previous violations, and such other matters as justice may require." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.
usc-t12-ch17:r-1-0-4-9-7-0-5
  rdf:type uslm:p ;
  uslm:class-p "indent0" ;
  composite:child usc-t12-ch17:r-1-0-4-9-7-0-5-1 ;
  composite:child usc-t12-ch17:r-1-0-4-9-7-0-5-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ". If no hearing is requested as herein provided, the assessment shall constitute a final and unappealable order." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ". The agency determination shall be made by final order which may be reviewed only as provided in" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "“(3) The company or person assessed shall be afforded an opportunity for agency hearing, upon request made within ten days after issuance of the notice of assessment. In such hearing all issues shall be determined on the record pursuant to" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "5"^^xsd:int ;
.

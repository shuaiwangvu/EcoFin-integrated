  composite:child usc-t12-ch17:r-1-0-4-3-2-5-2-1-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", only if the bank or company has the right consistent with its obligations under the instrument, agreement, or other arrangement establishing the fiduciary relationship to divest itself of such voting rights and fails to exercise that right to divest within a reasonable period not to exceed one year after" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "No bank and no company owning or controlling voting shares of a bank is a bank holding company by virtue of its ownership or control of shares in a fiduciary capacity, except as provided in paragraphs (2) and (3) of subsection (g) of this section. For the purpose of the preceding sentence, bank shares shall not be deemed to have been acquired in a fiduciary capacity if the acquiring bank or company has sole discretionary authority to exercise voting rights with respect thereto; except that this limitation is applicable in the case of a bank or company acquiring such shares prior to" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;

  rdf:type uslm:content ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The total amount of extensions of credit by a bank holding company which invests in an export trading company, when combined with all such extensions of credit by all the subsidiaries of such bank holding company, to an export trading company shall not exceed at any one time 10 per centum of the bank holding company’s consolidated capital and surplus. For purposes of the preceding sentence, an extension of credit shall not be deemed to include any amount invested by a bank holding company in the shares of an export trading company." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

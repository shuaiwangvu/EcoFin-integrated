  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "), which is a subsidiary of a bank holding company, or an agreement corporation, operating subject to section 25 of the Federal Reserve Act [" ;
      composite:index "6"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Notwithstanding any other provision of law, an Edge Act corporation, organized under section 25(a) " ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "et seq.], which is a subsidiary of a bank holding company, may invest directly and indirectly in the aggregate up to 5 per centum of its consolidated capital and surplus (25 per centum in the case of a corporation not engaged in banking) in the voting stock of " ;
      composite:index "8"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "of the Federal Reserve Act (" ;
      composite:index "4"^^xsd:int ;
    ] ;
  composite:child [

usc-t12-ch17:r-1-0-4-9-7-0-6
  rdf:type uslm:p ;
  uslm:class-p "indent0" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "“(4) If any company or person fails to pay an assessment after it has become a final and unappealable order, or after the court of appeals has entered final judgment in favor of the Board, the Board shall refer the matter to the Attorney General, who shall recover the amount assessed by action in the appropriate United States district court. In such action the validity and appropriateness of the final order imposing the penalty shall not be subject to review." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "6"^^xsd:int ;
.

<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.estrellaproject.org/lkif-core/time-modification.owl#"
    xmlns:action="http://www.estrellaproject.org/lkif-core/legal-action.owl#"
    xmlns:time="http://www.estrellaproject.org/lkif-core/time.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://www.estrellaproject.org/lkif-core/time-modification.owl">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >
	This module is part of LKIF-Core, version 1.0.3, part of Deliverable 1.4 of ESTRELLA, IST-2004-027665

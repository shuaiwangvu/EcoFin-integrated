	owl:disjointWith norm:Treaty .

norm:Mandatory_Precedent a owl:Class ;
	rdfs:comment "In law, a binding precedent (also mandatory precedent or binding authority) is a precedent which must be followed by all lower courts. It is usually created by the decision of a higher court, such as the House of Lords in the United Kingdom. Binding precedent relies on the legal principle of stare decisis." ;
	rdfs:subClassOf norm:Precedent .

expression:promised_by a owl:ObjectProperty ;
	rdfs:comment "Relates an expression to the promise over the expression" ;
	owl:inverseOf expression:promises ;
	rdfs:range expression:Promise ;
	rdfs:domain expression:Expression ;
	rdfs:subPropertyOf expression:stated_by .

norm:Code a owl:Class ;
	rdfs:comment "A legal code bears one or more norms, all of which are uttered by some legislative body. It cannot bear expressions which are not uttered by a legislative body." ;
	rdfs:subClassOf norm:Legal_Document , _:node1fqlpa3rgx265 , _:node1fqlpa3rgx270 .


	owl:inverseOf expression:utterer ;
	rdfs:range expression:Communicated_Attitude ;
	rdfs:domain action:Agent ;
	rdfs:subPropertyOf expression:holds .

expression:believes a owl:ObjectProperty ;
	rdfs:comment "Relates an agent to the belief(s) it holds" ;
	owl:inverseOf expression:believed_by ;
	rdfs:range expression:Belief ;
	rdfs:domain action:Agent ;
	rdfs:subPropertyOf expression:holds .

expression:intends a owl:ObjectProperty ;

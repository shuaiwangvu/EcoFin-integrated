	rdfs:subClassOf expression:Medium ;
	owl:disjointWith expression:Document .

norm:Exclusionary_Right a owl:Class ;
	rdfs:comment """An exclusionary right concerns the prohibition against performing certain inferences (against reasoning in certain ways), or 
against using certain kinds of premises for certain purposes, in the interest of a particular person. This is especially the case with anti-discrimination rules.""" ;
	rdfs:subClassOf norm:Obligative_Right .

expression:asserted_by a owl:ObjectProperty ;

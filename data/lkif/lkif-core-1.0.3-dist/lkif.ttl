	rdfs:subPropertyOf process:participant .

norm:Legal_Source a owl:Class ;
	rdfs:comment "A legal source is a source for legal statements, both norms and legal expressions. In a sense it is literally a 'source' of law" ;
	rdfs:subClassOf expression:Medium ;
	owl:equivalentClass _:node1fqlpa3rgx254 .

ns5:Extension a owl:Class ;
	rdfs:comment "Extends the scope of normative content to some particular jurisdiction" ;
	rdfs:subClassOf ns5:Modification_of_Scope .

norm:Strictly_Disallowed a owl:Class ;
	rdfs:comment "Loose end." ;
	rdfs:subClassOf norm:Disallowed .

norm:Disallowed a owl:Class ;
	rdfs:comment "See Qualified. Alternative labels: violation, prohibited, forbidden" ;
	rdfs:subClassOf norm:Normatively_Qualified ;
	owl:equivalentClass _:node1fqlpa3rgx316 , _:node1fqlpa3rgx317 .

ns5:Renewal a owl:Class ;

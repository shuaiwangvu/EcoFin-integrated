fr-rule-iaa:Relevant_Ex_USC_s80b-3a_a_1_Prohibited
  rdf:type owl:Class ;
  rdfs:label "USC IAA 202(a)(11)" ;
  rdfs:subClassOf fr-rule-iaa:LegalCondition ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          fibo-be-fct-fct:FunctionalBusinessEntity
          [
            rdf:type owl:Class ;
            owl:intersectionOf (
                [
                  rdf:type owl:Restriction ;
                  owl:onProperty fro-leg-ref:regulated_as ;
                  owl:someValuesFrom fr-rule-iaa:Legal_Status_Prohibition_Defined ;
                ]
                [
                  rdf:type owl:Class ;
                  owl:complementOf [
                      rdf:type owl:Restriction ;
                      owl:onProperty fro-leg-ref:regulated_as ;

fr-rule-iaa:SEC_Large_Adviser
  rdf:type owl:Class ;
  rdfs:label "Legal Status Expression: Defined as Service" ;
  rdfs:subClassOf fr-fin-ref:InvestmentAdviser ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          fr-fin-ref:InvestmentAdviser
          [
            rdf:type owl:Restriction ;
            owl:onProperty fr-fin-ref:hasAssetUnderManagementMonetaryAmount ;
            owl:someValuesFrom [
                rdf:type owl:Class ;
                owl:intersectionOf (
                    fibo-fnd-acc-cur:MonetaryAmount
                    [
                      rdf:type owl:Restriction ;
                      owl:hasValue fr-fin-ref:AUM_Total ;
                      owl:onProperty fibo-fnd-rel-rel:isClassifiedBy ;
                    ]
                    [
                      rdf:type owl:Restriction ;
                      owl:onProperty fibo-fnd-acc-cur:hasAmount ;
                      owl:someValuesFrom [
                          rdf:type rdfs:Datatype ;
                          owl:onDatatype xsd:decimal ;
                          owl:withRestrictions (
                              [
                                xsd:minExclusive "100000000"^^xsd:decimal ;
                              ]
                            ) ;
                        ] ;
                    ]
                  ) ;
              ] ;
          ]
        ) ;
    ] ;
  skos:definition "An Investment Adviser with more than 100 million assets under management." ;
.

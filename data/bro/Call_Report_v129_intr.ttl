  rdf:type linkbase:Label ;
  ffiec-cr-v129-intr:lang-label "en" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text """Memorandum item 10 is applicable to all banks filing the FFIEC 031 report forms and to those banks filing the FFIEC 041 report forms that have $300 million or more in total assets.


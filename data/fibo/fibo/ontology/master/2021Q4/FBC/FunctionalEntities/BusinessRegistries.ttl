	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:changeNote
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified per FIBO 2.0 RFC primarily to loosen the constraints on address properties and better support standards including ISO 9362 (BIC codes), ISO 13616 (IBAN and BBAN codes), and ISO 17442 (the GLIEF LEI standard)." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FBC 1.1 RTF report." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified to generalize certain unions where they were no longer required, use the composite date datatype where appropriate, add individuals for entity expiration reason and validation level to better align with the GLEIF LEI data, and move international registration authorities, such as SWIFT, to a separate ontology for better modularity." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to clarify the definition of registry identifier and augment the definitions of certain identifiers, such as an LEI, to make them registry identifiers, as well as to modify the definition of an LOU to be a Registrar rather than RegistrationAuthority, and deprecate the redundant LOU identifier." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to eliminate circular and ambiguous definitions." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to eliminate deprecated elements." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to eliminate duplication with concepts in LCC, simplify addresses, normalize definitions to be ISO 704 compliant, and rationalize address properties." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to reflect the move of certain organization-specific concepts from BE to FND." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to replace hasDefinition with isDefinedIn to clarify intent." ,
		"The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was revised to replace uses of hasTag in Relations with hasTag from LCC, as the more complex union of datatypes in the Relations concept is not needed here."
		;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Release ;
	.

fibo-be-le-fbo:ValueAddedTaxIdentificationNumber
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-fct-ra:RegistryIdentifier ;
	.

fibo-be-le-lei:LegalEntityIdentifier
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-fct-ra:RegistryIdentifier ;
	.

fibo-be-le-lp:LegalEntity
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasEntityExpirationReason ;
			owl:onClass fibo-fbc-fct-breg:EntityExpirationReason ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasEntityStatus ;
			owl:onClass fibo-fbc-fct-breg:EntityStatus ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasEntityExpirationDate ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			owl:onDataRange fibo-fnd-dt-fd:CombinedDateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-fpas:precedes ;
			owl:onClass fibo-be-le-lp:LegalEntity ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-cr:isClassifiedBy ;
			owl:onClass fibo-fbc-fct-breg:NorthAmericanIndustryClassificationSystemCode ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-cr:isClassifiedBy ;
			owl:onClass fibo-fbc-fct-breg:StandardIndustrialClassificationCode ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-le-fbo:hasHeadquartersAddress ;
			owl:onClass fibo-fnd-plc-adr:PhysicalAddress ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	.

fibo-fbc-fct-breg:ActiveStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityStatus
		;
	rdfs:label "active status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that as of the last report or update, the entity was legally registered and operating" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	lcc-lr:hasTag "ACTIVE" ;
	.

fibo-fbc-fct-breg:AnnulledStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "annulled status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that the registration was determined to be erroneous or invalid after issuance" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	lcc-lr:hasTag "ANNULLED" ;
	.

fibo-fbc-fct-breg:BusinessRegisterIdentifier
	a owl:Class ;
	rdfs:subClassOf
		lcc-lr:Identifier ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:identifies ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistry ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:isRegisteredBy ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
		]
		;
	rdfs:label "business register identifier" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "identifier that uniquely identifies a business register, such as a register identified by the Global Legal Entity Identifier Foundation (GLEIF) registration authorities list" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegistrationAuthority
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistrationAuthority ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:registers ;
			owl:someValuesFrom fibo-be-corp-corp:RegistrationIdentifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:manages ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistry ;
		]
		;
	rdfs:label "business registration authority" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "registration authority that is responsible for maintaining a registry of business entities" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "A business registry may include any government-managed registry for registering a business, such as a state department of corporations in the US, as well as other registries such as a local operating unit (LOU) for registration of legal entity identifiers (LEIs).  Any sanctioned registration authority as defined by the Registration Authorities List, published by GLEIF, is a business registration authority in this sense." ;
	.

fibo-fbc-fct-breg:BusinessRegistry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:Registry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistryName ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistryEntry ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistryEntry ;
		]
		;
	rdfs:label "business registry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "registry for registering and maintaining information about business entities" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegistryEntry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistryEntry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistrationStatus ;
			owl:onClass fibo-fbc-fct-breg:RegistrationStatus ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistrationDate ;
			owl:someValuesFrom fibo-fnd-dt-fd:CombinedDateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-be-corp-corp:RegistrationIdentifier ;
		]
		;
	rdfs:label "business registry entry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "entry in a business registry" ;
	.

fibo-fbc-fct-breg:CancelledStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "cancelled status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that the registration was abandoned prior to issuance" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	lcc-lr:hasTag "CANCELLED" ;
	.

fibo-fbc-fct-breg:DuplicateStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "duplicate status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "status indicating that the registration was determined to be a duplicate registration of the same entity as another registration; duplicate status is assigned to the non-surviving registration (i.e. the identifier that should no longer be used)" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	lcc-lr:hasTag "DUPLICATE" ;
	.

fibo-fbc-fct-breg:EntityExpirationReason
	a owl:Class ;
	rdfs:subClassOf
		lcc-lr:CodeElement ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:hasTag ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "entity expiration reason" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "code for the reason that a legal entity ceased to exist and/or operate" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityExpirationReasonCorporateAction
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityExpirationReason
		;
	rdfs:label "entity expiration reason - corporate action" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "expiration reason indicating that an entity was acquired or merged with another entity" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	lcc-lr:hasTag "CORPORATE_ACTION" ;
	.


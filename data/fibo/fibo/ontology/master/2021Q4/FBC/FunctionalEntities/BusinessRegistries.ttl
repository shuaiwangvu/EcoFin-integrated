	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	lcc-lr:hasTag "OTHER" ;
	.

fibo-fbc-fct-breg:EntityLegalFormRegistry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:Registry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistryEntry ;
			owl:someValuesFrom fibo-fbc-fct-breg:EntityLegalFormRegistryEntry ;
		]
		;

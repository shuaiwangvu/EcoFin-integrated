  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-17-275:r-1-24-9-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(d)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "You must pay FINRA (the operator of the IARD) an initial filing fee when you first electronically file Part 1A of Form ADV. After you pay the initial filing fee, you must pay an annual filing fee each time you file your annual updating amendment. No portion of either fee is refundable. The Commission has approved the filing fees. Your amended Form ADV will not be accepted by FINRA, and thus will not be considered filed with the Commission, until you have paid the filing fee." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "9"^^xsd:int ;

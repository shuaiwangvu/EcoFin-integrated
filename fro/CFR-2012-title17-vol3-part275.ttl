      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "14"^^xsd:int ;
.
cfr-17-275:r-1-12-15
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(5) You are not required to count a person as an investor, as that term is defined in paragraph (c)(2) of this section, in a private fund you advise if you count such person as a client in the United States." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "15"^^xsd:int ;

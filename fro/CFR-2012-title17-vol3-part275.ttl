      composite:index "2"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "for you to have custody of client funds or securities unless:" ;
      composite:index "4"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) If an amendment to any such application shall be filed, such amendment shall contain a similar statement or, in lieu thereof, shall state that the authorization described in the original application is applicable to the individual who signs such amendment and that such authorization still remains in effect." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "8"^^xsd:int ;
.

  composite:child cfr-17-275:r-1-33-16-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) The term" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "has the same meaning as in section 202(a)(5) of the Act (15 U.S.C. 80b-2(a)(5)), but does not include a company that is required to be registered under the Investment Company Act of 1940 but is not registered." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "16"^^xsd:int ;
.

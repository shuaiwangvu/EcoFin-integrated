  <file:///FinRegOnt/cfr/CFR-2012-title17-vol3-part275.xml#T-E> "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Multiple brochures." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.
cfr-17-275:r-1-26-23
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-17-275:r-1-26-23-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(f)Delivering a brochure or brochure supplement in compliance with this section does not relieve you of any other disclosure obligations you have to your advisory clients or prospective clients under any federal or state laws or regulations." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "23"^^xsd:int ;
.

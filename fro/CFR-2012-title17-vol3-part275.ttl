  composite:child cfr-17-275:r-1-6-2-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)(1) All papers required to be filed with the Commission shall, unless otherwise provided by the rules and regulations, be delivered through the mails or otherwise to the Securities and Exchange Commission, Washington, DC 20549. Except as otherwise provided by the rules and regulations, such papers shall be deemed to have been filed with the Commission on the date when they are actually received by it." ;
      composite:index "2"^^xsd:int ;
    ] ;

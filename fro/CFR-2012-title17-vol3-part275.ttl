cfr-17-275:r-1-26-2
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-17-275:r-1-26-2-1 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "If you are registered under the Act as an investment adviser, you must deliver a brochure and one or more brochure supplements to each client or prospective client that contains all information required by Part 2 of Form ADV [17 CFR 279.1]." ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.
cfr-17-275:r-1-26-2-1
  rdf:type cfr-fdsys-s:E ;
  <file:///FinRegOnt/cfr/CFR-2012-title17-vol3-part275.xml#T-E> "03" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "General requirements." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
cfr-17-275:r-1-26-20
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2) You do not have to deliver a wrap fee program brochure if another sponsor of the wrap fee program delivers, to the client or prospective client of the wrap fee program, a wrap fee program brochure containing all the information required by Part 2A, Appendix 1 of Form ADV." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "20"^^xsd:int ;
.
cfr-17-275:r-1-26-21
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Note to paragraph (d): A wrap fee program brochure does not take the place of any brochure supplements that you are required to deliver under paragraph (b) of this section." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "21"^^xsd:int ;
.
cfr-17-275:r-1-26-22
  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-17-275:r-1-26-22-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(e)If you provide substantially different advisory services to different clients, you may provide them with different brochures, so long as each client receives all information about the services and fees that are applicable to that client. The brochure you deliver to a client may omit any information required by Part 2A of Form ADV if the information does not apply to the advisory services or fees that you will provide or charge, or that you propose to provide or charge, to that client." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "22"^^xsd:int ;
.

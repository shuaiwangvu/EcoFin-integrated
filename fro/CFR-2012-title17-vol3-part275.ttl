  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-17-275:r-1-33-14-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(A) An executive officer, director, trustee, general partner, or personserving in a similar capacity, of the investment adviser; or" ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "14"^^xsd:int ;
.

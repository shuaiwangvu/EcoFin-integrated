  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(ii) A natural person who or a company that the investment adviser entering into the contract (and any person acting on his behalf) reasonably believes, immediately prior to entering into the contract, either:" ;

  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Provided, however," ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.
cfr-17-275:r-1-34-3
  rdf:type cfr-fdsys-s:P ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(b) For the purpose of this Rule, publicly distributed written materials are those which are distributed to 35 or more persons who pay for such materials, and publicly made oral statements are those made simultaneously to 35 or more persons who pay for access to such statements." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "3"^^xsd:int ;
.

  rdf:type cfr-fdsys-s:P ;
  composite:child cfr-17-275:r-1-32-4-0 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(2)shall mean a period consisting of a specified number of subperiods of definite length in which the most recent subperiod is substituted for the earliest subperiod as time passes." ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:index "4"^^xsd:int ;
.

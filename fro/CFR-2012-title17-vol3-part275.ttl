  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "(a)" ;
      composite:index "2"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The provisions of section 205(a)(1) of the Act (15 U.S.C. 80b-5(a)(1)) will not be deemed to prohibit an investment adviser from entering into, performing, renewing or extending an investment advisory contract that provides for compensation to the investment adviser on the basis of a share of the capital gains upon, or the capital appreciation of, the funds, or any portion of the funds, of a client,That the client entering into the contract subject to this section is a qualified client, as defined in paragraph (d)(1) of this section." ;
      composite:index "4"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

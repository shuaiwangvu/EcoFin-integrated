  fro-usc:hasContentText "an insurance company, with respect to insurance activities of the insurance company and activities incidental to such insurance activities, that is subject to supervision by a State insurance regulator; or" ;
  fro-usc:hasId "id831ea5a4-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/c/5/B/iv" ;

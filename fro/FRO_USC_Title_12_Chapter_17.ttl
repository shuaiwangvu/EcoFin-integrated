  fro-usc:hasContentText "the Federal Deposit Insurance Corporation learns that a bank insured by such Corporation is in danger of closing; and" ;
  fro-usc:hasId "id831ea5f4-1df7-11e6-87ee-8e4a3ab90f2a" ;

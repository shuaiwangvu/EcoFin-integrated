  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-6-4-5-2> ;
  fro-usc:hasContentText "Securities activities conducted in a functionally regulated subsidiary of a depository institution shall be subject to regulation by the Securities and Exchange Commission, and by relevant State securities authorities, as appropriate, subject to section 6701 of title 15 , to the same extent as if they were conducted in a nondepository institution subsidiary of a bank holding company." ;
  fro-usc:hasHeading "Securities activities" ;
  fro-usc:hasId "id831ea59c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/c/4/A" ;

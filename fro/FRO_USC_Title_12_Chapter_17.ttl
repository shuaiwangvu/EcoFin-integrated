  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-5-3-3-3> ;
  fro-usc:hasContentText "not later than 1 year after the date of establishment of the fund, be reduced through redemption, sale, or dilution to an amount that is not more than 3 percent of the total ownership interests of the fund;" ;
  fro-usc:hasId "id83211691-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/4/B/ii/I" ;
  fro-usc:hasNumberText "(I)" ;
.
fro-usc-t12-ch17:id83211692-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-5-3-3-4> ;
  fro-usc:hasContentText "be immaterial to the banking entity, as defined, by rule, pursuant to subsection (b)(2), but in no case may the aggregate of all of the interests of the banking entity in all such funds exceed 3 percent of the Tier 1 capital of the banking entity." ;
  fro-usc:hasId "id83211692-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/4/B/ii/II" ;
  fro-usc:hasNumberText "(II)" ;
.

  lkif-mereo:component fro-usc-t12-ch17:id8321167a-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8321167b-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8321167c-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8321167d-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8321167e-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id83211677-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-2> ;
  fro-usc:hasContentText "the banking entity provides bona fide trust, fiduciary, or investment advisory services;" ;
  fro-usc:hasId "id83211677-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/i" ;
  fro-usc:hasNumberText "(i)" ;
.
fro-usc-t12-ch17:id83211678-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-3> ;
  fro-usc:hasContentText "the fund is organized and offered only in connection with the provision of bona fide trust, fiduciary, or investment advisory services and only to persons that are customers of such services of the banking entity;" ;
  fro-usc:hasId "id83211678-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.
fro-usc-t12-ch17:id83211679-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-4> ;
  fro-usc:hasContentText "the banking entity does not acquire or retain an equity interest, partnership interest, or other ownership interest in the funds except for a de minimis investment subject to and in compliance with paragraph (4);" ;
  fro-usc:hasId "id83211679-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/iii" ;
  fro-usc:hasNumberText "(iii)" ;
.
fro-usc-t12-ch17:id8321167a-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-5> ;
  fro-usc:hasContentText "the banking entity complies with the restrictions under paragraphs (1) and (2) of subparagraph (f);" ;
  fro-usc:hasId "id8321167a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/iv" ;
  fro-usc:hasNumberText "(iv)" ;
.
fro-usc-t12-ch17:id8321167b-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-6> ;
  fro-usc:hasContentText "the banking entity does not, directly or indirectly, guarantee, assume, or otherwise insure the obligations or performance of the hedge fund or private equity fund or of any hedge fund or private equity fund in which such hedge fund or private equity fund invests;" ;
  fro-usc:hasId "id8321167b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/v" ;
  fro-usc:hasNumberText "(v)" ;
.
fro-usc-t12-ch17:id8321167c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-7> ;
  fro-usc:hasContentText "the banking entity does not share with the hedge fund or private equity fund, for corporate, marketing, promotional, or other purposes, the same name or a variation of the same name;" ;
  fro-usc:hasId "id8321167c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/vi" ;
  fro-usc:hasNumberText "(vi)" ;
.
fro-usc-t12-ch17:id8321167d-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-8> ;
  fro-usc:hasContentText "no director or employee of the banking entity takes or retains an equity interest, partnership interest, or other ownership interest in the hedge fund or private equity fund, except for any director or employee of the banking entity who is directly engaged in providing investment advisory or other services to the hedge fund or private equity fund; and" ;
  fro-usc:hasId "id8321167d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/vii" ;
  fro-usc:hasNumberText "(vii)" ;
.
fro-usc-t12-ch17:id8321167e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9-9> ;
  fro-usc:hasContentText "the banking entity discloses to prospective and actual investors in the fund, in writing, that any losses in such hedge fund or private equity fund are borne solely by investors in the fund and not by the banking entity, and otherwise complies with any additional rules of the appropriate Federal banking agencies, the Securities and Exchange Commission, or the Commodity Futures Trading Commission, as provided in subsection (b)(2), designed to ensure that losses in such hedge fund or private equity fund are borne solely by investors in the fund and not by the banking entity." ;
  fro-usc:hasId "id8321167e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G/viii" ;
  fro-usc:hasNumberText "(viii)" ;
.
fro-usc-t12-ch17:id8321167f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 10 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-10> ;
  fro-usc:hasContentText "Proprietary trading conducted by a banking entity pursuant to paragraph (9) or (13) of section 1843(c) of this title , provided that the trading occurs solely outside of the United States and that the banking entity is not directly or indirectly controlled by a banking entity that is organized under the laws of the United States or of one or more States." ;
  fro-usc:hasId "id8321167f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/H" ;
  fro-usc:hasNumberText "(H)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1843/c" ;
.
fro-usc-t12-ch17:id83211680-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 11 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-11> ;
  fro-usc:hasContentText "The acquisition or retention of any equity, partnership, or other ownership interest in, or the sponsorship of, a hedge fund or a private equity fund by a banking entity pursuant to paragraph (9) or (13) of section 1843(c) of this title solely outside of the United States, provided that no ownership interest in such hedge fund or private equity fund is offered for sale or sold to a resident of the United States and that the banking entity is not directly or indirectly controlled by a banking entity that is organized under the laws of the United States or of one or more States." ;
  fro-usc:hasId "id83211680-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/I" ;
  fro-usc:hasNumberText "(I)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1843/c" ;
.
fro-usc-t12-ch17:id83211681-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 12 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-12> ;
  fro-usc:hasContentText "Such other activity as the appropriate Federal banking agencies, the Securities and Exchange Commission, and the Commodity Futures Trading Commission determine, by rule, as provided in subsection (b)(2), would promote and protect the safety and soundness of the banking entity and the financial stability of the United States." ;
  fro-usc:hasId "id83211681-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/J" ;
  fro-usc:hasNumberText "(J)" ;
.

  fro-usc:hasContentText "the banking entity is in compliance with each of the limitations set forth in subsection (d)(1)(G) with regard to a hedge fund or private equity fund organized and offered by such banking entity;" ;
  fro-usc:hasId "id8321169d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/f/3/A/i" ;

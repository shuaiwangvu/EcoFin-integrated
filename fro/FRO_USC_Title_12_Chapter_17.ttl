  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-3-2-4> ;
  fro-usc:hasContentText "would result, directly or indirectly, in a material exposure by the banking entity to high-risk assets or high-risk trading strategies (as such terms shall be defined by rule as provided in subsection (b)(2));" ;
  fro-usc:hasId "id83211685-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/2/A/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.
fro-usc-t12-ch17:id83211686-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-3-2-5> ;
  fro-usc:hasContentText "would pose a threat to the safety and soundness of such banking entity; or" ;
  fro-usc:hasId "id83211686-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/2/A/iii" ;
  fro-usc:hasNumberText "(iii)" ;
.
fro-usc-t12-ch17:id83211687-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-3-2-6> ;
  fro-usc:hasContentText "would pose a threat to the financial stability of the United States." ;
  fro-usc:hasId "id83211687-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/2/A/iv" ;
  fro-usc:hasNumberText "(iv)" ;
.
fro-usc-t12-ch17:id83211688-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-3-3> ;
  fro-usc:hasContentText "The appropriate Federal banking agencies, the Securities and Exchange Commission, and the Commodity Futures Trading Commission shall issue regulations to implement subparagraph (A), as part of the regulations issued under subsection (b)(2)." ;
  fro-usc:hasHeading "Rulemaking" ;
  fro-usc:hasId "id83211688-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/2/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id83211689-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-4> ;
  fro-usc:hasContentText "The appropriate Federal banking agencies, the Securities and Exchange Commission, and the Commodity Futures Trading Commission shall, as provided in subsection (b)(2), adopt rules imposing additional capital requirements and quantitative limitations, including diversification requirements, regarding the activities permitted under this section if the appropriate Federal banking agencies, the Securities and Exchange Commission, and the Commodity Futures Trading Commission determine that additional capital and quantitative limitations are appropriate to protect the safety and soundness of banking entities engaged in such activities." ;
  fro-usc:hasHeading "Capital and quantitative limitations" ;
  fro-usc:hasId "id83211689-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/3" ;
  fro-usc:hasNumberText "(3)" ;
.

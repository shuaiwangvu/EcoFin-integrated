  fro-usc:hasHeading "Recordkeeping and reporting" ;
  fro-usc:hasId "id83211620-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1" ;
  fro-usc:hasNumberText "(1)" ;
  lkif-mereo:component fro-usc-t12-ch17:id83211621-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id83211622-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id83211621-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-2-2> ;
  fro-usc:hasContentText "Each supervised securities holding company and each affiliate of a supervised securities holding company shall make and keep for periods determined by the Board of Governors such records, furnish copies of such records, and make such reports, as the Board of Governors determines to be necessary or appropriate to carry out this section, to prevent evasions thereof, and to monitor compliance by the supervised securities holding company or affiliate with applicable provisions of law." ;
  fro-usc:hasHeading "Recordkeeping and reporting required" ;
  fro-usc:hasId "id83211621-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/A" ;
  fro-usc:hasNumberText "(A)" ;
.

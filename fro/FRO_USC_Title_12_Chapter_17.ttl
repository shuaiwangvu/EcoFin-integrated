.
fro-usc-t12-ch17:id8321169e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-7-4-2-4> ;
  fro-usc:hasContentText "the chief executive officer (or equivalent officer) of the banking entity certifies in writing annually (with a duty to update the certification if the information in the certification materially changes) that the conditions specified in subsection (d)(1)(g)(v)  2 are satisfied; and" ;
  fro-usc:hasId "id8321169e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/f/3/A/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.
fro-usc-t12-ch17:id8321169f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 5 ;

  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-6> ;
  fro-usc:hasContentText "The purchase, sale, acquisition, or disposition of securities and other instruments described in subsection (h)(4) on behalf of customers." ;
  fro-usc:hasId "id83211671-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/D" ;
  fro-usc:hasNumberText "(D)" ;
.
fro-usc-t12-ch17:id83211672-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-7> ;
  fro-usc:hasContentText "Investments in one or more small business investment companies, as defined in section 102  of the Small Business Investment Act of 1958 ( 15 U.S.C. 662 ), investments designed primarily to promote the public welfare, of the type permitted under paragraph (11) of section 24 of this title , or investments that are qualified rehabilitation expenditures with respect to a qualified rehabilitated building or certified historic structure, as such terms are defined in section 47 of title 26 or a similar State historic tax credit program." ;
  fro-usc:hasId "id83211672-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/E" ;
  fro-usc:hasNumberText "(E)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s24" ;
  fro-usc:hasReferenceText "/us/usc/t15/s662" ;
  fro-usc:hasReferenceText "/us/usc/t26/s47" ;
.

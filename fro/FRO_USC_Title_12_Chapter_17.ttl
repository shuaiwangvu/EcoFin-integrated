.
fro-usc-t12-ch17:id832116a3-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-8-2> ;
  fro-usc:hasContentText "Except as provided in this section, notwithstanding any other provision of law, the prohibitions and restrictions under this section shall apply to activities of a banking entity or nonbank financial company supervised by the Board, even if such activities are authorized for a banking entity or nonbank financial company supervised by the Board." ;
  fro-usc:hasHeading "Limitation on contrary authority" ;
  fro-usc:hasId "id832116a3-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/g/1" ;
  fro-usc:hasNumberText "(1)" ;
.
fro-usc-t12-ch17:id832116a4-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-8-3> ;
  fro-usc:hasContentText "Nothing in this section shall be construed to limit or restrict the ability of a banking entity or nonbank financial company supervised by the Board to sell or securitize loans in a manner otherwise permitted by law." ;
  fro-usc:hasHeading "Sale or securitization of loans" ;
  fro-usc:hasId "id832116a4-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/g/2" ;
  fro-usc:hasNumberText "(2)" ;
.
fro-usc-t12-ch17:id832116a5-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-8-4> ;
  fro-usc:hasContentText "Nothing in this section shall be construed to limit the inherent authority of any Federal agency or State regulatory authority under otherwise applicable provisions of law." ;
  fro-usc:hasHeading "Authority of Federal agencies and State regulatory authorities" ;
  fro-usc:hasId "id832116a5-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/g/3" ;
  fro-usc:hasNumberText "(3)" ;
.

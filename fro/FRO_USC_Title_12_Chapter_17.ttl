  fro-usc:hasContentText "protect taxpayers and consumers and enhance financial stability by minimizing the risk that insured depository institutions and the affiliates of insured depository institutions will engage in unsafe and unsound activities;" ;
  fro-usc:hasId "id8321164f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/b/1/B" ;

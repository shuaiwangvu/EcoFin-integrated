  fro-usc:hasContentText "registers as a bank holding company under section 1844(a) of this title ;" ;
  fro-usc:hasId "id8319ea98-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/5/A" ;

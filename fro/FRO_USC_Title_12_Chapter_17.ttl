  fro-usc:hasContentText "such arrangement does not violate section 106 of the Bank Holding Company Act Amendments of 1970 [ 12 U.S.C. 1971 et seq.]; and" ;
  fro-usc:hasId "id831c5c60-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/5/B/i" ;

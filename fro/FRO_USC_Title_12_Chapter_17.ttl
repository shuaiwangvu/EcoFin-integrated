  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-6-4-5-3> ;
  fro-usc:hasContentText "Subject to section 6701 of title 15 , insurance agency and brokerage activities and activities as principal conducted in a functionally regulated subsidiary of a depository institution shall be subject to regulation by a State insurance authority to the same extent as if they were conducted in a nondepository institution subsidiary of a bank holding company." ;
  fro-usc:hasHeading "Insurance activities" ;
  fro-usc:hasId "id831ea59d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/c/4/B" ;

  fro-usc:hasContentText "the Board has determined that such transaction is consistent with the safe and sound operation and condition of the banking entity." ;
  fro-usc:hasId "id8321169f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/f/3/A/iii" ;
  fro-usc:hasNumberText "(iii)" ;
.
fro-usc-t12-ch17:id832116a0-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-7-4-3> ;
  fro-usc:hasContentText "For purposes of subparagraph (A), a prime brokerage transaction described in subparagraph (A) shall be subject to section 371c–1 of this title as if the counterparty were an affiliate of the banking entity." ;
  fro-usc:hasHeading "Treatment of prime brokerage transactions" ;
  fro-usc:hasId "id832116a0-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/f/3/B" ;
  fro-usc:hasNumberText "(B)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s371c–1" ;
.
fro-usc-t12-ch17:id832116a1-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-7-5> ;
  fro-usc:hasContentText "The appropriate Federal banking agencies, the Securities and Exchange Commission, and the Commodity Futures Trading Commission shall adopt rules, as provided in subsection (b)(2), imposing additional capital charges or other restrictions for nonbank financial companies supervised by the Board to address the risks to and conflicts of interest of banking entities described in paragraphs (1), (2), and (3) of this subsection." ;
  fro-usc:hasHeading "Application to nonbank financial companies supervised by the Board" ;
  fro-usc:hasId "id832116a1-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/f/4" ;
  fro-usc:hasNumberText "(4)" ;
.

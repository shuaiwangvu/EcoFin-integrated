  fro-leg-ref:divided_by fro-usc-t12-ch17:id832116a5-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-8> ;
  fro-usc:hasHeading "Rules of construction" ;
  fro-usc:hasId "id832116a2-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/g" ;

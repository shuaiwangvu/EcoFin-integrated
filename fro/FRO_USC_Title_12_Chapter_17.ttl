  fro-usc:hasContentText "will meet all applicable regulatory capital requirements as a result of such acquisition." ;
  fro-usc:hasId "id831c5bca-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/3/B/iii" ;
  fro-usc:hasNumberText "(iii)" ;
.

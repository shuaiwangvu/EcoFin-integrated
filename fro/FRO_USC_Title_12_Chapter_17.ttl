fro-usc-t12-ch17:id8319ea17-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-3-3> ;
  fro-usc:hasContentText "immediately before the consummation of the acquisition for which such application is filed, the applicant (including any insured depository institution affiliate of the applicant) controls any insured depository institution or any branch of an insured depository institution in the home State of any bank to be acquired or in any host State in which any such bank maintains a branch; and" ;
  fro-usc:hasId "id8319ea17-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/B/i" ;
  fro-usc:hasNumberText "(i)" ;
.
fro-usc-t12-ch17:id8319ea18-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-3-4> ;
  fro-usc:hasContentText "the applicant (including all insured depository institutions which are affiliates of the applicant), upon consummation of the acquisition, would control 30 percent or more of the total amount of deposits of insured depository institutions in any such State." ;
  fro-usc:hasId "id8319ea18-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/B/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.
fro-usc-t12-ch17:id8319ea19-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-4> ;
  fro-usc:hasContentText "No provision of this subsection shall be construed as affecting the authority of any State to limit, by statute, regulation, or order, the percentage of the total amount of deposits of insured depository institutions in the State which may be held or controlled by any bank or bank holding company (including all insured depository institutions which are affiliates of the bank or bank holding company) to the extent the application of such limitation does not discriminate against out-of-State banks, out-of-State bank holding companies, or subsidiaries of such banks or holding companies." ;
  fro-usc:hasHeading "Effectiveness of State deposit caps" ;
  fro-usc:hasId "id8319ea19-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/C" ;
  fro-usc:hasNumberText "(C)" ;
.

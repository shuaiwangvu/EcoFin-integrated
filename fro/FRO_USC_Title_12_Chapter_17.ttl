  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-5-15-2-2> ;
  lkif-mereo:component fro-usc-t12-ch17:id831c5c55-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id831c5c56-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id831c5c57-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id831c5c55-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-15-2-3> ;
  fro-usc:hasContentText "the holding company lawfully was engaged in the activity or held the shares of such company on September 30, 1999 ;" ;
  fro-usc:hasId "id831c5c55-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/1/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id831c5c56-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-15-2-4> ;
  fro-usc:hasContentText "the holding company is predominantly engaged in financial activities as defined in paragraph (2); and" ;
  fro-usc:hasId "id831c5c56-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/1/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id831c5c57-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-15-2-5> ;
  fro-usc:hasContentText "the company engaged in such activity continues to engage only in the same activities that such company conducted on September 30, 1999 , and other activities permissible under this chapter." ;
  fro-usc:hasId "id831c5c57-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/1/C" ;
  fro-usc:hasNumberText "(C)" ;
.
fro-usc-t12-ch17:id831c5c58-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-15-3> ;
  fro-usc:hasContentText "For purposes of this subsection, a company is predominantly engaged in financial activities if the annual gross revenues derived by the holding company and all subsidiaries of the holding company (excluding revenues derived from subsidiary depository institutions), on a consolidated basis, from engaging in activities that are financial in nature or are incidental to a financial activity under subsection (k) represent at least 85 percent of the consolidated annual gross revenues of the company." ;
  fro-usc:hasHeading "Predominantly financial" ;
  fro-usc:hasId "id831c5c58-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/2" ;
  fro-usc:hasNumberText "(2)" ;
.
fro-usc-t12-ch17:id831c5c59-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-15-4> ;
  fro-usc:hasContentText "A financial holding company that engages in activities or holds shares pursuant to this subsection, or a subsidiary of such financial holding company, may not acquire, in any merger, consolidation, or other type of business combination, assets of any other company that is engaged in any activity that the Board has not determined to be financial in nature or incidental to a financial activity under subsection (k), except this paragraph shall not apply with respect to a company that owns a broadcasting station licensed under title III of the Communications Act of 1934 [ 47 U.S.C. 301 et seq.] and the shares of which are under common control with an insurance company since January 1, 1998 , unless such company is acquired by, or otherwise becomes an affiliate of, a bank holding company that, at the time such acquisition or affiliation is consummated, is 1 of the 5 largest domestic bank holding companies (as determined on the basis of the consolidated total assets of such companies)." ;
  fro-usc:hasHeading "No expansion of grandfathered commercial activities through merger or consolidation" ;
  fro-usc:hasId "id831c5c59-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/3" ;
  fro-usc:hasNumberText "(3)" ;
  fro-usc:hasReferenceText "/us/usc/t47/s301" ;
.
fro-usc-t12-ch17:id831c5c5a-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-15-5> ;
  fro-usc:hasContentText "Notwithstanding any other provision of this subsection, a financial holding company may continue to engage in activities or hold shares in companies pursuant to this subsection only to the extent that the aggregate annual gross revenues derived from all such activities and all such companies does not exceed 15 percent of the consolidated annual gross revenues of the financial holding company (excluding revenues derived from subsidiary depository institutions)." ;
  fro-usc:hasHeading "Continuing revenue limitation on grandfathered commercial activities" ;
  fro-usc:hasId "id831c5c5a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/n/4" ;
  fro-usc:hasNumberText "(4)" ;
.
fro-usc-t12-ch17:id831c5c5b-1df7-11e6-87ee-8e4a3ab90f2a

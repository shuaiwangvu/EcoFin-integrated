  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-2-4> ;
  fro-usc:hasContentText "the holding company does not engage in any activities other than those of managing and controlling banks as a result of the reorganization;" ;
  fro-usc:hasId "id8317a106-1df7-11e6-87ee-8e4a3ab90f2a" ;

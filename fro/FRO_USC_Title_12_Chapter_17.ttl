.
fro-usc-t12-ch17:id831ea5f0-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5f1-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5f2-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-3> ;
  fro-usc:hasHeading "Antitrust review" ;
  fro-usc:hasId "id831ea5f0-1df7-11e6-87ee-8e4a3ab90f2a" ;

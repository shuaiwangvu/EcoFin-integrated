  fro-usc:hasContentText "commencing any new activity under subsection (k) or (n) of this section, section 24a(a) of this title , or section 46(a) of the Federal Deposit Insurance Act; or" ;
  fro-usc:hasId "id831c5c46-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/l/2/A" ;

  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-4> ;
  fro-usc:hasHeading "Acquisition of insolvent savings associations" ;
  fro-usc:hasId "id831c5bc5-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/3" ;

  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-2-7> ;
  fro-usc:hasContentText "the term “supervised securities holding company” means a securities holding company that is supervised by the Board of Governors under this section; and" ;
  fro-usc:hasId "id831ea618-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/a/5" ;
  fro-usc:hasNumberText "(5)" ;
.
fro-usc-t12-ch17:id831ea619-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-2-8> ;
  fro-usc:hasContentText "the terms “affiliate”, “bank”, “bank holding company”, “company”, “control”, “savings association”, and “subsidiary” have the same meanings as in section 2 of the Bank Holding Company Act of 1956 [ 12 U.S.C. 1841 ]." ;
  fro-usc:hasId "id831ea619-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/a/6" ;
  fro-usc:hasNumberText "(6)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1841" ;
.
fro-usc-t12-ch17:id831ea61a-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea61b-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea61c-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-3> ;
  fro-usc:hasHeading "Supervision of a securities holding company not having a bank or savings association affiliate" ;
  fro-usc:hasId "id831ea61a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/b" ;
  fro-usc:hasNumberText "(b)" ;
.
fro-usc-t12-ch17:id831ea61b-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-3-2> ;
  fro-usc:hasContentText "A securities holding company that is required by a foreign regulator or provision of foreign law to be subject to comprehensive consolidated supervision may register with the Board of Governors under paragraph (2) to become a supervised securities holding company. Any securities holding company filing such a registration shall be supervised in accordance with this section, and shall comply with the rules and orders prescribed by the Board of Governors applicable to supervised securities holding companies." ;
  fro-usc:hasHeading "In general" ;
  fro-usc:hasId "id831ea61b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/b/1" ;
  fro-usc:hasNumberText "(1)" ;
.
fro-usc-t12-ch17:id831ea61c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-3-3> ;
  fro-usc:hasHeading "Registration as a supervised securities holding company" ;
  fro-usc:hasId "id831ea61c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/b/2" ;
  fro-usc:hasNumberText "(2)" ;
  lkif-mereo:component fro-usc-t12-ch17:id831ea61d-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8321161e-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id831ea61d-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-3-3-2> ;
  fro-usc:hasContentText "A securities holding company that elects to be subject to comprehensive consolidated supervision shall register by filing with the Board of Governors such information and documents as the Board of Governors, by regulation, may prescribe as necessary or appropriate in furtherance of the purposes of this section." ;
  fro-usc:hasHeading "Registration" ;
  fro-usc:hasId "id831ea61d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/b/2/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id8321161e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-3-3-3> ;
  fro-usc:hasContentText "A securities holding company that registers under subparagraph (A) shall be deemed to be a supervised securities holding company, effective on the date that is 45 days after the date of receipt of the registration information and documents under subparagraph (A) by the Board of Governors, or within such shorter period as the Board of Governors, by rule or order, may determine." ;
  fro-usc:hasHeading "Effective date" ;
  fro-usc:hasId "id8321161e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/b/2/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id8321161f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id83211620-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id8321162b-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id8321162e-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4> ;
  fro-usc:hasHeading "Supervision of securities holding companies" ;
  fro-usc:hasId "id8321161f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c" ;
  fro-usc:hasNumberText "(c)" ;
.

  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-16-6-2-4> ;
  fro-usc:hasContentText "make recommendations regarding any modifications to the concentration limit that the Council determines would more effectively implement this section." ;
  fro-usc:hasId "id832116da-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1852/e/1/B" ;

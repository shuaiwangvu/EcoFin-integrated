  fro-usc:hasContentText "the Federal Deposit Insurance Corporation, with respect to the acquisition of a State savings association." ;
  fro-usc:hasId "id831c5bce-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/4/A/ii" ;

  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-11-3> ;
  fro-usc:hasHeading "General standards for review" ;
  fro-usc:hasId "id831c5be6-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/j/2" ;
  fro-usc:hasNumberText "(2)" ;
  lkif-mereo:component fro-usc-t12-ch17:id831c5be7-1df7-11e6-87ee-8e4a3ab90f2a ;

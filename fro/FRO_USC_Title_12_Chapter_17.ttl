.
fro-usc-t12-ch17:id8319ea99-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-6-4> ;
  fro-usc:hasContentText "immediately upon such registration, complies with all of the requirements of this chapter, and regulations prescribed by the Board pursuant to this chapter, including the nonbanking restrictions of this section; and" ;
  fro-usc:hasId "id8319ea99-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/5/B" ;

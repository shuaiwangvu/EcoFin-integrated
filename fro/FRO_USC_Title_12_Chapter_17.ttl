  fro-leg-ref:hasSequenceNumber 13 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-13> ;
  fro-usc:hasContentText "shares owned directly or indirectly by a company covered in 1970 in a company which does not engage in any activities other than those in which the bank holding company, or its subsidiaries, may engage by virtue of this section, but nothing in this paragraph authorizes any bank holding company, or subsidiary thereof, to acquire any interest in or the assets of any going concern (except pursuant to a binding written contract entered into before June 30, 1968 , or pursuant to another provision of this chapter) other than one which was a subsidiary on June 30, 1968 ;" ;
  fro-usc:hasId "id8319ea48-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/11" ;

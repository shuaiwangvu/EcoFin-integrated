  fro-usc:hasContentText "exercise discount or borrowing privileges pursuant to section 461(b)(7) of this title ." ;
  fro-usc:hasId "id832116ad-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/h/1/D/ii" ;

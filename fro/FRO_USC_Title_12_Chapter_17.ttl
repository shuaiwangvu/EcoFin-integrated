  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-8> ;
  fro-usc:hasId "id83211673-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/F" ;
  fro-usc:hasNumberText "(F)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-15-5-2-8-1> ;

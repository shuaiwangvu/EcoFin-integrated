  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-5> ;
  fro-usc:hasHeading "Exceptions to subparagraph (B)" ;
  fro-usc:hasId "id8319ea1a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/D" ;

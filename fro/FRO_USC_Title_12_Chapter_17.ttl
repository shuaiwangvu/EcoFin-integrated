  fro-usc:hasContentText "corrected the condition or ceased the activity that caused the company to fail to continue to qualify for the exemption; or" ;
  fro-usc:hasId "id8319ea94-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/4/A/i" ;

  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-5-3-4> ;
  fro-usc:hasHeading "Capital" ;
  fro-usc:hasId "id83211693-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/4/B/iii" ;
  fro-usc:hasNumberText "(iii)" ;
.
fro-usc-t12-ch17:id83211694-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-5-4> ;
  fro-usc:hasContentText "Upon an application by a banking entity, the Board may extend the period of time to meet the requirements under subparagraph (B)(ii)(I) for 2 additional years, if the Board finds that an extension would be consistent with safety and soundness and in the public interest." ;
  fro-usc:hasHeading "Extension" ;
  fro-usc:hasId "id83211694-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/4/C" ;
  fro-usc:hasNumberText "(C)" ;
.
fro-usc-t12-ch17:id83211695-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id83211696-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id83211697-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-6> ;
  fro-usc:hasHeading "Anti-evasion" ;
  fro-usc:hasId "id83211695-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/e" ;
  fro-usc:hasNumberText "(e)" ;
.
fro-usc-t12-ch17:id83211696-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-6-2> ;
  fro-usc:hasContentText "The appropriate Federal banking agencies, the Securities and Exchange Commission, and the Commodity Futures Trading Commission shall issue regulations, as part of the rulemaking provided for in subsection (b)(2), regarding internal controls and recordkeeping, in order to insure compliance with this section." ;
  fro-usc:hasHeading "Rulemaking" ;
  fro-usc:hasId "id83211696-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/e/1" ;
  fro-usc:hasNumberText "(1)" ;
.
fro-usc-t12-ch17:id83211697-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-6-3> ;
  fro-usc:hasContentText "Notwithstanding any other provision of law, whenever an appropriate Federal banking agency, the Securities and Exchange Commission, or the Commodity Futures Trading Commission, as appropriate, has reasonable cause to believe that a banking entity or nonbank financial company supervised by the Board under the respective agency’s jurisdiction has made an investment or engaged in an activity in a manner that functions as an evasion of the requirements of this section (including through an abuse of any permitted activity) or otherwise violates the restrictions under this section, the appropriate Federal banking agency, the Securities and Exchange Commission, or the Commodity Futures Trading Commission, as appropriate, shall order, after due notice and opportunity for hearing, the banking entity or nonbank financial company supervised by the Board to terminate the activity and, as relevant, dispose of the investment. Nothing in this paragraph shall be construed to limit the inherent authority of any Federal agency or State regulatory authority to further restrict any investments or activities under otherwise applicable provisions of law." ;
  fro-usc:hasHeading "Termination of activities or investment" ;
  fro-usc:hasId "id83211697-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/e/2" ;
  fro-usc:hasNumberText "(2)" ;
.

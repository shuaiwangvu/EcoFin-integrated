.
fro-usc-t12-ch17:id831c5c4c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-2-4> ;
  fro-usc:hasContentText "such financial holding company is not in compliance with the requirements of subsection ( l )(1);" ;
  fro-usc:hasId "id831c5c4c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/1/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id831c5c4d-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-3> ;
  fro-usc:hasContentText "Not later than 45 days after the date of receipt by a financial holding company of a notice given under paragraph (1) (or such additional period as the Board may permit), the financial holding company shall execute an agreement with the Board to comply with the requirements applicable to a financial holding company under subsection ( l )(1)." ;
  fro-usc:hasHeading "Agreement to correct conditions required" ;
  fro-usc:hasId "id831c5c4d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/2" ;
  fro-usc:hasNumberText "(2)" ;
.
fro-usc-t12-ch17:id831c5c4e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-4> ;
  fro-usc:hasContentText "Until the conditions described in a notice to a financial holding company under paragraph (1) are corrected, the Board may impose such limitations on the conduct or activities of that financial holding company or any affiliate of that company as the Board determines to be appropriate under the circumstances and consistent with the purposes of this chapter." ;
  fro-usc:hasHeading "Board may impose limitations" ;
  fro-usc:hasId "id831c5c4e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/3" ;
  fro-usc:hasNumberText "(3)" ;
.

.
fro-usc-t12-ch17:id8319ea1b-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-5-3> ;
  fro-usc:hasContentText "there is a limitation described in subparagraph (C) in a State statute, regulation, or order which has the effect of permitting a bank or bank holding company (including all insured depository institutions which are affiliates of the bank or bank holding company) to control a greater percentage of total deposits of all insured depository institutions in the State than the percentage permitted under subparagraph (B); or" ;
  fro-usc:hasId "id8319ea1b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/D/i" ;
  fro-usc:hasNumberText "(i)" ;
.
fro-usc-t12-ch17:id8319ea1c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-5-4> ;
  fro-usc:hasContentText "the acquisition is approved by the appropriate State bank supervisor of such State and the standard on which such approval is based does not have the effect of discriminating against out-of-State banks, out-of-State bank holding companies, or subsidiaries of such banks or holding companies." ;
  fro-usc:hasId "id8319ea1c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/D/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.

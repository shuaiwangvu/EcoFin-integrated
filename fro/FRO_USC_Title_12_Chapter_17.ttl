  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-4> ;
  fro-usc:hasHeading "Examination authority" ;
  fro-usc:hasId "id8321162e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/3" ;
  fro-usc:hasNumberText "(3)" ;
  lkif-mereo:component fro-usc-t12-ch17:id8321162f-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id83211630-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8321162f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-4-2> ;
  fro-usc:hasContentText "The Board of Governors may make examinations of any supervised securities holding company and any affiliate of a supervised securities holding company to carry out this subsection, to prevent evasions thereof, and to monitor compliance by the supervised securities holding company or affiliate with applicable provisions of law." ;
  fro-usc:hasHeading "Focus of examination authority" ;
  fro-usc:hasId "id8321162f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/3/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id83211630-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-4-3> ;
  fro-usc:hasContentText "For purposes of this subparagraph, the Board of Governors shall, to the fullest extent possible, use the reports of examination made by other appropriate Federal or State regulatory authorities with respect to any functionally regulated subsidiary or any institution described in subparagraph (D), (F), or (H) of section 2(c)(2) of the Bank Holding Company Act of 1956 ( 12 U.S.C. 1841(c)(2) )." ;
  fro-usc:hasHeading "Deference to other examinations" ;
  fro-usc:hasId "id83211630-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/3/B" ;
  fro-usc:hasNumberText "(B)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1841/c/2" ;
.

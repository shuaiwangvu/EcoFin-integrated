  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-14-2> ;
  fro-usc:hasContentText "within the applicable time limits prescribed in subsection (a)(2) of this section (i) ceases to be a bank holding company, or (ii) ceases to retain direct or indirect ownership or control of those shares and to engage in those activities not authorized under this section; and" ;
  fro-usc:hasId "id8319ea4a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/12/A" ;

fro-usc-t12-ch17:id8319ea15-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-3-2> ;
  fro-usc:hasContentText "The Board may not approve an application pursuant to paragraph (1)(A) if the applicant (including all insured depository institutions which are affiliates of the applicant) controls, or upon consummation of the acquisition for which such application is filed would control, more than 10 percent of the total amount of deposits of insured depository institutions in the United States." ;
  fro-usc:hasHeading "Nationwide concentration limits" ;
  fro-usc:hasId "id8319ea15-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/2/A" ;

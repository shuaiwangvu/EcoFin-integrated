  fro-usc:hasContentText "a report concerning the extent to which the supervised securities holding company or affiliate has complied with the provisions of this section and any regulations prescribed and orders issued under this section." ;
  fro-usc:hasId "id8321162a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/B/ii/IV" ;

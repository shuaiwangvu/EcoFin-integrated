  fro-usc:hasContentText "acquire or retain any equity, partnership, or other ownership interest in or sponsor a hedge fund or a private equity fund." ;
  fro-usc:hasId "id8321164a-1df7-11e6-87ee-8e4a3ab90f2a" ;

  fro-usc:hasContentText "complies with such other conditions as the Board may by regulation or order prescribe;" ;
  fro-usc:hasId "id8319ea4b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/12/B" ;
  fro-usc:hasNumberText "(B)" ;
.

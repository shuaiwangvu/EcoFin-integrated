  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-2-6> ;
  fro-usc:hasContentText "the holding company will not acquire control of any additional bank as a result of the reorganization.. 1" ;
  fro-usc:hasId "id8317a108-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/a/iv" ;

  fro-usc:hasContentText "a registered investment adviser, properly registered by or on behalf of either the Securities and Exchange Commission or any State, with respect to the investment advisory activities of such investment adviser and activities incidental to such investment advisory activities;" ;
  fro-usc:hasId "id831ea5a2-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/c/5/B/ii" ;

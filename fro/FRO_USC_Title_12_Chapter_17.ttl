.
fro-usc-t12-ch17:id8319ea43-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-8> ;
  fro-usc:hasContentText "shares of any company which do not include more than 5 per centum of the outstanding voting shares of such company;" ;
  fro-usc:hasId "id8319ea43-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/6" ;
  fro-usc:hasNumberText "(6)" ;
.
fro-usc-t12-ch17:id8319ea44-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-9> ;
  fro-usc:hasContentText "shares of an investment company which is not a bank holding company and which is not engaged in any business other than investing in securities, which securities do not include more than 5 per centum of the outstanding voting shares of any company;" ;
  fro-usc:hasId "id8319ea44-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/7" ;

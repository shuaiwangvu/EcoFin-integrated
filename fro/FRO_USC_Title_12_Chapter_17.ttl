  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-5-2-5-4> ;
  fro-usc:hasContentText "that State law was in effect as of September 29, 1994 ;" ;
  fro-usc:hasId "id8319ea11-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/1/D/ii" ;

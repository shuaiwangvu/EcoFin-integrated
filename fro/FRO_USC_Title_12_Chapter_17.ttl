fro-usc-t12-ch17:id831ea5ca-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5cb-1df7-11e6-87ee-8e4a3ab90f2a ;

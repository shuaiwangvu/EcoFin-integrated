  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-2-3-2> ;
  fro-usc:hasContentText "the bank holding company meets the capital and other financial standards prescribed by the Board by regulation for such a bank holding company; and" ;
  fro-usc:hasId "id8317a104-1df7-11e6-87ee-8e4a3ab90f2a" ;

  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-3-2-3> ;
  fro-usc:hasContentText "would involve or result in a material conflict of interest (as such term shall be defined by rule as provided in subsection (b)(2)) between the banking entity and its clients, customers, or counterparties;" ;
  fro-usc:hasId "id83211684-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/2/A/i" ;

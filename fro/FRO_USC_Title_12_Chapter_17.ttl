  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-10-4> ;
  fro-usc:hasContentText "subject to the restrictions of section 106 of the Bank Holding Company Act Amendments of 1970 [ 12 U.S.C. 1971 et seq.], in connection with any transaction involving the products or services of such company or affiliate and those of a bank affiliate, as if such company or affiliate were a bank and such bank were a subsidiary of a bank holding company." ;
  fro-usc:hasId "id8319eaa3-1df7-11e6-87ee-8e4a3ab90f2a" ;

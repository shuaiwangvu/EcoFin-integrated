  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-5-3> ;
  fro-usc:hasContentText "In imposing standards under this subsection, the Board of Governors may differentiate among supervised securities holding companies on an individual basis, or by category, taking into consideration the requirements under paragraph (3)." ;
  fro-usc:hasHeading "Differentiation" ;
  fro-usc:hasId "id83211633-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/d/2" ;

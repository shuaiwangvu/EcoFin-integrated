  lkif-mereo:component fro-usc-t12-ch17:id831c5bd2-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id831c5bd1-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-6-2> ;
  fro-usc:hasContentText "The Board shall consult with the Comptroller of the Currency or the Federal Deposit Insurance Corporation, as appropriate, in establishing the scope of an examination by the Board of a bank holding company that directly or indirectly controls a savings association." ;
  fro-usc:hasHeading "Scope" ;
  fro-usc:hasId "id831c5bd1-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/5/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id831c5bd2-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-6-3> ;
  fro-usc:hasContentText "Upon the request of the Comptroller of the Currency or the Federal Deposit Insurance Corporation, the Board shall furnish the Comptroller of the Currency or the Federal Deposit Insurance Corporation, as applicable, with a copy of any inspection report, additional examination materials, or supervisory information relating to any bank holding company that directly or indirectly controls a savings association." ;
  fro-usc:hasHeading "Access to inspection reports" ;
  fro-usc:hasId "id831c5bd2-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/5/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id831c5bd3-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-7> ;
  fro-usc:hasContentText "The Board and the Comptroller of the Currency or the Federal Deposit Insurance Corporation, as applicable, shall cooperate in any enforcement action against any bank holding company that controls a savings association, if the relevant conduct involves such association." ;
  fro-usc:hasHeading "Coordination of enforcement efforts" ;
  fro-usc:hasId "id831c5bd3-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/6" ;
  fro-usc:hasNumberText "(6)" ;
.

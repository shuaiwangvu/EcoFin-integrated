  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-2-3> ;
  fro-usc:hasContentText "a financial holding company is engaged, directly or indirectly, in any activity under subsection (k), (n), or ( o ), other than activities that are permissible for a bank holding company under subsection (c)(8); and" ;
  fro-usc:hasId "id831c5c4b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/1/A" ;

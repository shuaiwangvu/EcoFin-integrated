  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-11-2-3> ;
  fro-usc:hasContentText "The notice submitted to the Board shall contain such information as the Board shall prescribe by regulation or by specific request in connection with a particular notice." ;

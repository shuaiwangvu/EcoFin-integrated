  fro-usc:hasContentText "a balance sheet or income statement of the supervised securities holding company or an affiliate of a supervised securities holding company;" ;
  fro-usc:hasId "id83211627-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/B/ii/I" ;
  fro-usc:hasNumberText "(I)" ;
.
fro-usc-t12-ch17:id83211628-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-2-3-3-4> ;
  fro-usc:hasContentText "an assessment of the consolidated capital and liquidity of the supervised securities holding company;" ;
  fro-usc:hasId "id83211628-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/B/ii/II" ;
  fro-usc:hasNumberText "(II)" ;
.
fro-usc-t12-ch17:id83211629-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-2-3-3-5> ;
  fro-usc:hasContentText "a report by an independent auditor attesting to the compliance of the supervised securities holding company with the internal risk management and internal control objectives of the supervised securities holding company; and" ;
  fro-usc:hasId "id83211629-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/B/ii/III" ;
  fro-usc:hasNumberText "(III)" ;
.
fro-usc-t12-ch17:id8321162a-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 6 ;

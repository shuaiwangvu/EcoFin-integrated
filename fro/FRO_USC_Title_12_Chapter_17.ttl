  fro-usc:hasContentText "limit activities that have caused undue risk or loss in banking entities and nonbank financial companies supervised by the Board, or that might reasonably be expected to create undue risk or loss in such banking entities and nonbank financial companies supervised by the Board;" ;
  fro-usc:hasId "id83211652-1df7-11e6-87ee-8e4a3ab90f2a" ;

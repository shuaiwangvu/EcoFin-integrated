  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-9-2-3> ;
  fro-usc:hasContentText "the home State of such insured depository institution is a State other than the home State of the bank holding company; and" ;
  fro-usc:hasId "id831c5bd7-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/8/A/i" ;

  fro-usc:hasContentText "was chartered or organized as a savings association before June 1, 1991 ;" ;
  fro-usc:hasId "id831c5bc8-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/3/B/i" ;

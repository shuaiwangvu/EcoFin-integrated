  fro-usc:hasContentText "the Federal Deposit Insurance Corporation has not determined that compliance with such State law would result in an unacceptable risk to the Deposit Insurance Fund; and" ;
  fro-usc:hasId "id8319ea12-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/d/1/D/iii" ;

  fro-usc:hasContentText "the control of which is not acquired by any company after August 10, 1987 ; or" ;
  fro-usc:hasId "id8317a0a1-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1841/c/2/H/i/III" ;

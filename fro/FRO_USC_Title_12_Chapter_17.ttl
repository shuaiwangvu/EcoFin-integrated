  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-2-3> ;
  fro-usc:hasContentText "after May 9, 1956 , acquire direct or indirect ownership or control of any voting shares of any company which is not a bank, or" ;
  fro-usc:hasId "id8319ea3a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/a/1" ;

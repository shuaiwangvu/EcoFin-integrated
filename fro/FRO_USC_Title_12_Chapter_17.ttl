fro-usc-t12-ch17:id831c5bc3-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-2> ;
  fro-usc:hasContentText "The Board may approve an application by any bank holding company under subsection (c)(8) to acquire any savings association in accordance with the requirements and limitations of this section." ;
  fro-usc:hasHeading "In general" ;
  fro-usc:hasId "id831c5bc3-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/1" ;

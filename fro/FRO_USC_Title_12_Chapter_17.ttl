  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-15-5-2-9> ;
  fro-usc:hasId "id83211676-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1851/d/1/G" ;
  fro-usc:hasNumberText "(G)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-15-5-2-9-1> ;

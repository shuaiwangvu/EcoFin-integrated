fro-usc-t12-ch17:id8319ea69-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-8-2-3> ;
  fro-usc:hasContentText "the operations of such company during the 2-year period beginning on the date such company commences operations shall not be taken into account in making any such determination; and" ;
  fro-usc:hasId "id8319ea69-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/G/i/I" ;
  fro-usc:hasNumberText "(I)" ;
.
fro-usc-t12-ch17:id8319ea6a-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-8-2-4> ;
  fro-usc:hasContentText "not less than 4 consecutive years of operations of such company (not including any portion of the period referred to in subclause (I)) shall be taken into account in making any such determination." ;
  fro-usc:hasId "id8319ea6a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/G/i/II" ;
  fro-usc:hasNumberText "(II)" ;
.
fro-usc-t12-ch17:id8319ea6b-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-8-3> ;
  fro-usc:hasHeading ".—" ;
  fro-usc:hasId "id8319ea6b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/G/ii" ;
  fro-usc:hasNumberText "(ii)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-5-4-16-8-3-2> ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea6c-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea6d-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8319ea6c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-8-3-3> ;
  fro-usc:hasContentText "the revenues of such company from the export, or facilitating the export, of goods or services produced in the United States exceed the revenues of such company from the import, or facilitating the import, into the United States of goods or services produced outside the United States; and" ;
  fro-usc:hasId "id8319ea6c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/G/ii/I" ;
  fro-usc:hasNumberText "(I)" ;
.
fro-usc-t12-ch17:id8319ea6d-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-8-3-4> ;
  fro-usc:hasContentText "at least ⅓ of such company’s total revenues are revenues from the export, or facilitating the export, of goods or services produced in the United States by persons not affiliated with such company." ;
  fro-usc:hasId "id8319ea6d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/G/ii/II" ;
  fro-usc:hasNumberText "(II)" ;
.
fro-usc-t12-ch17:id8319ea6e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-9> ;
  fro-usc:hasHeading "Inventory.—" ;
  fro-usc:hasId "id8319ea6e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/H" ;
  fro-usc:hasNumberText "(H)" ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea6f-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea70-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8319ea6f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-9-2> ;
  fro-usc:hasContentText "The Board may not prescribe by regulation any maximum dollar amount limitation on the value of goods which an export trading company may maintain in inventory at any time." ;
  fro-usc:hasHeading ".—" ;
  fro-usc:hasId "id8319ea6f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/H/i" ;
  fro-usc:hasNumberText "(i)" ;
.
fro-usc-t12-ch17:id8319ea70-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-16-9-3> ;
  fro-usc:hasContentText "Notwithstanding clause (i), the Board may issue an order establishing a maximum dollar amount limitation on the value of goods which a particular export trading company may maintain in inventory at any time (after such company has been operating for a reasonable period of time) if the Board finds that, under the facts and circumstances, such limitation is necessary to prevent risks that would affect the financial or managerial resources of an investor bank holding company to an extent which would be likely to have a materially adverse effect on the safety and soundness of any subsidiary bank of such bank holding company." ;
  fro-usc:hasHeading ".—" ;
  fro-usc:hasId "id8319ea70-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/14/H/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.
fro-usc-t12-ch17:id8319ea71-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-5> ;
  fro-usc:hasContentText "To the extent that such action would not be substantially at variance with the purposes of this chapter and subject to such conditions as it considers necessary to protect the public interest, the Board by order, after opportunity for hearing, may grant exemptions from the provisions of this section to any bank holding company which controlled one bank prior to July 1, 1968 , and has not thereafter acquired the control of any other bank in order (1) to avoid disrupting business relationships that have existed over a long period of years without adversely affecting the banks or communities involved, or (2) to avoid forced sales of small locally owned banks to purchasers not similarly representative of community interests, or (3) to allow retention of banks that are so small in relation to the holding company’s total interests and so small in relation to the banking market to be served as to minimize the likelihood that the bank’s powers to grant or deny credit may be influenced by a desire to further the holding company’s other interests." ;
  fro-usc:hasHeading "Exemption of company controlling one bank prior to" ;
  fro-usc:hasId "id8319ea71-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/d" ;
  fro-usc:hasNumberText "(d)" ;
.
fro-usc-t12-ch17:id8319ea72-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-6> ;
  fro-usc:hasContentText "With respect to shares which were not subject to the prohibitions of this section as originally enacted by reason of any exemption with respect thereto but which were made subject to such prohibitions by the subsequent repeal of such exemption, no bank holding company shall retain direct or indirect ownership or control of such shares after five years from the date of the repeal of such exemption, except as provided in paragraph (2) of subsection (a). Any bank holding company subject to such five-year limitation on the retention of nonbanking assets shall endeavor to divest itself of such shares promptly and such bank holding company shall report its progress in such divestiture to the Board two years after repeal of the exemption applicable to it and annually thereafter." ;
  fro-usc:hasHeading "Divestiture of nonexempt shares" ;
  fro-usc:hasId "id8319ea72-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/e" ;
  fro-usc:hasNumberText "(e)" ;
.

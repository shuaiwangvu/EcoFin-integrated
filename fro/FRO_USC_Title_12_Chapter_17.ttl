  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-5> ;
  fro-usc:hasHeading "Failure to correct" ;
  fro-usc:hasId "id831c5c4f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/4" ;
  fro-usc:hasNumberText "(4)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-5-14-5-2> ;

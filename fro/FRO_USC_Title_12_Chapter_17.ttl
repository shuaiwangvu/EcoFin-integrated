  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-6-6-2> ;
  fro-usc:hasId "id831ea5a8-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/e/1" ;
  fro-usc:hasNumberText "(1)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-6-6-2-1> ;
  fro-usc:refers_toContinuation <fro-usc-t12-ch17:r-1-0-4-6-6-2-4> ;
  lkif-mereo:component fro-usc-t12-ch17:id831ea5a9-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id831ea5aa-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id831ea5a9-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-6-6-2-2> ;
  fro-usc:hasContentText "order the bank holding company or any such nonbank subsidiaries, after due notice and opportunity for hearing, and after considering the views of the bank’s primary supervisor, which shall be the Comptroller of the Currency in the case of a national bank or the Federal Deposit Insurance Corporation and the appropriate State supervisory authority in the case of an insured nonmember bank, to terminate such activities or to terminate (within one hundred and twenty days or such longer period as the Board may direct in unusual circumstances) its ownership or control of any such subsidiary either by sale or by distribution of the shares of the subsidiary to the shareholders of the bank holding company; or" ;
  fro-usc:hasId "id831ea5a9-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/e/1/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id831ea5aa-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-6-6-2-3> ;
  fro-usc:hasContentText "order the bank holding company, after due notice and opportunity for hearing, and after consultation with the primary supervisor for the bank, which shall be the Comptroller of the Currency in the case of a national bank, and the Federal Deposit Insurance Corporation and the appropriate State supervisor in the case of an insured nonmember bank, to terminate (within 120 days or such longer period as the Board may direct) the ownership or control of any such bank by such company." ;
  fro-usc:hasId "id831ea5aa-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1844/e/1/B" ;

  fro-usc:hasHeading "Use of existing reports" ;
  fro-usc:hasId "id8321162b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/2" ;
  fro-usc:hasNumberText "(2)" ;
  lkif-mereo:component fro-usc-t12-ch17:id8321162c-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8321162d-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8321162c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-3-2> ;
  fro-usc:hasContentText "The Board of Governors shall, to the fullest extent possible, accept reports in fulfillment of the requirements of this paragraph that a supervised securities holding company or an affiliate of a supervised securities holding company has been required to provide to another regulatory agency or a self-regulatory organization." ;
  fro-usc:hasHeading "In general" ;
  fro-usc:hasId "id8321162c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/2/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id8321162d-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-3-3> ;
  fro-usc:hasContentText "A supervised securities holding company or an affiliate of a supervised securities holding company shall promptly provide to the Board of Governors, at the request of the Board of Governors, any report described in subparagraph (A), as permitted by law." ;
  fro-usc:hasHeading "Availability" ;
  fro-usc:hasId "id8321162d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/2/B" ;
  fro-usc:hasNumberText "(B)" ;
.

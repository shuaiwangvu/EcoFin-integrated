.
fro-usc-t12-ch17:id831ea5f6-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-3-3-3> ;
  fro-usc:hasContentText "Upon receipt of notice from the Federal Deposit Insurance Corporation under subparagraph (A) or at such earlier time as deemed appropriate by the Board, the Board shall immediately notify the Attorney General of the United States of the facts concerning the possible acquisition." ;
  fro-usc:hasId "id831ea5f6-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/b/2/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id831ea5f7-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-3-3-4> ;
  fro-usc:hasContentText "Within 5 days of receiving notice under subparagraph (B), the Attorney General shall notify the Board in writing of the Attorney General’s preliminary finding as to the consistency of the possible acquisition with the antitrust laws." ;
  fro-usc:hasId "id831ea5f7-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/b/2/C" ;
  fro-usc:hasNumberText "(C)" ;
.
fro-usc-t12-ch17:id831ea5f8-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-3-3-5> ;
  fro-usc:hasContentText "The Board may reduce or eliminate the post-approval waiting period established under paragraph (1) for an acquisition to which this paragraph applies, except that such period may not be eliminated or reduced to less than 5 days without the concurrence of the Attorney General." ;
  fro-usc:hasId "id831ea5f8-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/b/2/D" ;
  fro-usc:hasNumberText "(D)" ;
.
fro-usc-t12-ch17:id831ea5f9-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-4> ;
  fro-usc:hasContentText "In any action brought under the antitrust laws arising out of any acquisition, merger, or consolidation transaction approved by the Board under section 1842 of this title , the Board and any State banking supervisory agency having jurisdiction within the State involved, may appear as a party of its own motion and as of right, and be represented by its counsel." ;
  fro-usc:hasHeading "Antitrust proceedings; Board and State banking agency as party; representation by counsel" ;
  fro-usc:hasId "id831ea5f9-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/c" ;
  fro-usc:hasNumberText "(c)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1842" ;
.
fro-usc-t12-ch17:id831ea5fa-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-5> ;
  fro-usc:hasContentText "Any acquisition, merger, or consolidation of the kind described in section 1842(a) of this title which was consummated at any time prior or subsequent to May 9, 1956 , and as to which no litigation was initiated by the Attorney General prior to July 1, 1966 , shall be conclusively presumed not to have been in violation of any antitrust laws other than section 2 of title 15 ." ;
  fro-usc:hasHeading ", and not in litigation prior to" ;
  fro-usc:hasHeading "Treatment of merger transactions consummated prior or subsequent to" ;
  fro-usc:hasId "id831ea5fa-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/d" ;
  fro-usc:hasNumberText "(d)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1842/a" ;
  fro-usc:hasReferenceText "/us/usc/t15/s2" ;
.
fro-usc-t12-ch17:id831ea5fb-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-6> ;
  fro-usc:hasContentText "Any court having pending before it on or after July 1, 1966 , any litigation initiated under the antitrust laws by the Attorney General with respect to any acquisition, merger, or consolidation of the kind described in section 1842(a) of this title shall apply the substantive rule of law set forth in section 1842 of this title ." ;
  fro-usc:hasHeading ", with respect to merger transactions" ;
  fro-usc:hasHeading "Antitrust litigation; substantive law applicable to proceedings pending on or after" ;
  fro-usc:hasId "id831ea5fb-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/e" ;
  fro-usc:hasNumberText "(e)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1842" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1842/a" ;
.
fro-usc-t12-ch17:id831ea5fc-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-7> ;
  fro-usc:hasContentText "For the purposes of this section, the term “antitrust laws” means the Act of July 2, 1890 (the Sherman Antitrust Act), the Act of October 15, 1914 (the Clayton Act), and any other Acts in pari materia." ;
  fro-usc:hasHeading "“Antitrust laws” defined" ;
  fro-usc:hasId "id831ea5fc-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1849/f" ;
  fro-usc:hasNumberText "(f)" ;
.

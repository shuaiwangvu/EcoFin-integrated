  fro-usc:hasContentText "submitted a plan to the Board for approval to cease the activity or correct the condition in a timely manner (which shall not exceed 1 year); and" ;
  fro-usc:hasId "id8319ea95-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/4/A/ii" ;

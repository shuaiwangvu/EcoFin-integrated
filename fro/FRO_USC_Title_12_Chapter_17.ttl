  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id8321163e-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id8321163f-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-6> ;
  fro-usc:hasHeading "Other provisions of law applicable to supervised securities holding companies" ;
  fro-usc:hasId "id8321163d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/e" ;
  fro-usc:hasNumberText "(e)" ;
.
fro-usc-t12-ch17:id8321163e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-6-2> ;
  fro-usc:hasContentText "Subsections (b), (c) through (s), and (u) of section 1818 of this title shall apply to any supervised securities holding company, and to any subsidiary (other than a bank or an institution described in subparagraph (D), (F), or (H) of section 2(c)(2) of the Bank Holding Company Act of 1956 ( 12 U.S.C. 1841(c)(2) )) of a supervised securities holding company, in the same manner as such subsections apply to a bank holding company for which the Board of Governors is the appropriate Federal banking agency. For purposes of applying such subsections to a supervised securities holding company or a subsidiary (other than a bank or an institution described in subparagraph (D), (F), or (H) of section 2(c)(2) of the Bank Holding Company Act of 1956 ( 12 U.S.C. 1841(c)(2) )) of a supervised securities holding company, the Board of Governors shall be deemed the appropriate Federal banking agency for the supervised securities holding company or subsidiary." ;
  fro-usc:hasHeading "Federal Deposit Insurance Act" ;
  fro-usc:hasId "id8321163e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/e/1" ;
  fro-usc:hasNumberText "(1)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1818" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1841/c/2" ;
.
fro-usc-t12-ch17:id8321163f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-6-3> ;
  fro-usc:hasContentText "Except as the Board of Governors may otherwise provide by regulation or order, a supervised securities holding company shall be subject to the provisions of the Bank Holding Company Act of 1956 ( 12 U.S.C. 1841 et seq.) in the same manner and to the same extent a bank holding company is subject to such provisions, except that a supervised securities holding company may not, by reason of this paragraph, be deemed to be a bank holding company for purposes of section 4 of the Bank Holding Company Act of 1956 ( 12 U.S.C. 1843 )." ;
  fro-usc:hasHeading "Bank Holding Company Act of 1956" ;
  fro-usc:hasId "id8321163f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/e/2" ;
  fro-usc:hasNumberText "(2)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1841" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1843" ;
.

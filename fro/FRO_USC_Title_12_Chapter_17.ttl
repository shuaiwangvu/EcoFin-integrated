.
fro-usc-t12-ch17:id831c5bd8-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Clause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-9-2-4> ;
  fro-usc:hasContentText "the applicant (including all insured depository institutions which are affiliates of the applicant) controls, or upon consummation of the transaction would control, more than 10 percent of the total amount of deposits of insured depository institutions in the United States." ;
  fro-usc:hasId "id831c5bd8-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/i/8/A/ii" ;
  fro-usc:hasNumberText "(ii)" ;
.
fro-usc-t12-ch17:id831c5bd9-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-10-9-3> ;
  fro-usc:hasContentText "Subparagraph (A) shall not apply to an acquisition that involves an insured depository institution in default or in danger of default, or with respect to which the Federal Deposit Insurance Corporation provides assistance under section 13 of the Federal Deposit Insurance Act ( 12 U.S.C. 1823 )." ;
  fro-usc:hasHeading "Exception" ;
  fro-usc:hasId "id831c5bd9-1df7-11e6-87ee-8e4a3ab90f2a" ;

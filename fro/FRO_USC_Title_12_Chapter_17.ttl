  fro-usc:hasHeading "Competitive factors" ;
  fro-usc:hasId "id8319e9fd-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/1" ;
  fro-usc:hasNumberText "(1)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-4-4-2-2> ;
  lkif-mereo:component fro-usc-t12-ch17:id8319e9fe-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8319e9ff-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8319e9fe-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-2-3> ;
  fro-usc:hasContentText "any acquisition or merger or consolidation under this section which would result in a monopoly, or which would be in furtherance of any combination or conspiracy to monopolize or to attempt to monopolize the business of banking in any part of the United States, or" ;
  fro-usc:hasId "id8319e9fe-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/1/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id8319e9ff-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-2-4> ;
  fro-usc:hasContentText "any other proposed acquisition or merger or consolidation under this section whose effect in any section of the country may be substantially to lessen competition, or to tend to create a monopoly, or which in any other manner would be in restraint or  4 trade, unless it finds that the anticompetitive effects of the proposed transaction are clearly outweighed in the public interest by the probable effect of the transaction in meeting the convenience and needs of the community to be served." ;
  fro-usc:hasId "id8319e9ff-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/1/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id8319ea00-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-3> ;
  fro-usc:hasContentText "In every case, the Board shall take into consideration the financial and managerial resources and future prospects of the company or companies and the banks concerned, and the convenience and needs of the community to be served." ;
  fro-usc:hasHeading "Banking and community factors" ;
  fro-usc:hasId "id8319ea00-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/2" ;
  fro-usc:hasNumberText "(2)" ;
.
fro-usc-t12-ch17:id8319ea01-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-4> ;
  fro-usc:hasHeading "Supervisory factors" ;
  fro-usc:hasId "id8319ea01-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/3" ;
  fro-usc:hasNumberText "(3)" ;
  fro-usc:refers_toChapeau <fro-usc-t12-ch17:r-1-0-4-4-4-4-2> ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea02-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea03-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8319ea02-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-4-3> ;
  fro-usc:hasContentText "the company fails to provide the Board with adequate assurances that the company will make available to the Board such information on the operations or activities of the company, and any affiliate of the company, as the Board determines to be appropriate to determine and enforce compliance with this chapter; or" ;
  fro-usc:hasId "id8319ea02-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/3/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id8319ea03-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-4-4> ;
  fro-usc:hasContentText "in the case of an application involving a foreign bank, the foreign bank is not subject to comprehensive supervision or regulation on a consolidated basis by the appropriate authorities in the bank’s home country." ;
  fro-usc:hasId "id8319ea03-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/3/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id8319ea04-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-5> ;
  fro-usc:hasContentText "Notwithstanding any other provision of law, the Board shall not follow any practice or policy in the consideration of any application for the formation of a one-bank holding company if following such practice or policy would result in the rejection of such application solely because the transaction to form such one-bank holding company involves a bank stock loan which is for a period of not more than twenty-five years. The previous sentence shall not be construed to prohibit the Board from rejecting any application solely because the other financial arrangements are considered unsatisfactory. The Board shall consider transactions involving bank stock loans for the formation of a one-bank holding company having a maturity of twelve years or more on a case by case basis and no such transaction shall be approved if the Board believes the safety or soundness of the bank may be jeopardized." ;
  fro-usc:hasHeading "Treatment of certain bank stock loans" ;
  fro-usc:hasId "id8319ea04-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/4" ;
  fro-usc:hasNumberText "(4)" ;
.
fro-usc-t12-ch17:id8319ea05-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-6> ;
  fro-usc:hasContentText "Consideration of the managerial resources of a company or bank under paragraph (2) shall include consideration of the competence, experience, and integrity of the officers, directors, and principal shareholders of the company or bank." ;
  fro-usc:hasHeading "Managerial resources" ;
  fro-usc:hasId "id8319ea05-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/5" ;
  fro-usc:hasNumberText "(5)" ;
.
fro-usc-t12-ch17:id8319ea06-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-7> ;
  fro-usc:hasContentText "In every case, the Board shall take into consideration the effectiveness of the company or companies in combatting money laundering activities, including in overseas branches." ;
  fro-usc:hasHeading "Money laundering" ;
  fro-usc:hasId "id8319ea06-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/6" ;
  fro-usc:hasNumberText "(6)" ;
.
fro-usc-t12-ch17:id8319ea07-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-4-4-8> ;
  fro-usc:hasContentText "In every case, the Board shall take into consideration the extent to which a proposed acquisition, merger, or consolidation would result in greater or more concentrated risks to the stability of the United States banking or financial system." ;
  fro-usc:hasHeading "Financial stability" ;
  fro-usc:hasId "id8319ea07-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1842/c/7" ;
  fro-usc:hasNumberText "(7)" ;
.

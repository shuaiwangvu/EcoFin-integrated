.
fro-usc-t12-ch17:id8319ea9a-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-6-5> ;
  fro-usc:hasContentText "does not, at the time of such registration, control banks in more than one State, the acquisition of which would be prohibited by section 1842(d) of this title if an application for such acquisition by such company were filed under section 1842(a) of this title ." ;
  fro-usc:hasId "id8319ea9a-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/5/C" ;
  fro-usc:hasNumberText "(C)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1842/a" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1842/d" ;
.
fro-usc-t12-ch17:id8319ea9b-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-7> ;
  fro-usc:hasContentText "Each company described in paragraph (1) shall, within 60 days after August 10, 1987 , provide the Board with the name and address of such company, the name and address of each bank such company controls, and a description of each such bank’s activities." ;
  fro-usc:hasHeading "Information requirement" ;
  fro-usc:hasId "id8319ea9b-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/6" ;
  fro-usc:hasNumberText "(6)" ;
.
fro-usc-t12-ch17:id8319ea9c-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 8 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-8> ;
  fro-usc:hasContentText "The Board may, from time to time, examine a company described in paragraph (1), or a bank controlled by such company, or require reports under oath from appropriate officers or directors of such company or bank solely for purposes of assuring compliance with the provisions of this subsection and enforcing such compliance." ;
  fro-usc:hasHeading "Examination" ;
  fro-usc:hasId "id8319ea9c-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/7" ;
  fro-usc:hasNumberText "(7)" ;
.
fro-usc-t12-ch17:id8319ea9d-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 9 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-9> ;
  fro-usc:hasHeading "Enforcement" ;
  fro-usc:hasId "id8319ea9d-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/8" ;
  fro-usc:hasNumberText "(8)" ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea9e-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8319ea9f-1df7-11e6-87ee-8e4a3ab90f2a ;
  lkif-mereo:component fro-usc-t12-ch17:id8319eaa0-1df7-11e6-87ee-8e4a3ab90f2a ;
.
fro-usc-t12-ch17:id8319ea9e-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-9-2> ;
  fro-usc:hasContentText "In addition to any other power of the Board, the Board may enforce compliance with the provisions of this chapter which are applicable to any company described in paragraph (1), and any bank controlled by such company, under section 8 of the Federal Deposit Insurance Act [ 12 U.S.C. 1818 ] and such company or bank shall be subject to such section (for such purposes) in the same manner and to the same extent as if such company or bank were a State member insured bank." ;
  fro-usc:hasHeading "In general" ;
  fro-usc:hasId "id8319ea9e-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/8/A" ;
  fro-usc:hasNumberText "(A)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1818" ;
.
fro-usc-t12-ch17:id8319ea9f-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-9-3> ;
  fro-usc:hasContentText "Any violation of this chapter by any company described in paragraph (1), and any bank controlled by such company, may also be treated as a violation of the Federal Deposit Insurance Act [ 12 U.S.C. 1811 et seq.] for purposes of subparagraph (A)." ;
  fro-usc:hasHeading "Application of other act" ;
  fro-usc:hasId "id8319ea9f-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/8/B" ;
  fro-usc:hasNumberText "(B)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1811" ;
.
fro-usc-t12-ch17:id8319eaa0-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-9-4> ;
  fro-usc:hasContentText "No provision of this paragraph shall be construed as limiting any authority of the Comptroller of the Currency or the Federal Deposit Insurance Corporation." ;
  fro-usc:hasHeading "No effect on other authority" ;
  fro-usc:hasId "id8319eaa0-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/f/8/C" ;

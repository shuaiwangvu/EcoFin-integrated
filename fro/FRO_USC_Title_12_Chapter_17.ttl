fro-usc-t12-ch17:id831c5bc1-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-9-3> ;
  fro-usc:hasContentText "A company that controls an institution described in subparagraph (D), (F), (G), or (H) of section 1841(c)(2) of this title and any of such company’s other affiliates, shall be subject to the tying restrictions of section 106 of the Bank Holding Company Act Amendments of 1970 [ 12 U.S.C. 1971 et seq.] in connection with any transaction involving the products or services of such company or affiliate and those of such institution, as if such company or affiliate were a bank and such institution were a subsidiary of a bank holding company." ;
  fro-usc:hasHeading "Applicable with respect to certain transactions" ;
  fro-usc:hasId "id831c5bc1-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/h/2" ;
  fro-usc:hasNumberText "(2)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1841/c/2" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1971" ;
.

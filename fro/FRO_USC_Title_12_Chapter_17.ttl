  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-4-12> ;
  fro-usc:hasContentText "shares lawfully acquired and owned prior to May 9, 1956 , by a bank which is a bank holding company, or by any of its wholly owned subsidiaries;" ;
  fro-usc:hasId "id8319ea47-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/c/10" ;
  fro-usc:hasNumberText "(10)" ;
.
fro-usc-t12-ch17:id8319ea48-1df7-11e6-87ee-8e4a3ab90f2a

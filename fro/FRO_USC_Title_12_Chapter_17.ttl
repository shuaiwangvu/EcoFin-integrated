  fro-leg-ref:divided_by fro-usc-t12-ch17:id8317a076-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id8317a082-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-3-2> ;
  fro-usc:hasId "id8317a06e-1df7-11e6-87ee-8e4a3ab90f2a" ;

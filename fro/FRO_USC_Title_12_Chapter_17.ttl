  fro-usc:hasContentText "be prepared in such form and according to such specifications (including certification by a registered public accounting firm), as the Board of Governors may require; and" ;
  fro-usc:hasId "id83211624-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/B/i/I" ;
  fro-usc:hasNumberText "(I)" ;
.
fro-usc-t12-ch17:id83211625-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subclause ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-14-4-2-3-2-4> ;
  fro-usc:hasContentText "be provided promptly to the Board of Governors at any time, upon request by the Board of Governors." ;
  fro-usc:hasId "id83211625-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1850a/c/1/B/i/II" ;
  fro-usc:hasNumberText "(II)" ;
.

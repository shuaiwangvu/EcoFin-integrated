  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-7-4-4-3> ;
  fro-usc:hasContentText "is fully secured, as required by the Board, by bonds, notes, or other obligations that are direct obligations of the United States or on which the principal and interest are fully guaranteed by the United States or by securities and obligations eligible for settlement on the Federal Reserve book entry system; or" ;
  fro-usc:hasId "id8319ea8e-1df7-11e6-87ee-8e4a3ab90f2a" ;

  fro-usc:hasHeading "Reservation of rights to States" ;
  fro-usc:hasId "id831ea5c2-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1846" ;
  fro-usc:hasNumberText "§ 1846." ;

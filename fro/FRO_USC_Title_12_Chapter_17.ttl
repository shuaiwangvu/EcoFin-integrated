  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-5-3> ;
  fro-usc:hasContentText "to divest control of any subsidiary depository institution; or" ;
  fro-usc:hasId "id831c5c50-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/4/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id831c5c51-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-5-4> ;
  fro-usc:hasContentText "at the election of the financial holding company instead to cease to engage in any activity conducted by such financial holding company or its subsidiaries (other than a depository institution or a subsidiary of a depository institution) that is not an activity that is permissible for a bank holding company under subsection (c)(8)." ;
  fro-usc:hasId "id831c5c51-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/4/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id831c5c52-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-14-6> ;
  fro-usc:hasContentText "In taking any action under this subsection, the Board shall consult with all relevant Federal and State regulatory agencies and authorities." ;
  fro-usc:hasHeading "Consultation" ;
  fro-usc:hasId "id831c5c52-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/m/5" ;
  fro-usc:hasNumberText "(5)" ;
.

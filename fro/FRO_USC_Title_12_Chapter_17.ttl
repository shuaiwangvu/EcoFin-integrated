fro-usc-t12-ch17:id831ea5ef-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-12-2> ;
  fro-usc:hasContentText "Nothing herein contained shall be interpreted or construed as approving any act, action, or conduct which is or has been or may be in violation of existing law, nor shall anything herein contained constitute a defense to any action, suit, or proceeding pending or hereafter instituted on account of any prohibited antitrust or monopolistic act, action, or conduct, except as specifically provided in this section." ;
  fro-usc:hasHeading "General rule" ;
  fro-usc:hasId "id831ea5ef-1df7-11e6-87ee-8e4a3ab90f2a" ;

  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-2> ;
  fro-usc:hasHeading "Criminal penalty" ;
  fro-usc:hasId "id831ea5ca-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/a" ;
  fro-usc:hasNumberText "(a)" ;
  fro-usc:refers_toContinuation <fro-usc-t12-ch17:r-1-0-4-9-2-4> ;
.
fro-usc-t12-ch17:id831ea5cb-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-2-2> ;
  fro-usc:hasContentText "Whoever knowingly violates any provision of this chapter or, being a company, violates any regulation or order issued by the Board under this chapter, shall be imprisoned not more than 1 year, fined not more than $100,000 per day for each day during which the violation continues, or both." ;
  fro-usc:hasId "id831ea5cb-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/a/1" ;
  fro-usc:hasNumberText "(1)" ;
.
fro-usc-t12-ch17:id831ea5cc-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-2-3> ;
  fro-usc:hasContentText "Whoever, with the intent to deceive, defraud, or profit significantly, knowingly violates any provision of this chapter shall be imprisoned not more than 5 years, fined not more than $1,000,000 per day for each day during which the violation continues, or both." ;
  fro-usc:hasId "id831ea5cc-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/a/2" ;
  fro-usc:hasNumberText "(2)" ;
.
fro-usc-t12-ch17:id831ea5cd-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5ce-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5cf-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5d0-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5d1-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5d2-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:divided_by fro-usc-t12-ch17:id831ea5d3-1df7-11e6-87ee-8e4a3ab90f2a ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3> ;
  fro-usc:hasHeading "Civil money penalty" ;
  fro-usc:hasId "id831ea5cd-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b" ;
  fro-usc:hasNumberText "(b)" ;
.
fro-usc-t12-ch17:id831ea5ce-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3-2> ;
  fro-usc:hasContentText "Any company which violates, and any individual who participates in a violation of, any provision of this chapter, or any regulation or order issued pursuant thereto, shall forfeit and pay a civil penalty of not more than $25,000 for each day during which such violation continues." ;
  fro-usc:hasHeading "Penalty" ;
  fro-usc:hasId "id831ea5ce-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b/1" ;
  fro-usc:hasNumberText "(1)" ;
.
fro-usc-t12-ch17:id831ea5cf-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3-3> ;
  fro-usc:hasContentText "Any penalty imposed under paragraph (1) may be assessed and collected by the Board in the manner provided in subparagraphs (E), (F), (G), and (I) of section 1818(i)(2) of this title for penalties imposed (under such section) and any such assessment shall be subject to the provisions of such section." ;
  fro-usc:hasHeading "Assessment; etc." ;
  fro-usc:hasId "id831ea5cf-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b/2" ;
  fro-usc:hasNumberText "(2)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1818/i/2" ;
.
fro-usc-t12-ch17:id831ea5d0-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3-4> ;
  fro-usc:hasContentText "The company or other person against whom any penalty is assessed under this subsection shall be afforded an agency hearing if such association or person submits a request for such hearing within 20 days after the issuance of the notice of assessment. Section 1818(h) of this title shall apply to any proceeding under this subsection." ;
  fro-usc:hasHeading "Hearing" ;
  fro-usc:hasId "id831ea5d0-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b/3" ;
  fro-usc:hasNumberText "(3)" ;
  fro-usc:hasReferenceText "/us/usc/t12/s1818/h" ;
.
fro-usc-t12-ch17:id831ea5d1-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 5 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3-5> ;
  fro-usc:hasContentText "All penalties collected under authority of this subsection shall be deposited into the Treasury." ;
  fro-usc:hasHeading "Disbursement" ;
  fro-usc:hasId "id831ea5d1-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b/4" ;
  fro-usc:hasNumberText "(4)" ;
.
fro-usc-t12-ch17:id831ea5d2-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 6 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3-6> ;
  fro-usc:hasContentText "For purposes of this section, the term “violate” includes any action (alone or with another or others) for or toward causing, bringing about, participating in, counseling, or aiding or abetting a violation." ;
  fro-usc:hasHeading "“Violate” defined" ;
  fro-usc:hasId "id831ea5d2-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b/5" ;
  fro-usc:hasNumberText "(5)" ;
.
fro-usc-t12-ch17:id831ea5d3-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Paragraph ;
  fro-leg-ref:hasSequenceNumber 7 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-3-7> ;
  fro-usc:hasContentText "The Board shall prescribe regulations establishing such procedures as may be necessary to carry out this subsection." ;
  fro-usc:hasHeading "Regulations" ;
  fro-usc:hasId "id831ea5d3-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/b/6" ;
  fro-usc:hasNumberText "(6)" ;
.
fro-usc-t12-ch17:id831ea5d4-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subsection ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-9-4> ;
  fro-usc:hasContentText "The resignation, termination of employment or participation, or separation of an institution-affiliated party (within the meaning of section 1813(u) of this title ) with respect to a bank holding company (including a separation caused by the deregistration of such a company) shall not affect the jurisdiction and authority of the Board to issue any notice and proceed under this section against any such party, if such notice is served before the end of the 6-year period beginning on the date such party ceased to be such a party with respect to such holding company (whether such date occurs before, on, or after August 9, 1989 )." ;
  fro-usc:hasHeading "Notice under this section after separation from service" ;
  fro-usc:hasId "id831ea5d4-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1847/c" ;
  fro-usc:hasNumberText "(c)" ;

  fro-usc:hasContentText "shares which are of the kinds and amounts eligible for investment by national banking associations under the provisions of section 24 of this title ;" ;
  fro-usc:hasId "id8319ea42-1df7-11e6-87ee-8e4a3ab90f2a" ;

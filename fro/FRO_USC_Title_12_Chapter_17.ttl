fro-usc-t12-ch17:id831c5be7-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 2 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-11-3-2> ;
  fro-usc:hasContentText "In connection with a notice under this subsection, the Board shall consider whether performance of the activity by a bank holding company or a subsidiary of such company can reasonably be expected to produce benefits to the public, such as greater convenience, increased competition, or gains in efficiency, that outweigh possible adverse effects, such as undue concentration of resources, decreased or unfair competition, conflicts of interests, unsound banking practices, or risk to the stability of the United States banking or financial system." ;
  fro-usc:hasHeading "Criteria" ;
  fro-usc:hasId "id831c5be7-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/j/2/A" ;
  fro-usc:hasNumberText "(A)" ;
.
fro-usc-t12-ch17:id831c5be8-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 3 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-11-3-3> ;
  fro-usc:hasContentText "The Board may deny any proposed transaction or activity for which notice has been submitted pursuant to this subsection if the bank holding company submitting such notice neglects, fails, or refuses to furnish the Board all the information required by the Board." ;
  fro-usc:hasHeading "Grounds for disapproval" ;
  fro-usc:hasId "id831c5be8-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/j/2/B" ;
  fro-usc:hasNumberText "(B)" ;
.
fro-usc-t12-ch17:id831c5be9-1df7-11e6-87ee-8e4a3ab90f2a
  rdf:type fro-usc:USC_Subparagraph ;
  fro-leg-ref:hasSequenceNumber 4 ;
  fro-ref:hasSourceInstance <http://finregont.com/fro/usc/USC-2015-title12-chapter17.ttl#r-1-0-4-5-11-3-4> ;
  fro-usc:hasContentText "Nothing in this subsection limits the authority of the Board to impose conditions in connection with an action under this section." ;
  fro-usc:hasHeading "Conditional action" ;
  fro-usc:hasId "id831c5be9-1df7-11e6-87ee-8e4a3ab90f2a" ;
  fro-usc:hasIdentifierText "/us/usc/t12/s1843/j/2/C" ;
  fro-usc:hasNumberText "(C)" ;
.
fro-usc-t12-ch17:id831c5bea-1df7-11e6-87ee-8e4a3ab90f2a

  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Rules and regulations prohibiting fraudulent, deceptive or manipulative courses of conduct" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
usc-t15-ch2D:r-1-0-4-3-19-11-2
  rdf:type uslm:content ;
  composite:child usc-t15-ch2D:r-1-0-4-3-19-11-2-0 ;

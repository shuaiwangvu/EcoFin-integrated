usc-t15-ch2D:r-1-0-4-3-18-4-2
  rdf:type uslm:chapeau ;
  composite:child usc-t15-ch2D:r-1-0-4-3-18-4-2-1 ;
  composite:child usc-t15-ch2D:r-1-0-4-3-18-4-2-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", under an indenture of trust which does not provide for the election of trustees by the shareholders. No natural person shall serve as trustee of such a trust, which is registered as an investment company, after the holders of record of not less than two-thirds of the outstanding shares of beneficial interests in such trust have declared that he be removed from that office either by declaration in writing filed with the custodian of the securities of the trust or by votes cast in person or by proxy at a meeting called for the purpose. Solicitation of such a declaration shall be deemed a solicitation of a proxy within the meaning of" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The foregoing provisions of this section shall not apply to a common-law trust existing on" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "2"^^xsd:int ;
.

usc-t15-ch2D:r-1-0-4-3-28-2-3-1
  rdf:type uslm:content ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "designates one or more trustees or custodians, each of which is a bank, and provides that each such trustee or custodian shall have at all times an aggregate capital, surplus, and undivided profits of a specified minimum amount, which shall not be less than $500,000 (but may also provide, if such trustee or custodian publishes reports of condition at least annually, pursuant to law or to the requirements of its supervising or examining authority, that for the purposes of this paragraph the aggregate capital, surplus, and undivided profits of such trustee or custodian shall be deemed to be its aggregate capital, surplus, and undivided profits as set forth in its most recent report of condition so published);" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.
usc-t15-ch2D:r-1-0-4-3-28-2-4
  rdf:type uslm:paragraph ;
  uslm:class-paragraph "indent1" ;
  uslm:id-paragraph "idd0485645-0c74-11e6-aa53-e455a13f2ad9" ;
  uslm:identifier-paragraph "/us/usc/t15/s80a–26/a/2" ;
  uslm:style-paragraph "-uslm-lc:I12" ;
  composite:child usc-t15-ch2D:r-1-0-4-3-28-2-4-0 ;
  composite:child usc-t15-ch2D:r-1-0-4-3-28-2-4-1 ;
  composite:index "4"^^xsd:int ;
.

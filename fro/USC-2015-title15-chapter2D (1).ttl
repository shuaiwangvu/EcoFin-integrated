  composite:child usc-t15-ch2D:r-1-0-4-3-30-5-10-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", That in the event such option is exercised by the company, interest shall accrue on any payment or payments due to the holder, for the period of such deferment at a rate equal to that used in accumulating the reserves for such certificate:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;

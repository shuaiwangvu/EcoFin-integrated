  composite:child usc-t15-ch2D:r-1-0-4-3-20-7-2-1-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", That immediately after any such borrowing there is an asset coverage of at least 300 per centum for all borrowings of such registered company:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", That in the event that such asset coverage shall at any time fall below 300 per centum such registered company shall, within three days thereafter (not including Sundays and holidays) or such longer period as the Commission may prescribe by rules and regulations, reduce the amount of its borrowings to an extent that the asset coverage of such borrowings shall be at least 300 per centum." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "It shall be unlawful for any registered open-end company to issue any class of senior security or to sell any senior security of which it is the issuer, except that any such registered company shall be permitted to borrow from any bank:" ;

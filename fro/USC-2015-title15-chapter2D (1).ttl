usc-t15-ch2D:r-1-0-4-3-28-2-5-1
  rdf:type uslm:content ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "provides, in substance, that the trustee or custodian shall not resign until either (A) the trust has been completely liquidated and the proceeds of the liquidation distributed to the security holders of the trust, or (B) a successor trustee or custodian, having the qualifications prescribed in paragraph (1) of this subsection, has been designated and has accepted such trusteeship or custodianship; and" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "1"^^xsd:int ;
.

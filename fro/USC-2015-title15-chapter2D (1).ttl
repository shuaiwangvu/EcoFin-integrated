      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Any certificate may provide that the company at its option may, prior to the maturity thereof, defer any payment or payments to the certificate holder to which he may be entitled under this subsection, for a period of not more than thirty days:" ;

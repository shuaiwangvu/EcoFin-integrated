  rdf:type uslm:content ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "if the foregoing minimum percentages of the gross annual payments required under the provisions of such certificate should produce reserve payments larger than are necessary at 3½ per centum per annum compounded annually to provide the minimum maturity or face amount of the certificate when due, the reserve shall be based upon reserve payments accumulated as provided under preceding subparagraph (A) of this paragraph except that in lieu of the 3½ per centum rate specified therein, such rate shall be lowered to the minimum rate, expressed in multiples of one-eighth of 1 per centum, which will accumulate such reserve payments to the maturity value when due;" ;
      composite:index "1"^^xsd:int ;

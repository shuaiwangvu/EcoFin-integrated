  composite:child usc-t15-ch2D:r-1-0-4-3-30-4-2-0-3 ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text ", That where qualified investments are maintained on deposit by such company in respect of its liabilities under certificates issued to or held by residents of any State as required by the statute of such State or by any order, regulation, or requirement of such State or any official or agency thereof, the amount so on deposit, but not to exceed the amount of reserves required by subsection (a) hereof for the certificates so issued or held, shall be deducted from the amount of qualified investments that may be required to be deposited hereunder." ;
      composite:index "5"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "The Commission shall by rule, regulation, or order, in the public interest or for the protection of investors, require a registered face-amount certificate company to deposit and maintain, upon such terms and conditions as the Commission shall prescribe and as are appropriate for the protection of investors, with one or more institutions having the qualifications required by paragraph (1) of" ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "for a trustee of a unit investment trust, all or any part of the investments maintained by such company as certificate reserve requirements under the provisions of subsection (b) hereof:" ;
      composite:index "3"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;
.

usc-t15-ch2D:r-1-0-4-4-13-2-2-0
  rdf:type uslm:p ;
  uslm:class-p "indent0" ;
  uslm:style-p "-uslm-lc:I11" ;
  composite:child [
      rdf:type sxml:TextNode ;
      sxml:text "Whenever it shall appear to the Commission, either upon complaint or otherwise, that the provisions of this subchapter or of any rule or regulation prescribed under the authority thereof, have been or are about to be violated by any person, it may in its discretion require, and in any event shall permit, such person to file with it a statement in writing, under oath or otherwise, as to all the facts and circumstances relevant to such violation, and may otherwise investigate all such facts and circumstances." ;
      composite:index "1"^^xsd:int ;
    ] ;
  composite:index "0"^^xsd:int ;

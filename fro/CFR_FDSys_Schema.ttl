cfr-fdsys-s:ParagraphTextNode
  rdf:type owl:Class ;
  rdfs:label "source Instance" ;
  rdfs:subClassOf sxml:TextNode ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf [
          rdf:type rdf:List ;
          rdf:first sxml:TextNode ;
          rdf:rest [
              rdf:type rdf:List ;
              rdf:first [
                  rdf:type owl:Restriction ;
                  owl:onProperty composite:parent ;
                  owl:someValuesFrom cfr-fdsys-s:P ;
                ] ;
              rdf:rest () ;
            ] ;
        ] ;
    ] ;

	sm:filename "USExampleEntities.rdf" ;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/BE/Corporations/Corporations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/BE/GovernmentEntities/NorthAmericanJurisdiction/USGovernmentEntitiesAndJurisdictions/> ,

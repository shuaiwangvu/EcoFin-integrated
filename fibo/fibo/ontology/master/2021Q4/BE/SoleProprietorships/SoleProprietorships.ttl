<https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships/>
	a owl:Ontology ;
	rdfs:label "Sole Proprietorships Ontology" ;
	dct:abstract "This ontology defines the fundamental concepts for representing sole proprietorships -- i.e., organizations that are owned by an individual that is responsible for the liabilities of the organization." ;
	dct:license "http://opensource.org/licenses/MIT"^^xsd:anyURI ;
	sm:contentLanguage "https://www.w3.org/TR/owl2-quick-reference/"^^xsd:anyURI ;
	sm:copyright
		"Copyright (c) 2016-2021 EDM Council, Inc." ,
		"Copyright (c) 2016-2021 Object Management Group, Inc."
		;
	sm:dependsOn
		"https://spec.edmcouncil.org/fibo/ontology/BE/LegalEntities/LegalPersons/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FND/"^^xsd:anyURI
		;
	sm:fileAbbreviation "fibo-be-sps-sps" ;
	sm:filename "SoleProprietorships.rdf" ;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/BE/LegalEntities/LegalPersons/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/BE/OwnershipAndControl/OwnershipParties/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/AgentsAndPeople/Agents/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/Law/LegalCapacity/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/Organizations/FormalOrganizations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/OwnershipAndControl/Ownership/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/OwnershipAndControl/OwnershipAndControl/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/Parties/Parties/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/Parties/Roles/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/Relations/Relations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/FND/Utilities/AnnotationVocabulary/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/2021Q4/BE/SoleProprietorships/SoleProprietorships/> ;
	skos:changeNote
		"The https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships.rdf version of this ontology was modified per the FIBO 2.0 RFC." ,
		"The https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships.rdf version of this ontology was modified to eliminate a redundant subclass relationship, enhance ownership relations, and revise definitions to be ISO 704 compliant." ,
		"The https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships.rdf version of this ontology was modified to eliminate unnecessary references to external sources." ,
		"The https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships.rdf version of this ontology was modified to use natural person rather than legally capable person."
		;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Release ;
	.

fibo-be-sps-sps:SoleProprietor
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-oac-opty:EntityOwner ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:onClass fibo-be-le-lp:LegallyCompetentNaturalPerson ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-oac-opty:hasInvestmentEntity ;
			owl:someValuesFrom fibo-be-sps-sps:SoleProprietorship ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-oac-oac:ownsAndControls ;
				owl:someValuesFrom fibo-be-sps-sps:SoleProprietorship ;
			] ;
		]
		;
	rdfs:label "sole proprietor" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships/> ;
	skos:definition "party that owns a business, has the rights to all profits from that business and is considered a single entity (unincorporated) together with that business for tax and liability purposes" ;
	fibo-fnd-utl-av:explanatoryNote "A sole proprietor has unlimited liability with respect to any business debts." ;
	fibo-fnd-utl-av:synonym
		"sole owner" ,
		"sole trader"
		;
	.

fibo-be-sps-sps:SoleProprietorship
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-le-lp:BusinessEntity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-oac-oac:isOwnedAndControlledBy ;
			owl:onClass fibo-be-le-lp:LegallyCompetentNaturalPerson ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-law-lcap:hasCapacity ;
			owl:someValuesFrom fibo-fnd-law-lcap:LiabilityCapacity ;
		]
		;
	rdfs:label "sole proprietorship" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/SoleProprietorships/SoleProprietorships/> ;
	skos:definition "unincorporated business owned by a single person" ;
	.


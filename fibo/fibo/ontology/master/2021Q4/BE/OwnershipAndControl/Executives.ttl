	rdfs:label "has responsibility" ;
	rdfs:domain fibo-fnd-pty-pty:IndependentParty ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-fnd-law-lcap:Duty ;
	skos:definition "specifies a commitment or obligation that an independent party has" ;

	rdfs:subPropertyOf fibo-fnd-pty-pty:hasActor ;
	rdfs:label "has authorizing party" ;
	rdfs:domain fibo-be-oac-exec:Authorization ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-be-oac-exec:AuthorizingParty ;
	skos:definition "indicates the party that endorses, enables, empowers, or gives permission in the situation" ;
	.

fibo-be-oac-exec:hasCorporateOfficer
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-oac-ctl:hasControllingParty ;

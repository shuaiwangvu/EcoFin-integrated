	rdfs:subPropertyOf fibo-fnd-pty-pty:hasPartyInRole ;
	rdfs:label "has responsible party" ;
	rdfs:domain fibo-fnd-org-fm:FormalOrganization ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-be-oac-exec:ResponsibleParty ;
	skos:definition "identifies a party that has some assignment, commitment or obligation with respect to the formal organization" ;
	.

fibo-be-oac-exec:hasSigningAuthorityFor

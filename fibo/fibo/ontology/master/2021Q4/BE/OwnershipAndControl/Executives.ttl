	rdfs:subPropertyOf fibo-fnd-pty-pty:isAffectedBy ;
	rdfs:label "is authorized by" ;
	rdfs:domain fibo-be-oac-exec:AuthorizedParty ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-be-oac-exec:AuthorizingParty ;
	skos:definition "is endorsed, enabled, empowered, or otherwise permitted by" ;
	.

fibo-be-oac-exec:isAuthorizedThrough
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-pty-pty:undergoes ;
	rdfs:label "is authorized through" ;
	rdfs:domain fibo-be-oac-exec:AuthorizedParty ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-be-oac-exec:Authorization ;
	owl:inverseOf fibo-be-oac-exec:hasAuthorizedParty ;
	skos:definition "indicates the situation that faciliates endorsement of the authorized party for some purpose" ;
	.

fibo-be-oac-exec:isDirectorOf

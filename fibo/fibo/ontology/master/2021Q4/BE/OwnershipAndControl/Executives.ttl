	rdfs:subPropertyOf
		fibo-be-oac-exec:authorizes ,
		fibo-fnd-rel-rel:designates
		;
	rdfs:label "delegates control to" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-be-oac-exec:LegallyDelegatedAuthority ;
	skos:definition "indicates a party to which this legal person has delegated some authority or control" ;
	.

fibo-be-oac-exec:designatesSignatory
	a owl:ObjectProperty ;
	rdfs:subPropertyOf
		fibo-be-oac-exec:authorizes ,
		fibo-fnd-rel-rel:designates
		;
	rdfs:label "designates signatory" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/> ;
	rdfs:range fibo-be-oac-exec:Signatory ;

	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an amount over par that a debt instrument holder must pay to sell the security early" ;
	.

fibo-sec-dbt-dbti:PutSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:Schedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:PutEvent ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-sec-dbt-dbti:PutPremium
					fibo-fnd-acc-cur:MonetaryPrice
				) ;
			] ;
		]
		;
	rdfs:label "put schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a schedule that defines the events associated with the put feature of a debt instrument, i.e, the dates on which the debt instrument may be sold at what price by the holder" ;
	.

fibo-sec-dbt-dbti:PutWindow

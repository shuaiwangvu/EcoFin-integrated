fibo-sec-dbt-bnd:AmortizingBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRepaymentTerms ;
			owl:someValuesFrom fibo-sec-dbt-bnd:BondAmortizationPaymentTerms ;
		]
		;
	rdfs:label "amortizing bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:BulletBond ;
	skos:definition "bond that regularly pays down the principal (face value) on the debt along with its interest expense over the life of the bond" ;
	.

fibo-sec-dbt-bnd:Bond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:TradableDebtInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasInterestPaymentTerms ;
			owl:someValuesFrom fibo-sec-dbt-bnd:CouponPaymentTerms ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRepaymentTerms ;
			owl:someValuesFrom fibo-sec-dbt-bnd:BondPrincipalRepaymentTerms ;
		]
		;
	rdfs:label "bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "tradable debt instrument representing a loan in which the issuer owes the holder(s) a debt" ;
	fibo-fnd-utl-av:explanatoryNote "Depending on the terms of the contract, the issuer is obliged to pay interest (the coupon) and/or to repay the principal at maturity.  The most common bonds are corporate or governmental, typically used to finance specific projects or operations." ;
	.

fibo-sec-dbt-bnd:BondAmortizationPaymentTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:BondPrincipalRepaymentTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:someValuesFrom fibo-fbc-dae-dbt:AmortizationSchedule ;
		]
		;
	rdfs:label "bond amortization payment terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:BulletPrincipalRepaymentTerms ;
	skos:definition "terms that include a schedule for repayment of the principal over the lifetime of the bond, typically in equal payments at regular intervals" ;
	.

fibo-sec-dbt-bnd:BondConversionTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-sec-iss:ConversionTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-sec-iss:specifiesConversionInto ;
			owl:someValuesFrom fibo-sec-eq-eq:ListedShare ;
		]
		;
	rdfs:label "bond conversion terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "terms indicating when a convertible bond can be converted to another security (usually a publicly traded share issued by of the same issuer)" ;
	.

fibo-sec-dbt-bnd:BondCoupon
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:InterestRate ;
	rdfs:label "bond coupon" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "interest rate on a debt security that the issuer promises to pay to the holder until maturity, expressed as an annual percentage of the face value" ;
	fibo-fnd-utl-av:synonym
		"coupon percent rate" ,
		"coupon rate" ,
		"nominal yield"
		;
	.

fibo-sec-dbt-bnd:BondInsurance
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-dae-gty:InsurancePolicy ;
	rdfs:label "bond insurance" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "insurance policy that a bond issuer purchases that guarantees the repayment of the principal and all associated interest payments to the bondholders in the event of default" ;
	.

fibo-sec-dbt-bnd:BondPrincipalRepaymentTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-dae-dbt:PrincipalRepaymentTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:someValuesFrom fibo-fnd-pas-psch:PaymentSchedule ;
		]
		;
	rdfs:label "bond principal repayment terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-dbti:CallFeature ;
	skos:definition "terms for the repayment of the principal on a bond" ;
	.

fibo-sec-dbt-bnd:BondRegistrar
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:Registrar ,
		fibo-fbc-pas-fpas:LegalAgent ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:registers ;
			owl:someValuesFrom fibo-sec-dbt-bnd:Bond ;
		]
		;
	rdfs:label "bond registrar" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "party responsible for maintaining records on behalf of the issuer that identify the owners of a registered bond issue" ;
	fibo-fnd-utl-av:explanatoryNote "The trustee under a bond contract often also acts as registrar." ;
	.

fibo-sec-dbt-bnd:BondVariableCoupon
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-dae-dbt:VariableInterestRate ,
		fibo-sec-dbt-bnd:BondCoupon
		;
	rdfs:label "bond variable coupon" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond coupon that has a variable interest rate" ;
	.

fibo-sec-dbt-bnd:BondWithPartialCall
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:CallableBond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasCallFeature ;
			owl:someValuesFrom fibo-sec-dbt-bnd:PartialCallFeature ;
		]
		;
	rdfs:label "bond with partial call" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond with a feature whereby the issue can be partially called for amounts that are at the discretion of the issuer" ;
	.

fibo-sec-dbt-bnd:BondWithPublishedSinkingFund
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:AmortizingBond ;
	rdfs:label "bond with published sinking fund" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond whereby amortization is via a known and published schedule" ;
	.

fibo-sec-dbt-bnd:BondWithWarrant
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-dbti:DebtOffering ;
	rdfs:label "bond with warrant" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond issue whereby the issue includes a warrant" ;
	skos:editorialNote 'This need not be any specific type of bond. This is NOT a kind of Bond Contract but a kind of Issue Offer, and is modeled as such, in the Issuance Terms section of the model. This class of contract is retained in the model for now in order to provide traceability with the CFI term for "Bond With Warrant" only.' ;
	.

fibo-sec-dbt-bnd:BuildAmericaBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:MunicipalBond ;
	rdfs:label "Build America Bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "taxable municipal bond issued through December 31, 2010 under the American Recovery and Reinvestment Act of 2009 (ARRA)" ;
	fibo-fnd-utl-av:explanatoryNote "BABs may be direct pay subsidy bonds or tax credit bonds." ;
	.

fibo-sec-dbt-bnd:BulletBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRepaymentTerms ;
			owl:someValuesFrom fibo-sec-dbt-bnd:BulletPrincipalRepaymentTerms ;
		]
		;
	rdfs:label "bullet bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith
		fibo-sec-dbt-bnd:AmortizingBond ,
		fibo-sec-dbt-bnd:CallableBond
		;
	skos:definition "bond whose entire principal value is paid on the maturity date, rather than amortized over its lifetime" ;
	.

fibo-sec-dbt-bnd:BulletPrincipalRepaymentTerms
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:BondPrincipalRepaymentTerms ;
	rdfs:label "bond bullet principal repayment terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "terms for repayment of the principal for a bond as a single payment covering the whole principal amount" ;
	.

fibo-sec-dbt-bnd:CallableBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:isCallable ;
			owl:hasValue "true"^^xsd:boolean ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-bnd:hasExtraordinaryRedemptionProvision ;
			owl:onClass fibo-sec-dbt-bnd:ExtraordinaryRedemptionProvision ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasCallFeature ;
			owl:someValuesFrom fibo-sec-dbt-dbti:CallFeature ;
		]
		;
	rdfs:label "callable bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond that includes a stipulation allowing the issuer the right to repurchase and retire the bond at the call price after the call protection period" ;
	.

fibo-sec-dbt-bnd:CallableConvertibleBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:CallableBond ,
		fibo-sec-dbt-bnd:ConvertibleBond
		;
	rdfs:label "callable convertible bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "convertible bond that is also callable" ;
	.

fibo-sec-dbt-bnd:CertificateOfObligation
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:MunicipalSecurity ;
	rdfs:label "certificate of obligation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "municipal security available to governing councils in case of emergency, such as a natural disaster, that needs immediate action without time for voter referendum" ;
	fibo-fnd-utl-av:abbreviation "CO" ;
	fibo-fnd-utl-av:explanatoryNote
		"CO's are similar to GO bonds, except that they do not require voter approval before they are issued. The CO's are also guaranteed by the City's taxation power and are counted in the calculation of the tax rate that is needed to support debt payments." ,
		"For example, when a hurricane destroys the police and emergency services building, there is no time to go through the process of voter referendum. The local council must be able to borrow the money to set up provisional buildings and necessary equipment for police and emergency services so that the community is served in continuity."
		;
	.

fibo-sec-dbt-bnd:CertificateOfParticipation
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-fi-fi:DebtInstrument ;
	rdfs:label "certificate of participation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "debt instrument evidencing a pro rata share in a specific pledged revenue stream, usually lease payments by the issuer that are typically subject to annual appropriation" ;
	fibo-fnd-utl-av:abbreviation "COP" ;
	fibo-fnd-utl-av:explanatoryNote "A certificate of participation (COP) is a type of financing where an investor purchases a share of the lease revenues of a program rather than the bond being secured by those revenues. The certificate generally entitles the holder to receive a share, or participation, in the payments from a particular project. The payments are passed through the lessor to the certificate holders. The lessor typically assigns the lease and the payments to a trustee, which then distributes the payments to the certificate holders." ;
	.

fibo-sec-dbt-bnd:ConventionalGilt
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:FixedCouponBond ,
		fibo-sec-dbt-bnd:UKGovernmentSecurity
		;
	rdfs:label "conventional gilt" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	rdfs:seeAlso <https://www.dmo.gov.uk/responsibilities/gilt-market/about-gilts/> ;
	skos:definition "fixed coupon bond issued by HM Treasury that guarantees to pay the holder of the gilt a fixed cash payment (coupon) every six months until the maturity date, at which point the holder receives the final coupon payment and the return of the principal" ;
	fibo-fnd-utl-av:explanatoryNote "Conventional gilts are the simplest form of government bond and constitute around 75 percent of the gilt portfolio." ;
	.

fibo-sec-dbt-bnd:ConvertibleBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		fibo-sec-sec-iss:ConvertibleSecurity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
			owl:onClass fibo-sec-dbt-bnd:BondConversionTerms ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "convertible bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond that gives the holder the right to convert the bond into a fixed number of shares (conversion ratio) if the equity price rises above a specified level (strike price)" ;
	fibo-fnd-utl-av:explanatoryNote "If the equity price remains below the strike price throughout the term of the bond it matures and is redeemed like a regular bond. The conversion ratio and strike price are usually set when the convertible bond is issued." ;
	.

fibo-sec-dbt-bnd:CorporateBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isIssuedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
				owl:someValuesFrom fibo-be-le-lp:LegalEntity ;
			] ;
		]
		;
	rdfs:label "corporate bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith
		fibo-sec-dbt-bnd:GovernmentBond ,
		fibo-sec-dbt-bnd:MunicipalBond
		;
	skos:definition "bond issued by a company in order to raise financing" ;
	fibo-fnd-utl-av:explanatoryNote
		"Corporate bonds are issued for purposes such as mergers and acquisitions, business expansion, or to cover ongoing operational needs, and are typically longer-term debt instruments that have a maturity of at least one year. Corporate debt instruments with maturity shorter than one year are referred to as commercial paper." ,
		"Note that some classification schemes consider any bond except those issued by a government in its own currency to be a corporate bond, for example, a bond issued by Canada in US dollars might be classified as a corporate bond. Bonds issued by multinational / supranational organizations such as the European Bank for Reconstruction and Development (EBRD) may also be considered corporate bonds rather than government bonds."
		;
	.

fibo-sec-dbt-bnd:CouponPayment
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-pas-psch:PaymentEvent ;
	rdfs:label "coupon payment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "payment event involving an interest payment on a bond" ;
	.

fibo-sec-dbt-bnd:CouponPaymentTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-dae-dbt:InterestPaymentTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
			owl:someValuesFrom fibo-sec-dbt-bnd:BondCoupon ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasCurrency ;
			owl:someValuesFrom fibo-fnd-acc-cur:Currency ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:someValuesFrom fibo-sec-dbt-bnd:CouponSchedule ;
		]
		;
	rdfs:label "coupon payment terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "terms for payment of interest on a bond" ;
	.

fibo-sec-dbt-bnd:CouponSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:RegularSchedule ,
		fibo-fnd-pas-psch:PaymentSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-bnd:CouponPayment ;
		]
		;
	rdfs:label "coupon schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "payment schedule that consists of interest payments on a bond" ;
	.

fibo-sec-dbt-bnd:EquityLinkedBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:VariableCouponBond ;
	rdfs:label "equity linked bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond based on the return on an equity over time, i.e. the price and dividend payments or the total return (similar to total return swaps)" ;
	.

fibo-sec-dbt-bnd:ExtraordinaryRedemptionProvision
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-dbti:CallFeature ;
	rdfs:label "extraordinary redemption provision" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "provision that gives a bond issuer the right to call its bonds due to an unusual one-time occurrence, as specified in the offering statement" ;
	fibo-fnd-utl-av:explanatoryNote "Such redemptions may occur when bond proceeds are not spent according to schedule; when bond proceeds are used in a way that makes nontaxable bond interest taxable; or when a catastrophe destroys the project being financed, among other reasons." ;
	.

fibo-sec-dbt-bnd:FirstRegularCouponDate
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:ExplicitDate ;
	rdfs:label "first regular coupon date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "scheduled date for the first regular coupon payment, in a regular repeating series of coupon periods, after any non standard (long or short) first coupon period in the schedule" ;
	.

fibo-sec-dbt-bnd:FixedCouponBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		fibo-sec-dbt-dbti:FixedIncomeSecurity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasInterestPaymentTerms ;
			owl:someValuesFrom fibo-sec-dbt-bnd:FixedCouponTerms ;
		]
		;
	rdfs:label "fixed coupon bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:VariableCouponBond ;
	skos:definition "bond whose coupon rate and principal amount are fixed at the time of origination or sale and remain constant while the security is outstanding" ;
	.

fibo-sec-dbt-bnd:FixedCouponTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:CouponPaymentTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
			owl:someValuesFrom fibo-fbc-dae-dbt:FixedInterestRate ;
		]
		;
	rdfs:label "fixed coupon terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:VariableCouponTerms ;
	skos:definition "terms for payment of interest on a bond with a fixed interest rate" ;
	.

fibo-sec-dbt-bnd:FloatingRateNote
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:VariableCouponBond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
			owl:someValuesFrom fibo-ind-ir-ir:ReferenceInterestRate ;
		]
		;
	rdfs:label "floating rate note" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond with a variable interest rate based on a published reference interest rate" ;
	fibo-fnd-utl-av:explanatoryNote 'The adjustments to the interest rate (coupon) are made periodically, usually on a quarterly or monthly basis, and are tied to a certain money-market index. Also known as a "floater". For example six months USD LIBOR + 0.20%.' ;
	.

fibo-sec-dbt-bnd:FullFaithCreditBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:UnsecuredBond ;
	rdfs:label "full faith credit bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond secured by an unconditional promise to pay by another entity" ;
	fibo-fnd-utl-av:explanatoryNote "Full faith and credit bonds are typically backed by a government entity and are considered low risk." ;
	fibo-fnd-utl-av:synonym "full faith and credit bond" ;
	.

fibo-sec-dbt-bnd:GeneralObligationMunicipalBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:FullFaithCreditBond ,
		fibo-sec-dbt-bnd:MunicipalBond
		;
	rdfs:label "general obligation municipal bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:RevenueBond ;
	skos:definition "municipal bond that is backed by the full faith and credit and general resources of the issuing municipality, including its general taxing authority" ;
	fibo-fnd-utl-av:abbreviation "GO bond" ;
	.

fibo-sec-dbt-bnd:GovernmentBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		fibo-sec-dbt-bnd:GovernmentIssuedDebtSecurity
		;
	rdfs:label "government bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "debt security issued by a government to fund government spending" ;
	fibo-fnd-utl-av:synonym "government-issued bond" ;
	.

fibo-sec-dbt-bnd:GovernmentIssuedDebtSecurity
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:DebtInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isIssuedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
				owl:someValuesFrom fibo-be-ge-ge:Polity ;
			] ;
		]
		;
	rdfs:label "government issued debt security" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "debt security issued by some government on behalf of some polity, including sovereign and municipal debt" ;
	.

fibo-sec-dbt-bnd:GreenBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:Bond ;
	rdfs:label "green bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond issued specifically to fund climate or environmental projects" ;
	fibo-fnd-utl-av:explanatoryNote "These bonds are typically asset-linked and backed by the issuer's balance sheet.  Green bonds finance projects aimed at energy efficiency, pollution prevention, sustainable agriculture, fishery and forestry, the protection of aquatic and terrestrial ecosystems, clean transportation, sustainable water management and the cultivation of environmentally friendly technologies, and often include incentives such as tax exemption." ;
	.

fibo-sec-dbt-bnd:ImplicitFullFaithCreditBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:UnsecuredBond ;
	rdfs:label "implicit full faith credit bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond issued by a government sponsored agency or corporation rather than by the government directly" ;
	fibo-fnd-utl-av:explanatoryNote "It doesn't carry an explicit full faith and credit guarantee but the market believes the government wouldn't let it default or fail." ;
	fibo-fnd-utl-av:synonym "implicit full faith and credit bond" ;
	.

fibo-sec-dbt-bnd:IndexLinkedBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:VariableIncomeBond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:isBasedOn ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-ind-ei-ei:EconomicIndicator
					fibo-ind-ind-ind:MarketRate
				) ;
			] ;
		]
		;
	rdfs:label "index-linked bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond whose income may vary over time, because either the coupon rate or principal amount is related to a specific index, such as the Consumer Price Index" ;
	.

fibo-sec-dbt-bnd:InflationLinkedBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:IndexLinkedBond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:isBasedOn ;
			owl:someValuesFrom fibo-ind-ei-ei:InflationRate ;
		]
		;
	rdfs:label "inflation-linked bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond indexed to inflation so that the principal or interest payments rise and fall with the rate of inflation" ;
	fibo-fnd-utl-av:abbreviation "ILB" ;
	fibo-fnd-utl-av:explanatoryNote "Inflation-linked bonds are primarily issued by sovereign governments, such as the U.S. and the UK." ;
	fibo-fnd-utl-av:synonym "inflation-indexed bond" ;
	.

fibo-sec-dbt-bnd:LastRegularCouponDate
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:ExplicitDate ;
	rdfs:label "last regular coupon date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "scheduled date of the last regular coupon payment, that is the last of the payments that are made according to a regular repeating series of coupon periods, before any non-standard (long or short) final coupon period in the schedule" ;
	.

fibo-sec-dbt-bnd:ListedBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		fibo-sec-sec-lst:ListedSecurity
		;
	rdfs:label "listed bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:UnlistedBond ;
	skos:definition "bond that may be traded on an exchange" ;
	fibo-fnd-utl-av:explanatoryNote "Most exchange traded bonds are corporate bonds (but most corporate bonds are not exchange traded bonds)." ;
	.

fibo-sec-dbt-bnd:LotteryConvention
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-bnd:PartialRedemptionAllocationConvention
		;
	rdfs:label "lottery convention" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "convention involving an impartial lottery for determining which securities are subject to partial call or partial redemption" ;
	.

fibo-sec-dbt-bnd:MakeWholeCall
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-dbti:CallEvent ;
	rdfs:label "make whole call" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "call allowing the issuer to pay off remaining debt early" ;
	fibo-fnd-utl-av:explanatoryNote "The issuer typically has to make a lump sum payment to the investor(s) derived from a formula based on the net present value (NPV) of future coupon payments that will not be paid incrementally because of the call combined with the principal payment the investor would have received at maturity." ;
	.

fibo-sec-dbt-bnd:MandatoryConvertibleBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:ConvertibleBond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-agr-ctr:hasContractualElement ;
			owl:someValuesFrom fibo-sec-dbt-bnd:BondConversionTerms ;
		]
		;
	rdfs:label "mandatory convertible bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "convertible bond that converts into shares at maturity regardless of the equity price" ;
	fibo-fnd-utl-av:explanatoryNote "The conversion ratio at maturity reflects the equity price and par value of the bond when issued. There is also typically a second higher conversion ratio if the equity price rises above the strike during the term of the bond." ;
	.

fibo-sec-dbt-bnd:MediumTermNote
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:Bond ;
	rdfs:label "medium term note" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond issued over time under a shelf registration program, where each issue may have a different coupon and maturity typically ranging from one to ten years" ;
	fibo-fnd-utl-av:explanatoryNote
		"A medium-term note (MTN) is a debt note that usually matures (is paid back) in 5 to 10 years, but the term may be less than one year or as long as 100 years. They can be issued on a fixed or floating coupon basis." ,
		"By shelf registration we mean the security registration process where an issuer registers in advance, and can issue lots of securities for up to three years." ,
		"Medium term notes are typically issued by corporations and financial institutions, although GSEs also have MTN programs. MTNs may be issued under a shelf registration program which allows the company to issue bonds over time with varying maturities and coupons. Companies issue MTNs to have a more flexible source of funding. They may also issue MTN in response to 'reverse inquiry' by investors looking for bonds with specific maturities, issue size and coupon."
		;
	.

fibo-sec-dbt-bnd:MunicipalBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:GovernmentBond ,
		fibo-sec-dbt-bnd:MunicipalSecurity
		;
	rdfs:label "municipal bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "government bond that may be issued by a regional, rather than national, authority" ;
	fibo-fnd-utl-av:abbreviation "muni" ;
	fibo-fnd-utl-av:explanatoryNote "Municipal bonds may be issued by states, cities, counties, special tax districts or special agencies or authorities of state or local governments." ;
	.

fibo-sec-dbt-bnd:MunicipalDebtFundsUsage
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-arr-doc:LegalDocument ;
	rdfs:label "municipal debt funds usage" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "official statement specifying how the funds raised via a new issue of municipal securities are to be applied" ;
	.

fibo-sec-dbt-bnd:MunicipalDebtRemarketingAgent
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-pas-fpas:Dealer ,
		fibo-fnd-agr-ctr:ContractThirdParty
		;
	rdfs:label "municipal debt remarketing agent" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "municipal securities dealer responsible for reselling to investors securities (such as variable rate demand obligations and other tender option bonds) that have been tendered for purchase by their owner" ;
	fibo-fnd-utl-av:explanatoryNote "The remarketing agent also typically is responsible for resetting the interest rate for a variable rate issue and may act as tender agent." ;
	.

fibo-sec-dbt-bnd:MunicipalDebtSourceOfFunds
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-arr-doc:LegalDocument ;
	rdfs:label "municipal debt source of funds" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "official statement identifying the source of funds for a new issue of municipal securities" ;
	.

fibo-sec-dbt-bnd:MunicipalNote
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:MunicipalSecurity ;
	rdfs:label "municipal note" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "short-term obligation to repay a specified principal amount on a certain date, together with interest at a stated rate, usually payable from a defined source of anticipated revenues" ;
	fibo-fnd-utl-av:explanatoryNote "Notes usually mature in one year or less, although notes of longer maturities are also issued." ;
	.

fibo-sec-dbt-bnd:MunicipalSecurity
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:GovernmentIssuedDebtSecurity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-gao-obj:hasObjective ;
			owl:someValuesFrom fibo-sec-dbt-bnd:MunicipalDebtFundsUsage ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isIssuedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
				owl:someValuesFrom [
					a owl:Class ;
					owl:unionOf (
						fibo-be-ge-ge:MunicipalEntity
						fibo-be-ge-ge:RegionalSovereignty
					) ;
				] ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-bnd:hasFundingSource ;
			owl:someValuesFrom fibo-sec-dbt-bnd:MunicipalDebtSourceOfFunds ;
		]
		;
	rdfs:label "municipal security" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:SovereignDebtInstrument ;
	skos:definition "debt obligation issued by a regional governmental entity" ;
	fibo-fnd-utl-av:explanatoryNote "A municipal security is typically a bond, note, warrant, certificate or other similar obligation issued by a state or local government or their agencies or authorities (such as cities, towns, villages, counties or special districts or authorities).  A prime feature of most municipal securities is that interest or other investment earnings on them are generally excluded from gross income of the bondholder for federal income tax purposes. Some municipal securities are subject to federal income tax, although the issuers or bondholders may receive other federal tax advantages for certain types of taxable municipal securities. Some examples include Build America Bonds, municipal fund securities and direct pay subsidy bonds." ;
	.

fibo-sec-dbt-bnd:MunicipalTrustee
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-tr-tr:Trustee ,
		fibo-fbc-fct-fse:FinancialInstitution
		;
	rdfs:label "municipal trustee" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "financial institution with trust powers, designated by the issuer, that acts, pursuant to a bond contract, in a fiduciary capacity for the benefit of the bondholders in enforcing the terms of the bond contract" ;
	fibo-fnd-utl-av:explanatoryNote "In many cases, the trustee also acts as custodian, paying agent, registrar and/or transfer agent for the bonds." ;
	.

fibo-sec-dbt-bnd:OriginalIssueDiscountBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRelativePriceAtMaturity ;
			owl:hasValue fibo-sec-dbt-dbti:ParValue ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-bnd:hasOriginalIssueDiscountAmount ;
			owl:onClass fibo-fnd-acc-cur:MonetaryAmount ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "original issue discount bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "interest-bearing bond issued at a deep discount to face value" ;
	fibo-fnd-utl-av:explanatoryNote
		"An original issue discount (OID) is the discount in price from a bond's face value at the time a bond or other debt instrument is first issued. The OID is the amount of discount or the difference between the original face value and the price paid for the bond." ,
		"The principal amount accretes over time at a constant accrual rate and is redeemed at full face value at maturity."
		;
	fibo-fnd-utl-av:synonym "OID bond" ;
	.

fibo-sec-dbt-bnd:PartialCall
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-dbti:CallEvent ;
	rdfs:label "partial call" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "call of part of an issue" ;
	.

fibo-sec-dbt-bnd:PartialCallFeature
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:CallFeature ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-bnd:hasPartialRedemptionAllocationConvention ;
			owl:someValuesFrom fibo-sec-dbt-bnd:PartialRedemptionAllocationConvention ;
		]
		;
	rdfs:label "partial call feature" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "call feature whereby the issuer can recall part of the issue on scheduled dates, where bonds are selected to be called according to some rule, or by selecting various bonds at random" ;
	.

fibo-sec-dbt-bnd:PartialRedemptionAllocationConvention
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-bd:Convention ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasRateValue ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:decimal ;
		]
		;
	rdfs:label "partial redemption allocation convention" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "convention used to determine how the partial call will be actioned with respect to bond selection" ;
	.

fibo-sec-dbt-bnd:PercentageCumulativeAverageValue
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-bnd:RateBasisConvention
		;
	rdfs:label "percentage cumulative average value" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "rate basis calculated as a percentage of cumulative average value" ;
	fibo-fnd-utl-av:abbreviation "percentage CAV" ;
	.

fibo-sec-dbt-bnd:PercentageParValue
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-bnd:RateBasisConvention
		;
	rdfs:label "percentage par value" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "rate basis calculated as a percentage of par value" ;
	.

fibo-sec-dbt-bnd:PerpetualBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:Bond ;
	rdfs:label "perpetual bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond that has no maturity date, i.e., one that pays interest in perpetuity" ;
	fibo-fnd-utl-av:explanatoryNote "Perpetual bonds function much like dividend-paying stocks or certain preferred securities. Just as the owner of the stock receives a dividend payment as long as the stock is held, the perpetual bond owner receives an interest payment as long as the bond is held." ;
	fibo-fnd-utl-av:synonym "consul" ;
	.

fibo-sec-dbt-bnd:ProRataConvention
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-bnd:PartialRedemptionAllocationConvention
		;
	rdfs:label "pro rata convention" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "convention whereby securities that are subject to partial redemption are allocated proportionately" ;
	.

fibo-sec-dbt-bnd:RateBasisConvention
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-bd:Convention ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasRateValue ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:decimal ;
		]
		;
	rdfs:label "rate basis convention" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "basis for a percentage quoted rate as used in specifying bond call percentage amounts" ;
	.

fibo-sec-dbt-bnd:RedemptionPayment
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pas-psch:PaymentEvent ,
		fibo-sec-dbt-dbti:RedemptionEvent
		;
	rdfs:label "redemption payment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "repayment event involving payment of a bond's principal amount at maturity or when it is called" ;
	.

fibo-sec-dbt-bnd:RedemptionSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pas-psch:PaymentSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:RedemptionEvent ;
		]
		;
	rdfs:label "redemption schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "repayment schedule whereby a given percentage of a bond issue is redeemed on predefined dates" ;
	.

fibo-sec-dbt-bnd:RegularCouponSchedule
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:CouponSchedule ;
	rdfs:label "regular coupon schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "schedule including an interval of regular coupon payment dates" ;
	fibo-fnd-utl-av:explanatoryNote "A regular schedule may include an initial and/or final stub period." ;
	.

fibo-sec-dbt-bnd:RegulatoryCall
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-dbti:CallEvent ;
	rdfs:label "regulatory call" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "call triggered by some regulation-specific rule" ;
	.

fibo-sec-dbt-bnd:RemarketableBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:CorporateBond ,
		fibo-sec-dbt-bnd:FloatingRateNote
		;
	rdfs:label "remarketable bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "corporate bond program where the coupon rate on outstanding bonds is periodically reset through an auction process" ;
	fibo-fnd-utl-av:explanatoryNote "A remarketing agent (dealer or underwriter) periodically surveys bond holders to identify those who want to sell bonds. The agent surveys market (or holds an auction) to determine interest rate at which the bonds can be resold. The rate on all outstanding bonds resets at the new rate. These programs are perpetual in the sense they often don't have a fixed maturity date, but the company can redeem them. If an auction fails, i.e., the agent can't place all the bonds then." ;
	.

fibo-sec-dbt-bnd:RevenueBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:MunicipalBond ,
		fibo-sec-dbt-bnd:SecuredBond
		;
	rdfs:label "revenue bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "municipal bond supported by the revenue from a specific project" ;
	fibo-fnd-utl-av:explanatoryNote "Revenue bonds are municipal bonds that finance income-producing projects, such as toll bridges, highways, or local stadiums, and are secured by a specified revenue source. Typically, revenue bonds can be issued by any government agency or fund that is managed in the manner of a business, such as entities having both operating revenues and expenses." ;
	.

fibo-sec-dbt-bnd:SecuredBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:Bond ;
	rdfs:label "secured bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:UnsecuredBond ;
	skos:definition "bond that is backed by collateral, such as a tangible asset or income stream, in addition to a general promise to pay" ;
	fibo-fnd-utl-av:explanatoryNote "A secured bond may be collateralized by a claim on real assets, such as a factory or auto fleet; or by a claim on a revenue stream.  A secured bond differs from a mortgage in that proceeds of the bond sale aren't used to acquire the asset." ;
	.

fibo-sec-dbt-bnd:SinkingFundAmortizationTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:BondAmortizationPaymentTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-bnd:isMandatory ;
			owl:onDataRange xsd:boolean ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "sinking fund amortization terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "terms for the paydown of principal in a sinking fund type of amortizing security" ;
	skos:editorialNote "At present there is only a schedule, there should be other terms for what happens on the scheduled dates. Sinking fund may be bullet e.g. x percent over year for y years. SF may be mandatory or contingent on some other economic event." ;
	.

fibo-sec-dbt-bnd:SovereignBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:GovernmentBond ,
		fibo-sec-dbt-bnd:SovereignDebtInstrument
		;
	rdfs:label "sovereign bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	owl:disjointWith fibo-sec-dbt-bnd:MunicipalBond ;
	skos:definition "bond issued by the government of a country" ;
	fibo-fnd-utl-av:explanatoryNote "Sovereign bonds issued by G20 developed countries are generally full faith and credit obligations. Sovereign bonds issued by emerging and developing countries may be issued in local currency or a G7 currency, and may either be full faith and credit (unsecured) or secured." ;
	.

fibo-sec-dbt-bnd:SovereignDebtInstrument
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:GovernmentIssuedDebtSecurity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isIssuedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
				owl:someValuesFrom fibo-be-ge-ge:SovereignState ;
			] ;
		]
		;
	rdfs:label "sovereign debt instrument" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "debt security issued by the government of a country" ;
	.

fibo-sec-dbt-bnd:SpecialAssessmentBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:MunicipalBond ,
		fibo-sec-dbt-bnd:SecuredBond
		;
	rdfs:label "special assessment bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "municipal bond used to fund a development project that is payable from the revenues of an assessment (tax) on the community that is intended to benefit from the project" ;
	fibo-fnd-utl-av:explanatoryNote "A special assessment is a charge imposed against a property in a particular locality because that property receives a special benefit by virtue of some public improvement, separate and apart from the general benefit accruing to the public at large. Special assessments may be apportioned according to the value of the benefit received, rather than merely the cost of the improvement." ;
	.

fibo-sec-dbt-bnd:SpecialObligationBond
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-bnd:MunicipalBond ;
	rdfs:label "special obligation bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond secured by a limited revenue source, such as receipts derived from a designated project" ;
	.

fibo-sec-dbt-bnd:SpecialTaxBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:MunicipalBond ,
		fibo-sec-dbt-bnd:SecuredBond
		;
	rdfs:label "special tax bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond secured by revenues derived from designated taxes other than ad valorem taxes" ;
	skos:example "For example, bonds for a particular purpose might be supported by sales, cigarette, fuel or business license taxes." ;
	.

fibo-sec-dbt-bnd:StepEvent
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:PrescriptiveEvent ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:involves ;
			owl:someValuesFrom fibo-sec-dbt-bnd:FixedCouponTerms ;
		]
		;
	rdfs:label "step event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "event whereby a set of fixed coupon terms comes into force as specified in a step schedule" ;
	.

fibo-sec-dbt-bnd:StepSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:AdHocSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-bnd:StepEvent ;
		]
		;
	rdfs:label "step schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "schedule specifying the dates and rates relevant to securities that pay an initial interest rate but also have a feature where set rate increases happen at periodic intervals" ;
	.

fibo-sec-dbt-bnd:StepUpBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:Bond ,
		fibo-sec-dbt-dbti:FixedIncomeSecurity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasInterestPaymentTerms ;
			owl:someValuesFrom fibo-sec-dbt-bnd:SteppedCouponTerms ;
		]
		;
	rdfs:label "step up bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond with a coupon that increases (steps up) while the bond is outstanding" ;
	fibo-fnd-utl-av:explanatoryNote "The step change may be one time, or occur according to a schedule or at regular intervals." ;
	fibo-fnd-utl-av:synonym "step down bond" ;
	.

fibo-sec-dbt-bnd:SteppedCouponTerms
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:CouponPaymentTerms ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:someValuesFrom fibo-sec-dbt-bnd:StepSchedule ;
		]
		;
	rdfs:label "stepped coupon terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "coupon payment terms for securities with a coupon that increases (steps up) while the bond is outstanding" ;
	.

fibo-sec-dbt-bnd:StripBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:ZeroCouponBond ,
		fibo-sec-dbt-dbti:Strip ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:isBasedOn ;
			owl:someValuesFrom fibo-sec-dbt-bnd:Bond ;
		]
		;
	rdfs:label "strip bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond that is part of a series of bonds formed by selling each interest payment and the principal amount of a bond as separate zero coupon bonds." ;
	.

fibo-sec-dbt-bnd:TaxAllocationBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:MunicipalBond ,
		fibo-sec-dbt-bnd:SecuredBond
		;
	rdfs:label "tax allocation bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "bond payable from the incremental increase in tax revenues realized from any increase in property value and other economic activity, often designed to capture the economic benefit resulting from a bond financing" ;
	fibo-fnd-utl-av:explanatoryNote "Tax increment bonds, also known as tax allocation bonds, often are used to finance the redevelopment of blighted areas." ;
	.

fibo-sec-dbt-bnd:TreasuryBill
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:USTreasurySecurity ,
		fibo-sec-dbt-tstd:MoneyMarketInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRelativePriceAtIssue ;
			owl:hasValue fibo-sec-dbt-dbti:AtADiscount ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRelativePriceAtMaturity ;
			owl:hasValue fibo-sec-dbt-dbti:ParValue ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
			owl:someValuesFrom fibo-ind-ir-ir:ReferenceInterestRate ;
		]
		;
	rdfs:label "treasury bill" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "short-term zero coupon treasury obligation with a maturity ranging from one to twelve months" ;
	fibo-fnd-utl-av:abbreviation "T-bill" ;
	.

fibo-sec-dbt-bnd:TreasuryBond
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-bnd:SovereignBond ,
		fibo-sec-dbt-bnd:USTreasurySecurity
		;
	rdfs:label "treasury bond" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/> ;
	skos:definition "long term term coupon bearing treasury obligation with original maturity over ten years, with 30 years being most common" ;
	.

fibo-sec-dbt-bnd:TreasuryInflationProtectedSecurity

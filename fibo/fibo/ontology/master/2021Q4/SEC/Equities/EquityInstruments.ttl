fibo-sec-eq-eq:AdjustableRateDividend
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredDividend ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-qt-qtu:hasQuantityKind ;
			owl:someValuesFrom fibo-ind-ir-ir:InterestRateBenchmark ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasAdjustableDividendRate ;
			owl:someValuesFrom fibo-fnd-acc-cur:PercentageMonetaryAmount ;
		]
		;
	rdfs:label "adjustable rate dividend" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "dividend that varies with a benchmark" ;
	fibo-fnd-utl-av:explanatoryNote "The value of the dividend from the preferred share is set by a predetermined formula to move with rates, and because of this flexibility preferred prices are often more stable then fixed-rate preferred stocks." ;
	.

fibo-sec-eq-eq:AuctionRateDividend
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredDividend ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasAdjustableDividendRate ;
			owl:someValuesFrom fibo-fnd-acc-cur:PercentageMonetaryAmount ;
		]
		;
	rdfs:label "auction rate dividend" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "preferred share dividend whose rate is periodically reset through an auction, typically every 7, 14, 28, or 35 days" ;
	.

fibo-sec-eq-eq:CommonShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:Share ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:pays ;
			owl:onClass fibo-sec-eq-eq:OrdinaryDividend ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "common share"@en-US ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	owl:disjointWith fibo-sec-eq-eq:PreferredShare ;
	skos:definition "share that signifies a unit of ownership in a corporation and represents a claim on part of the corporation's assets and earnings" ;
	fibo-fnd-utl-av:explanatoryNote "In the event that the corporation is liquidated, claims of secured and unsecured creditors and owners of bonds and preferred shares take precedence over claims of common share holders." ;
	fibo-fnd-utl-av:synonym "ordinary share"@en-GB ;
	.

fibo-sec-eq-eq:ConvertibleCommonShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:CommonShare ,
		fibo-sec-sec-iss:ConvertibleSecurity
		;
	rdfs:label "convertible common share" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "common share that is convertible into another security" ;
	.

fibo-sec-eq-eq:ConvertiblePreferredShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredShare ,
		fibo-sec-sec-iss:ConvertibleSecurity
		;
	rdfs:label "convertible preferred share" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "preferred share that includes an option for the holder to convert the shares into a fixed number of common shares after a predetermined date" ;
	fibo-fnd-utl-av:explanatoryNote "Most convertible preferred stock is exchanged at the request of the shareholder, but sometimes there is a provision that allows the company, or issuer, to force conversion. The value of a convertible preferred stock is ultimately based on the performance of the common stock." ;
	.

fibo-sec-eq-eq:CumulativePreferredShare
	a owl:Class ;
	rdfs:subClassOf fibo-sec-eq-eq:PreferredShare ;
	rdfs:label "cumulative preferred share" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "preferred share whose dividends, if not paid on time, accumulate until paid out" ;
	fibo-fnd-utl-av:explanatoryNote "These dividends have precedence over ordinary dividends which cannot be paid until any cumulative dividend obligations have been paid. Dividends are typically deferred due to insufficient earnings or other business reasons." ;
	.

fibo-sec-eq-eq:Custodian
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-fse:FinancialInstitution ,
		fibo-fbc-pas-fpas:ThirdPartyAgent ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:holds ;
			owl:someValuesFrom fibo-sec-eq-eq:Share ;
		]
		;
	rdfs:label "custodian" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "financial institution that holds customers' securities for safekeeping"@en ;
	fibo-fnd-utl-av:adaptedFrom "https://www.investopedia.com/terms/c/custodian.asp"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "The custodian may hold stocks or other assets in electronic or physical form for mutual funds, individuals, and organizational clients." ;
	.

fibo-sec-eq-eq:DistributionByCashPayment
	a
		owl:NamedIndividual ,
		fibo-sec-eq-eq:DividendDistributionMethod
		;
	rdfs:label "distribution by payment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "distribution method involving payment of cash to the shareholder" ;
	.

fibo-sec-eq-eq:DistributionByReinvestment
	a
		owl:NamedIndividual ,
		fibo-sec-eq-eq:DividendDistributionMethod
		;
	rdfs:label "distribution by reinvestment" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "distribution method involving automatic reinvestment of shareholders' dividends in additional, potentially fractional, shares in the same asset" ;
	.

fibo-sec-eq-eq:Dividend
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-agr-agr:Commitment ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasDistributionMethod ;
			owl:onClass fibo-sec-eq-eq:DividendDistributionMethod ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pas-psch:hasPaymentAmount ;
			owl:onClass fibo-fnd-acc-cur:MonetaryAmount ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasSchedule ;
			owl:onClass fibo-sec-eq-eq:DividendSchedule ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "dividend" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "announced commitment to make a specific distribution of a portion of earnings to shareholders, prorated by class of security" ;
	fibo-fnd-utl-av:explanatoryNote "The amount and timing of payment is set by the board of directors, typically quarterly. Dividends may be paid in the form of money, shares, scrip, or on rare occasion, property." ;
	.

fibo-sec-eq-eq:DividendDistributionMethod
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-bd:Convention ;
	rdfs:label "dividend distribution method" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "convention by which dividends are provided to shareholders" ;
	fibo-fnd-utl-av:explanatoryNote "Distribution may be by payment of a monetary amount or by reinvestment, as specified by the board of directors at the time a decision to issue a dividend is made." ;
	.

fibo-sec-eq-eq:DividendSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pas-psch:PaymentSchedule ,
		fibo-sec-sec-sch:ParametricSchedule
		;
	rdfs:label "dividend schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "payment schedule indicating the dates on which dividends are due to be paid" ;
	.

fibo-sec-eq-eq:EnhancedVotingRight
	a
		owl:NamedIndividual ,
		fibo-sec-eq-eq:VotingRight ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:confersNumberOfVotesPerShare ;
			owl:someValuesFrom [

fibo-sec-eq-eq:PrivatelyHeldShare
	a owl:Class ;
	rdfs:subClassOf fibo-sec-eq-eq:Share ;
	rdfs:label "privately held share"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	owl:disjointWith fibo-sec-eq-eq:ListedShare ;
	skos:definition "share in a security that signifies ownership in an entity that is not publicly traded"@en ;
	fibo-fnd-utl-av:explanatoryNote "Privately owned companies include family-owned businesses, sole proprietorships and the vast majority of small and medium-sized businesses. These companies are often too small for an initial public offering (IPO) due, for example to a small market capitalization and/or low trading volume, and fulfill their financing requirements in other ways, including through smaller offerings." ;
	.

fibo-sec-eq-eq:QualifiedDividend
	a owl:Class ;
	rdfs:subClassOf fibo-sec-eq-eq:Dividend ;
	rdfs:label "qualified dividend" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "dividend that falls under capital gains tax rates that are lower than the income tax rates on unqualified (ordinary) dividends" ;
	.

fibo-sec-eq-eq:RedeemablePreferredShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredShare ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fi-fi:hasRedemptionProvision ;
			owl:someValuesFrom fibo-sec-eq-eq:EquityRedemptionProvision ;
		]
		;
	rdfs:label "redeemable preferred share" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "preferred share that gives the issuer the right to redeem the stock under specified conditions" ;
	.

fibo-sec-eq-eq:RedeemablePreferredShareWithExtendableMaturityDate
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredShareWithFixedMaturityDate ,
		fibo-sec-eq-eq:RedeemablePreferredShare ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasExtendableMaturityDate ;
			owl:hasValue "true"^^xsd:boolean ;
		]
		;
	rdfs:label "redeemable preferred share with extendable maturity date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "redeemable preferred share with a fixed maturity date whose issuer has the option to extend the maturity date" ;
	.

fibo-sec-eq-eq:RedeemablePreferredShareWithExtendableRedemptionDate
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:RedeemablePreferredShare ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-agr-ctr:hasExtensionProvision ;
			owl:someValuesFrom fibo-sec-eq-eq:EquityRedemptionProvisionWithExtendableRedemptionDate ;
		]
		;
	rdfs:label "redeemable preferred share with extendable redemption date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "redeemable preferred share whose redemption date can be modified" ;
	fibo-fnd-utl-av:synonym "extendible preferred share" ;
	.

fibo-sec-eq-eq:RestrictedShare
	a owl:Class ;
	rdfs:subClassOf fibo-sec-eq-eq:Share ;
	rdfs:label "restricted share"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	owl:disjointWith fibo-sec-eq-eq:UnrestrictedShare ;
	skos:definition "share whose ownership/transfer/sale is subject to special conditions including country-specific restrictions"@en ;
	.

fibo-sec-eq-eq:RestrictedVotingRight
	a
		owl:NamedIndividual ,
		fibo-sec-eq-eq:VotingRight ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:confersNumberOfVotesPerShare ;
			owl:someValuesFrom [
				a rdfs:Datatype ;
				owl:onDatatype xsd:decimal ;
				owl:withRestrictions (
					[
						xsd:maxExclusive "1.0"^^xsd:decimal ;
					]
				) ;
			] ;
		]
		;
	rdfs:label "restricted voting right" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "right to less than one vote per share" ;
	.

fibo-sec-eq-eq:RestrictedVotingShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:Share ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:confers ;
			owl:hasValue fibo-sec-eq-eq:RestrictedVotingRight ;
		]
		;
	rdfs:label "restricted voting share"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "share that confers less than one vote per share"@en ;
	.

fibo-sec-eq-eq:RetractablePreferredShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredShare ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fi-fi:hasRedemptionProvision ;
			owl:someValuesFrom fibo-sec-eq-eq:EquityRedemptionProvision ;
		]
		;
	rdfs:label "retractable preferred share" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "preferred share that gives the owner (shareholder) the right to redeem the stock under specified conditions" ;
	fibo-fnd-utl-av:explanatoryNote "When retractable preferred shares reach maturity, the shareholder has the right to sell them back to the stock issuer at the price stated on the agreement. In some cases, the issuer can force the shareholder to sell, and may have the option of exchanging retractable preferred shares for common shares instead of cash." ;
	.

fibo-sec-eq-eq:RetractablePreferredShareWithExtendableMaturityDate
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:PreferredShareWithFixedMaturityDate ,
		fibo-sec-eq-eq:RetractablePreferredShare ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasExtendableMaturityDate ;
			owl:hasValue "true"^^xsd:boolean ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasExtendableMaturityDate ;
			owl:hasValue "true"^^xsd:boolean ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-agr-ctr:hasExtensionProvision ;
			owl:someValuesFrom fibo-fnd-agr-ctr:ExtensionProvision ;
		]
		;
	rdfs:label "retractable preferred share with extendable maturity date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "retractable preferred share with a fixed maturity date whose issuer and/or holders have the option to extend the maturity date" ;
	.

fibo-sec-eq-eq:RetractablePreferredShareWithExtendableRedemptionDate
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:RetractablePreferredShare ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-agr-ctr:hasExtensionProvision ;
			owl:someValuesFrom fibo-sec-eq-eq:EquityRedemptionProvisionWithExtendableRedemptionDate ;
		]
		;
	rdfs:label "retractable preferred share with extendable redemption date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "retractable preferred share whose redemption date can be modified" ;
	fibo-fnd-utl-av:synonym "extendible preferred share" ;
	.

fibo-sec-eq-eq:Share
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:EquityInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasFloatingShares ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasSharesIssued ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasSharesOutstanding ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasTreasuryShares ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasShareClass ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-corp-corp:hasSharesAuthorized ;
			owl:someValuesFrom xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:confers ;
			owl:someValuesFrom fibo-sec-eq-eq:VotingRight ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:confersOwnershipOf ;
			owl:someValuesFrom fibo-fnd-acc-aeq:ShareholdersEquity ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasSharePaymentStatus ;
			owl:someValuesFrom fibo-sec-eq-eq:SharePaymentStatus ;
		]
		;
	rdfs:label "share"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "financial instrument that signifies a unit of equity ownership in a corporation, or a unit of ownership in a mutual fund, or interest in a general or limited partnership, or a unit of ownership in a structured product, such as a real estate investment trust"@en ;
	.

fibo-sec-eq-eq:ShareIssuer
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:Issuer ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:issues ;
			owl:someValuesFrom fibo-sec-eq-eq:Share ;
		]
		;
	rdfs:label "share issuer" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "issuer of securities that represent an ownership interest in something" ;
	fibo-fnd-utl-av:explanatoryNote "This includes shares that represent equity ownership in a corporation, or ownership in a mutual fund, or an interest in a general or limited partnership, or ownership in a structured product, such as a real estate investment trust." ;
	.

fibo-sec-eq-eq:SharePaymentStatus
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-lif:LifecycleStatus ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-cr:classifies ;
			owl:someValuesFrom fibo-sec-eq-eq:Share ;
		]
		;
	rdfs:label "share payment status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "classifier that specifies the overall payment status for shares issued" ;
	fibo-fnd-utl-av:explanatoryNote "When a company issues shares upon incorporation or through an initial or secondary issuance, shareholders are required to pay a set amount for those shares. Once the company has received the full amount from shareholders, the shares become fully paid shares." ;
	.

fibo-sec-eq-eq:ShareYield
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-ip:Yield ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-alx:hasArgument ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-sec-eq-eq:OrdinaryDividend
					fibo-fbc-fi-ip:SecurityPrice
				) ;
			] ;
		]
		;
	rdfs:label "share yield"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "ratio of the annualized dividend per share divided by the (current) price per share"@en ;
	fibo-fnd-utl-av:synonym
		"dividend yield"@en ,
		"dividend-price ratio"@en
		;
	.

fibo-sec-eq-eq:SingleVotingRight
	a
		owl:NamedIndividual ,
		fibo-sec-eq-eq:VotingRight
		;
	rdfs:label "single voting right" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "right to exactly one vote per share" ;
	fibo-sec-eq-eq:confersNumberOfVotesPerShare "1.0"^^xsd:decimal ;
	.

fibo-sec-eq-eq:SingleVotingShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-eq-eq:Share ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:confers ;
			owl:hasValue fibo-sec-eq-eq:SingleVotingRight ;
		]
		;
	rdfs:label "single voting share"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "share that has the right to exactly one vote"@en ;
	.

fibo-sec-eq-eq:SoftRetractablePreferredShare
	a owl:Class ;
	rdfs:subClassOf fibo-sec-eq-eq:RetractablePreferredShare ;
	rdfs:label "soft retractable preferred share" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "retractable preferred share whose retraction value may be paid in cash or in an equal value of common stock of the issuer, at the option of the issuer" ;
	.

fibo-sec-eq-eq:SpecialDividend
	a owl:Class ;
	rdfs:subClassOf fibo-sec-eq-eq:Dividend ;
	rdfs:label "special dividend" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "dividend that is paid to shareholders on a one-time basis" ;
	fibo-fnd-utl-av:usageNote "Special dividends may be included in a dividend schedule as an ad-hoc entry, since they still need to be tracked based on the date of issuance."@en ;
	.

fibo-sec-eq-eq:UnrestrictedShare
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:NegotiableSecurity ,
		fibo-sec-eq-eq:Share
		;
	rdfs:label "unrestricted share"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "share whose ownership/transfer/sale is not subject to special conditions including country-specific restrictions"@en ;
	.

fibo-sec-eq-eq:VotingRight
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-law-lcap:ContractualRight ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:hasVotingRestriction ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-eq-eq:confersNumberOfVotesPerShare ;
			owl:onDataRange xsd:decimal ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isConferredBy ;
			owl:someValuesFrom fibo-fnd-agr-ctr:WrittenContract ;
		]
		;
	rdfs:label "voting right" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "contractual right that specifies shareholder voting entitlements, such as to elect directors, elect outside auditors, and vote on matters of corporate policy" ;
	fibo-fnd-utl-av:explanatoryNote "Voting may involve decisions on issuing securities, initiating stock splits, and making substantial changes in the corporation's operations.  Note that a given share may not have voting rights, in which case the number of votes per share would be zero." ;
	.

fibo-sec-eq-eq:confersNumberOfVotesPerShare
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf fibo-fnd-acc-cur:hasAmount ;
	rdfs:label "number of votes per share"@en ;
	rdfs:domain fibo-sec-eq-eq:VotingRight ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range xsd:decimal ;
	skos:definition "grants the right to vote on a per share basis to the shareholder"@en ;
	fibo-fnd-utl-av:explanatoryNote "Voting rights may be set at zero, one, or more votes per share, depending on the contract."@en ;
	.

fibo-sec-eq-eq:confersOwnershipOf
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:confers ;
	rdfs:label "confers ownership of" ;
	rdfs:domain fibo-fnd-agr-ctr:Contract ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	skos:definition "grants ownership via contract"@en ;
	.

fibo-sec-eq-eq:hasAdjustableDividendRate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf lcc-lr:has ;
	rdfs:label "has adjustable dividend rate" ;
	rdfs:domain fibo-sec-eq-eq:PreferredDividend ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-fnd-acc-cur:PercentageMonetaryAmount ;
	skos:definition "indicates a variable dividend rate, typically specified in the prospectus as a formula based on a benchmark or set at auction" ;
	.

fibo-sec-eq-eq:hasDistributionMethod
	a owl:ObjectProperty ;
	rdfs:subPropertyOf lcc-lr:has ;
	rdfs:label "has distribution method" ;
	rdfs:domain fibo-sec-eq-eq:Dividend ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-sec-eq-eq:DividendDistributionMethod ;
	skos:definition "indicates the method by which dividend payments are to be distributed" ;
	.

fibo-sec-eq-eq:hasDividend
	a owl:ObjectProperty ;
	rdfs:subPropertyOf lcc-lr:has ;
	rdfs:label "has dividend" ;
	rdfs:domain fibo-sec-eq-eq:Share ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range [
		a owl:Class ;
		owl:unionOf (
			fibo-sec-eq-eq:OrdinaryDividend
			fibo-sec-eq-eq:PreferredDividend
		) ;
	] ;
	skos:definition "relates a share to a dividend that applies to that share" ;
	.

fibo-sec-eq-eq:hasDividendDeclarationDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasExplicitDate ;
	rdfs:label "has declaration date" ;
	rdfs:domain fibo-sec-eq-eq:Share ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-fnd-dt-fd:ExplicitDate ;
	skos:definition "indicates a date on which an organization states that a dividend payment may be anticipated, including the the dividend amount and the ex-dividend and payment dates" ;
	fibo-fnd-utl-av:synonym "has announcement date" ;
	.

fibo-sec-eq-eq:hasDividendGracePeriod
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasDuration ;
	rdfs:label "has dividend grace period" ;
	rdfs:domain fibo-sec-eq-eq:PreferredDividend ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-fnd-dt-fd:Duration ;
	skos:definition "indicates a period of time after a dividend payment becomes due, before the issuer is subject to penalties" ;
	.

fibo-sec-eq-eq:hasEarliestRedemptionDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasExplicitDate ;
	rdfs:label "has earliest redemption date" ;
	rdfs:domain fibo-sec-eq-eq:EquityRedemptionProvision ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-fnd-dt-fd:ExplicitDate ;
	skos:definition "indicates a date on or after which a stock can be redeemed" ;
	.

fibo-sec-eq-eq:hasExDividendDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasExplicitDate ;
	rdfs:label "has ex-dividend date" ;
	rdfs:domain fibo-sec-eq-eq:Share ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-fnd-dt-fd:ExplicitDate ;
	skos:definition "indicates a date on which a stock 'goes ex-dividend', typically about three weeks before the dividend is paid to shareholders of record" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.investor.gov/introduction-investing/investing-basics/glossary/ex-dividend-dates-when-are-you-entitled-stock-and"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote
		"Once the company sets the record date, the ex-dividend date is set based on stock exchange rules. If you purchase a stock on its ex-dividend date or after, you will not receive the next dividend payment." ,
		"Shares listed on the New York Stock Exchange go ex-dividend four business days prior to the record date."
		;
	fibo-fnd-utl-av:synonym
		"has ex-date" ,
		"has expected dividend date"
		;
	.

fibo-sec-eq-eq:hasExtendableMaturityDate
	a owl:DatatypeProperty ;
	rdfs:label "has extendable maturity date" ;
	rdfs:domain fibo-sec-eq-eq:PreferredShare ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range xsd:boolean ;
	skos:definition "indicates whether the issuer and/or holders of redeemable shares with a fixed maturity date have the option to extend the maturity date" ;
	.

fibo-sec-eq-eq:hasExtendableRedemptionDate
	a owl:DatatypeProperty ;
	rdfs:label "has extendable redemption date" ;
	rdfs:domain fibo-sec-eq-eq:EquityRedemptionProvision ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range xsd:boolean ;
	skos:definition "indicates whether the issuer and/or holders of redeemable shares with a specified redemption date have the option to extend that date" ;
	.

fibo-sec-eq-eq:hasFixedDividendRate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf lcc-lr:has ;
	rdfs:label "has fixed dividend rate" ;
	rdfs:domain fibo-sec-eq-eq:PreferredDividend ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range fibo-fnd-acc-cur:PercentageMonetaryAmount ;
	skos:definition "indicates a pre-arranged fixed dividend rate, typically specified in the prospectus, for a preferred share" ;
	.

fibo-sec-eq-eq:hasFloatingShares
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf fibo-fnd-acc-cur:hasAmount ;
	rdfs:label "has floating shares"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Equities/EquityInstruments/> ;
	rdfs:range xsd:nonNegativeInteger ;
	skos:definition "indicates the number of shares that are available for trading, i.e., the number of shares outstanding less closely held shares (those held by insiders) and restricted shares"@en ;
	fibo-fnd-utl-av:explanatoryNote "A relatively small float results in higher volatility, as a large purchase or sell order will have significant influence on the value of the stock."@en ;
	.

fibo-sec-eq-eq:hasMinimumRedemptionPrice

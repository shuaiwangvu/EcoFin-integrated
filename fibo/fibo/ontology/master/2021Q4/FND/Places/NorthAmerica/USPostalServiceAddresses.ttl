	owl:equivalentClass [
		a owl:Restriction ;
		owl:onProperty fibo-fnd-plc-loc:hasCountry ;
		owl:onClass lcc-cr:Country ;
		owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	] ;
	skos:definition "physical address that explicitly includes a country" ;
	.


			owl:onProperty lcc-lr:hasMember ;
			owl:someValuesFrom fibo-fnd-aap-ppl:NationalIdentificationNumber ;
		]
		;
	rdfs:label "national identification number scheme" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/> ;
	skos:definition "system for allocating identifiers to national identification numbers" ;
	fibo-fnd-utl-av:explanatoryNote "Schemes for national identification numbers are jurisdiction-specific by country." ;
	.

fibo-fnd-aap-ppl:Passport

	.

fibo-fnd-aap-ppl:hasMinimumLegalWorkingAge
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasAge ;
	rdfs:label "has minimum legal working age" ;

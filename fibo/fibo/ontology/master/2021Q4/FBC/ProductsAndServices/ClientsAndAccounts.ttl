fibo-fbc-pas-caa:TimeCertificateOfDepositAccount
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-pas-caa:TimeDepositAccount ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-caa:isRealizedBy ;
			owl:someValuesFrom fibo-fbc-pas-caa:CertificateOfDeposit ;

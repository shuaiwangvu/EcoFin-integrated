	fibo-fnd-utl-av:adaptedFrom "https://www.fdic.gov/regulations/resources/call/crinst/2010-09/910gloss_093010.pdf" ;
	.

fibo-fbc-pas-caa:TimeDepositAccount
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-pas-caa:NonTransactionDepositAccount ,
		fibo-fnd-pas-pas:ContractualProduct
		;
	rdfs:label "time deposit account" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "deposit account that the depositor does not have a right, and is not permitted, to make withdrawals from within six days after the date of deposit unless the deposit is subject to an early withdrawal penalty of at least seven days' simple interest on amounts withdrawn within the first six days after deposit" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.fdic.gov/regulations/resources/call/crinst/2010-09/910gloss_093010.pdf" ;
	fibo-fnd-utl-av:explanatoryNote "A time deposit from which partial early withdrawals are permitted must impose additional early withdrawal penalties of at least seven days' simple interest on amounts withdrawn within six days after each partial withdrawal. If such additional early withdrawal penalties are not imposed, the account ceases to be a time deposit. The account may become a savings deposit if it meets the requirements for a savings deposit; otherwise it becomes a demand deposit." ;
	.

fibo-fbc-pas-caa:TimeDepositOpenAccount
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-pas-caa:TimeDepositAccount ;
	rdfs:label "time deposit open account" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	owl:disjointWith fibo-fbc-pas-caa:TimeCertificateOfDepositAccount ;
	skos:definition "time deposit account that allows deposits (other than time certificates of deposit) for which there is in force a written contract with the depositor that neither the whole nor any part of such deposit may be withdrawn prior to (1) the date of maturity, which shall be not less than seven days after the date of the deposit, or (2) the expiration of a specified period of written notice of not less than seven days" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.fdic.gov/regulations/resources/call/crinst/2010-09/910gloss_093010.pdf" ;
	.

fibo-fbc-pas-caa:TransactionCategory
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-cls:Classifier ,
		lcc-lr:CodeElement ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:hasTag ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-cr:classifies ;
			owl:someValuesFrom fibo-fbc-pas-caa:IndividualTransaction ;
		]
		;
	rdfs:label "transaction category" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "high-level classifier for an individual transaction" ;
	skos:example "credit, debit, fee" ;
	.

fibo-fbc-pas-caa:TransactionDate
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:ExplicitDate ;
	rdfs:label "transaction date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "date on which a specific transaction was initiated" ;
	.

fibo-fbc-pas-caa:TransactionDepositAccount
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-pas-caa:DepositAccount ;
	rdfs:label "transaction deposit account" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "deposit account from which the depositor / account holder is permitted to make transfers or withdrawals by negotiable / transferable instruments, payment orders of withdrawal, telephone transfers, and so forth, and that may be accessible via an electronic device such as an automated teller machine (ATM), remote service unit (RSU), mobile device, and by debit card" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.fdic.gov/regulations/resources/call/crinst/2010-09/910gloss_093010.pdf" ;
	fibo-fnd-utl-av:explanatoryNote "Excluded from transaction accounts are savings deposits (both money market deposit accounts (MMDAs) and other savings deposits), even though such deposits permit some third-party transfers. However, an account that otherwise meets the definition of a savings deposit but that authorizes or permits the depositor to exceed the transfer limitations specified for that account shall be reported as a transaction account." ;
	.

fibo-fbc-pas-caa:TransactionIdentifier
	a owl:Class ;
	rdfs:subClassOf
		lcc-lr:Identifier ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:identifies ;
			owl:onClass fibo-fbc-pas-caa:IndividualTransaction ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "transaction identifier" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "identifier for an individual transaction associated with an account" ;
	.

fibo-fbc-pas-caa:TransactionRecord
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:Registry ,
		fibo-fnd-arr-doc:Record ,
		fibo-fnd-dt-fd:DatedStructuredCollection ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:isIdentifiedBy ;
			owl:onClass fibo-fbc-pas-caa:TransactionRecordIdentifier ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-caa:hasCloseDate ;
			owl:onClass fibo-fbc-pas-caa:CloseDate ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-caa:hasOpenDate ;
			owl:onClass fibo-fbc-pas-caa:OpenDate ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-caa:hasTransactionRecordStatus ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-caa:appliesToAccount ;
			owl:onClass fibo-fbc-pas-caa:Account ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:onClass fibo-fbc-pas-caa:AccountProvider ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistryEntry ;
			owl:someValuesFrom fibo-fbc-pas-caa:IndividualTransaction ;
		]
		;
	rdfs:label "transaction record" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "record of transactions associated with an account" ;
	fibo-fnd-utl-av:usageNote "The date a particular transaction record is closed typically corresponds to (and may precede) the date the account is closed, though in the case of certain accounts, such as a credit card account, if a customer is issued a new account or card number due to loss, fraud, or for some other reason, it is possible that multiple transaction records would be associated with the account.  In that case, the close date might correspond to the date that a hold was placed on the original account." ;
	.

fibo-fbc-pas-caa:TransactionRecordIdentifier
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistryIdentifier ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:identifies ;
			owl:onClass fibo-fbc-pas-caa:TransactionRecord ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:hasTag ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "transaction record identifier" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "unique identifier for record of transactions" ;
	.

fibo-fbc-pas-caa:TransactionSubcategory
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-cls:Classifier ,
		lcc-lr:CodeElement ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:hasTag ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-cr:classifies ;
			owl:someValuesFrom fibo-fbc-pas-caa:IndividualTransaction ;
		]
		;
	rdfs:label "transaction subcategory" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "second-level classifier for a transaction, e.g., direct deposit, check, cash advance, withdrawal, payment, purchase, and so forth" ;
	.

fibo-fbc-pas-caa:appliesToAccount
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:appliesTo ;
	rdfs:label "applies to account" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:Account ;
	skos:definition "indicates the account to which the transaction record or individual transaction applies" ;
	.

fibo-fbc-pas-caa:hasBalance
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-acc-cur:hasMonetaryAmount ;
	rdfs:label "has balance" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:Balance ;
	skos:definition "relates an account to the net amount of money available in that account" ;
	.

fibo-fbc-pas-caa:hasCloseDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf
		fibo-fnd-dt-fd:hasEndDate ,
		fibo-fnd-dt-fd:hasExplicitDate
		;
	rdfs:label "has close date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:CloseDate ;
	skos:definition "relates something to the date that it was closed" ;
	.

fibo-fbc-pas-caa:hasEndingBalance
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fbc-pas-caa:hasBalance ;
	rdfs:label "has ending balance" ;
	rdfs:domain fibo-fbc-pas-caa:AccountStatement ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:Balance ;
	skos:definition "relates an account statement to the amount of money available in that account at the end of the statement period" ;
	.

fibo-fbc-pas-caa:hasOpenDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf
		fibo-fnd-dt-fd:hasExplicitDate ,
		fibo-fnd-dt-fd:hasStartDate
		;
	rdfs:label "has open date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:OpenDate ;
	skos:definition "relates something to the date that it was created" ;
	.

fibo-fbc-pas-caa:hasPaymentDueDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasExplicitDate ;
	rdfs:label "has payment due date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:PaymentDueDate ;
	skos:definition "indicates the date by which payment of some amount must be made to the creditor" ;
	.

fibo-fbc-pas-caa:hasPostingDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasExplicitDate ;
	rdfs:label "has posting date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:PostingDate ;
	skos:definition "indicates the date that the transaction was posted to the account" ;
	.

fibo-fbc-pas-caa:hasPrimaryAccountHolder
	a
		owl:ObjectProperty ,
		owl:FunctionalProperty
		;
	rdfs:subPropertyOf fibo-fnd-rel-rel:isHeldBy ;
	rdfs:label "has primary account holder" ;
	rdfs:domain fibo-fbc-pas-caa:CustomerAccount ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:CustomerAccountHolder ;
	skos:definition "relates an account to a client or customer that is considered the primary owner of the account" ;
	fibo-fnd-utl-av:explanatoryNote "Note that for many financial institutions, there must be a client or customer designated as the primary owner. In cases where there is a tax identifier associated with the account, it is that of the primary owner." ;
	fibo-fnd-utl-av:synonym "has primary account owner" ;
	.

fibo-fbc-pas-caa:hasSecondaryAccountHolder
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:isHeldBy ;
	rdfs:label "has secondary account holder" ;
	rdfs:domain fibo-fbc-pas-caa:CustomerAccount ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:CustomerAccountHolder ;
	skos:definition "relates an account to a client or customer that is considered a secondary, co-owner of the account" ;
	fibo-fnd-utl-av:synonym
		"has account co-owner" ,
		"has secondary account owner"
		;
	.

fibo-fbc-pas-caa:hasStartingBalance
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fbc-pas-caa:hasBalance ;
	rdfs:label "has starting balance" ;
	rdfs:domain fibo-fbc-pas-caa:AccountStatement ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:Balance ;
	skos:definition "relates an account statement to the amount of money available in that account at the beginning of the statement period" ;
	.

fibo-fbc-pas-caa:hasTransactionDate
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-dt-fd:hasExplicitDate ;
	rdfs:label "has transaction date" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:TransactionDate ;
	skos:definition "indicates the date on which the transaction actually occurred" ;
	.

fibo-fbc-pas-caa:hasTransactionDescription
	a owl:DatatypeProperty ;
	rdfs:label "has transaction description" ;
	rdfs:domain fibo-fbc-pas-caa:IndividualTransaction ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range xsd:string ;
	skos:definition "provides a textual description of the transaction" ;
	.

fibo-fbc-pas-caa:hasTransactionRecordStatus
	a owl:DatatypeProperty ;
	rdfs:label "has transaction record status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range xsd:string ;
	skos:definition "indicates the status of the transaction record" ;
	.

fibo-fbc-pas-caa:involvesMerchant
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:involves ;
	rdfs:label "involves merchant" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-be-fct-fct:Merchant ;
	skos:definition "indicates the merchant (seller) involved in the transaction" ;
	.

fibo-fbc-pas-caa:isLinkedToAccount
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fbc-pas-fpas:relatesTo ;
	rdfs:label "is linked to account" ;
	rdfs:domain fibo-fbc-pas-caa:CustomerAccount ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	rdfs:range fibo-fbc-pas-caa:CustomerAccount ;
	skos:definition "connects a given customer account to another customer account" ;
	.

fibo-fbc-pas-caa:isRealizedBy
	a owl:ObjectProperty ;
	rdfs:label "is realized by" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	owl:inverseOf fibo-fbc-pas-caa:realizes ;
	skos:definition "is made concrete by" ;
	.

fibo-fbc-pas-caa:realizes
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:appliesTo ;
	rdfs:label "realizes" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	skos:definition "makes concrete" ;
	.

fibo-fbc-pas-caa:recordsTransaction
	a owl:ObjectProperty ;
	rdfs:label "records transaction" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/ProductsAndServices/ClientsAndAccounts/> ;
	owl:propertyChainAxiom (
		fibo-fbc-pas-caa:appliesToAccount
		fibo-fnd-arr-doc:hasRecord
		fibo-fbc-fct-ra:hasRegistryEntry
	) ;
	skos:definition "links an account statement to the individual transactions it documents" ;
	.

fibo-fnd-pas-pas:ContractualProduct
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-fbc-pas-caa:isRealizedBy ;
		owl:someValuesFrom fibo-fnd-agr-ctr:Contract ;
	] ;
	.


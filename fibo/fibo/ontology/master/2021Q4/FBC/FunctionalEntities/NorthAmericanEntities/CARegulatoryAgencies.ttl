fibo-fbc-fct-cajrga:BusinessNumber
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-org-org:OrganizationIdentifier ,
		fibo-fnd-pty-pty:TaxIdentifier ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-law-jur:appliesIn ;
			owl:hasValue fibo-be-ge-caj:CanadianJurisdiction ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty lcc-lr:isMemberOf ;
			owl:hasValue fibo-fbc-fct-cajrga:BusinessNumberRegistrationIdentifierScheme ;
		]
		;
	rdfs:label "business number" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/NorthAmericanEntities/CARegulatoryAgencies/> ;
	skos:definition "unique, 9-digit number that is the standard identifier for legal entities in Canada which are typically a business" ;
	skos:example "000000000" ;
	fibo-fnd-utl-av:abbreviation "BN" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.canada.ca/en/services/taxes/business-number.html"^^xsd:anyURI ;
	.

fibo-fbc-fct-cajrga:BusinessNumberRegistrationIdentifierScheme
	a
		owl:NamedIndividual ,
		fibo-be-corp-corp:RegistrationIdentifierScheme ,
		fibo-fnd-pty-pty:TaxIdentificationScheme
		;
	rdfs:label "Canada Revenue Agency business number registration identifier scheme" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/NorthAmericanEntities/CARegulatoryAgencies/> ;
	skos:definition "registration identifier scheme for business numbers defined by the Canada Revenue Agency for the registration of a business or legal entity" ;
	.


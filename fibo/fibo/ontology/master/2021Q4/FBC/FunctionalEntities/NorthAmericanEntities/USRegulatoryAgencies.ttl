				a owl:Restriction ;
				owl:onProperty fibo-fnd-law-jur:hasReach ;
				owl:someValuesFrom [
					a owl:Class ;
					owl:intersectionOf (
						lcc-cr:CountrySubdivision
						[
							a owl:Restriction ;
							owl:onProperty lcc-cr:isSubregionOf ;
							owl:hasValue lcc-3166-1:UnitedStatesOfAmerica ;
						]
					) ;
				] ;
			] ;
		]
		;
	rdfs:label "state government entity or agency" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/NorthAmericanEntities/USRegulatoryAgencies/> ;
	skos:definition "formal organization that is an independent agency, instrumentality or other permanent or semi-permanent organization in the machinery of government of any one of the states or territories of the United States" ;
